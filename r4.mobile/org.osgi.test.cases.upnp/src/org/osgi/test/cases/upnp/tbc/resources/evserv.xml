<?xml version="1.0"?>
<scpd xmlns="urn:schemas-upnp-org:service-1-0">
  <specVersion>
    <major>1</major>
    <minor>0</minor>
  </specVersion>
  <serviceStateTable>
    <stateVariable sendEvents="yes">
      <name>INT</name>
      <dataType>int</dataType>
      <defaultValue>4</defaultValue>
    </stateVariable>
    <stateVariable sendEvents="yes">
      <name>UI4</name>
      <dataType>ui4</dataType>
      <allowedValueRange>
        <minimum>4</minimum>
        <maximum>444</maximum>
        <step>4</step>
      </allowedValueRange>
    </stateVariable>
    <stateVariable sendEvents="yes">
      <name>NUMBER</name>
      <dataType>r8</dataType>
      <defaultValue>0.1</defaultValue>
      <allowedValueRange>
        <minimum>0.0</minimum>
        <maximum>1.1</maximum>
        <step>0.1</step>
      </allowedValueRange>
    </stateVariable>
    <stateVariable sendEvents="yes">
      <name>FLOAT</name>
      <dataType>float</dataType>
    </stateVariable>
    <stateVariable sendEvents="yes">
      <name>CHAR</name>
      <dataType>char</dataType>
    </stateVariable>
    <stateVariable sendEvents="yes">
      <name>STRING</name>
      <dataType>string</dataType>
      <defaultValue>qwerty</defaultValue>
      <allowedValueList>
        <allowedValue>123456</allowedValue>
        <allowedValue>qwerty</allowedValue>
        <allowedValue>asdfgh</allowedValue>
        <allowedValue>zxcvbn</allowedValue>
      </allowedValueList>
    </stateVariable>
    <stateVariable sendEvents="yes">
      <name>BOOLEAN</name>
      <dataType>boolean</dataType>
    </stateVariable>
    <stateVariable sendEvents="yes">
      <name>BIN_HEX</name>
      <dataType>bin.hex</dataType>
    </stateVariable>
    <stateVariable sendEvents="yes">
      <name>OUT</name>
      <dataType>string</dataType>
    </stateVariable>
  </serviceStateTable>
</scpd>
