	${junit}	

-runbundles						= \
	org.osgi.impl.bundle.jmx; version=project, \
	org.osgi.impl.service.useradmin; version=project, \
	org.osgi.impl.service.log; version=project, \
	org.apache.felix.configadmin; version=latest, \
	org.osgi.impl.service.provisioning; version=project, \
	osgi.cmpn; version=latest

-runproperties					= report=true

Test-Cases: ${classes;extending;*TestCase}
	