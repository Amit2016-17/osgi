	org.osgi.test.support.*
	
Include-Resource					= \
	tb1.jar	,\
	tb2.jar

-buildpath							= \
	org.osgi.jmx;					version=project	, \
	osgi;									version=4.0		, \
	org.osgi.service.cm; version=project, \
	org.osgi.test.support;	version=project, \
	org.osgi.service.permissionadmin; version=project, \
	org.osgi.service.provisioning; version=project, \
