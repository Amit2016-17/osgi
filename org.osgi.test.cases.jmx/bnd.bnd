javac.source 			= 1.5
javac.target 			= 1.5

Bundle-Version: 4.2.0.${project.build}

Import-Package: org.osgi.jmx, *
Private-Package						= ${p}.junit.*, \
	org.osgi.test.support.*
	
Include-Resource					= \
	tb1.jar	,\
	tb2.jar

-buildpath							= ee.j2se; version=1.5, \
	org.osgi.test.support;			version=project, \
	org.osgi.jmx;					version=project	, \
	osgi.core;						version=4.2		, \
	osgi.enterprise;				version=4.2		, \
	${junit}	

-runbundles						= \
	org.osgi.impl.bundle.jmx; version=project, \
	org.osgi.impl.service.useradmin; version=latest, \
	org.osgi.impl.service.log; version=latest, \
	org.apache.felix.configadmin; version=latest, \
	org.eclipse.equinox.ip; version=latest, \
	org.eclipse.equinox.util; version=latest, \
	osgi.cmpn;						version=latest		, \
	org.osgi.impl.bundle.servlet;		version=2.1

-runproperties					= report=true, \
	osgi.compatibility.bootdelegation=false, \
	osgi.resolverMode=strict, \
	osgi.support.multipleHosts=true, \
	org.osgi.framework.security=osgi, \
	org.osgi.framework.trust.repositories=${build}/keystore

Test-Cases							= ${testcases}
-signaturetest                      = \
            org.osgi.jmx, \
            org.osgi.jmx.framework, \
            org.osgi.jmx.service.cm, \
			org.osgi.jmx.service.permissionadmin, \
			org.osgi.jmx.service.provisioning, \
			org.osgi.jmx.service.useradmin
