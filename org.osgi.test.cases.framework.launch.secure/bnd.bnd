# Set javac settings from JDT prefs
-include: ${project.workspace}/cnf/eclipse/jdt.bnd

Bundle-Description					: Framework test cases.

Private-Package						= org.osgi.test.support.*, \
	${p}.junit
	
Include-Resource = \
    launch.secure.tb1.jar
	
Test-Cases							= ${testcases}

-buildpath							= \
    org.osgi.test.support;                  version=project , \
	org.osgi.framework;						version=project	, \
	osgi.core;								version=4.2	, \
	osgi.cmpn;								version=4.0	, \
	ee.minimum; 							version=1.2	, \
	${junit}

-runbundles							= \
	
-runframework = none

-runproperties = ${runproperties}, \
	${p}.trust.repositories=${build}/keystore, \
	${p}.storageroot=${target-dir}/${p}.testStorageRoot 
