Bundle-Version						: 4.2.0.${project.build}
Bundle-Description					: Framework test cases.

Private-Package						= org.osgi.test.support.*, \
	${p}.junit
	
Include-Resource = \
    launch.secure.tb1.jar
	
Test-Cases							= ${classes;extending;org.junit.framework.TestCase|*DefaultTestBundleControl}

-buildpath							= \
    org.osgi.test.support;                  version=project , \
	org.osgi.framework;						version=project	, \
	osgi.core;								version=4.2	, \
	osgi.cmpn;								version=4.0	, \
	ee.minimum; 							version=1.2	, \
	${junit}

-runbundles							= \
	

-runproperties = report=true, \
	osgi.compatibility.bootdelegation=false, \
	osgi.resolverMode=strict, \
	osgi.support.multipleHosts=true, \
	org.osgi.test.cases.framework.launch.secure.trust.repository=${build}/keystore, \
	noframework=true, \
	org.osgi.test.cases.framework.launch.storageroot=generated/testStorageRoot 
