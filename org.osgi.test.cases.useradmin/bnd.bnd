Bundle-Description: Test Bundle for UserAdmin Service tests
Bundle-Version: 4.2.0.${project.build}

Private-Package						= org.osgi.test.support.*, \
	${p}.junit.*
	
Test-Cases							= ${testcases}
-signaturetest                      = org.osgi.service.useradmin

-buildpath							= \
	org.osgi.service.useradmin;				version=project, \
    org.osgi.test.support;                  version=project , \
	osgi.core;								version=4.1	, \
	ee.minimum;								version=1.2		, \
	${junit}

-runbundles							= \
	osgi.cmpn; version=${cmpn.published.version};strategy=lowest, \
	org.osgi.impl.service.log; version=latest, \
	org.osgi.impl.service.useradmin; version=latest

-runproperties = ${runproperties}
