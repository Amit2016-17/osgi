Bundle-Version: 4.2.0.${project.build}
Export-Package: 		${p}.tbc;-split-package:=first
Private-Package						= ${p}.tbc.*, \
	org.osgi.test.support.*

Include-Resource					= \
	dev0.jar, \
	dev1.jar, \
	dev2.jar, \
	dev3.jar, \
	dev4.jar, \
	dev20.jar, \
	dev100.jar, \
	drv1.jar, \
	drv2.jar, \
	drv3.jar, \
	drv4.jar, \
	drv5.jar, \
	drv6.jar, \
	drv7.jar
	
Test-Cases							= ${classes;extending;org.junit.framework.TestCase|*DefaultTestBundleControl}

-buildpath							= \
    org.osgi.test.support;                  version=project , \
	osgi;									version=4.1		, \
	ee.minimum												, \
	${junit}

-runbundles							= \
	org.osgi.impl.service.log; version=latest, \
	org.osgi.impl.service.device.manager;  version=latest, \
	org.osgi.impl.service.device.basicdriverlocator;  version=latest

