javac.profile=compact2
# Set javac settings from JDT prefs
-include: ${build}/eclipse/jdt.bnd, ${build}/companion.bnd

Export-Package: ${p}.*; -split-package:=first

-buildpath: \
	osgi.core;version=4.3.1,\
	org.osgi.service.jdbc;version=latest,\
	org.eclipse.persistence.jpa_spec;version=2.0,\
	org.osgi.annotation.versioning;version=latest
