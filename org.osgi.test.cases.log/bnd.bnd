Bundle-Version: 3.0.0.${project.build}

Private-Package						= ${p}.junit, \
	org.osgi.test.support.*

-buildpath							= \
	org.osgi.test.support; 	version=project, \
	org.osgi.service.log;					version=project	, \
	osgi.core;								version=4.1		, \
	ee.minimum;								version=1.2		, \
	${junit}

Test-Cases							= ${testcases}
-signaturetest                      = org.osgi.service.log
-testbundles						= ${p}

-runbundles						= \
	org.osgi.impl.service.log; version=project, \
	org.apache.felix.configadmin;  version=latest

-runproperties					= report=true
