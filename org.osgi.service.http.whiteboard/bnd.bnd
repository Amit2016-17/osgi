# Set javac settings from JDT prefs
-include: ${build}/eclipse/jdt.bnd, ${build}/companion.bnd

Export-Package:  \
	org.osgi.service.http.*;-split-package:=first

-contract: JavaServlet
-buildpath: \
	osgi.core;maven-scope=provided;version=6.0,\
	osgi.annotation;maven-scope=provided;version=${osgi.annotation.version},\
	org.osgi.service.component.annotations;version=latest,\
	org.apache.felix.http.servlet-api;version=1.1,\
	org.osgi.namespace.implementation;version=latest
