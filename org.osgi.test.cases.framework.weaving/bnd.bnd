# Set javac settings from JDT prefs
-include: ${project.workspace}/cnf/eclipse/jdt.bnd

Conditional-Package					= org.osgi.test.support.*

Private-Package:                         ${p}.*

Import-Package						= \
	org.osgi.framework;						version="[1.6,2.0)" , \
	org.osgi.framework.hooks.weaving;		version="[1.0,2.0)" , \
	*

Include-Resource 					= \
	testclasses.jar , \
	org.objectweb.asm-3.2.0.jar

-buildpath							= \
	osgi.core;									version=4.3		, \
	ee.minimum;									version=1.2		, \
	org.osgi.test.support;              		version=project , \
	${junit}

-runproperties					= report=true

Test-Cases							= ${testcases}

-signaturetest                      = org.osgi.framework.hooks.weaving