Bundle-Version: 4.2.0.${project.build}

Private-Package 					= org.osgi.test.support.* , \
	${p}.*
	
Export-Package						= \
	${p}.provider, \
	${p}.provider.ct, \
	${p}.service

Import-Package 						= \
	* 


Include-Resource 					= \
	initialContextFactory1.jar , \
	initialContextFactory2.jar , \
	initialContextFactoryBuilder1.jar , \
	objectFactory1.jar , \
	objectFactory2.jar , \
	objectFactoryBuilder1.jar , \
	service1.jar , \
	urlContext1.jar
	
Test-Cases							= ${classes;extending;org.junit.framework.TestCase|*DefaultTestBundleControl}

-buildpath							= \
    org.osgi.test.support;                  version=project , \
	osgi;									version=latest	, \
	ee.j2se; version=latest											, \
	${junit}
	