# Set javac settings from JDT prefs
-include: ${project.workspace}/cnf/eclipse/jdt.bnd

Conditional-Package					= org.osgi.test.support.*
Private-Package						=  \
    ${p}.junit.*, \
    ${p}.util.*
Import-Package: ${-signaturetest}, *

-buildpath:  \
	org.osgi.test.support;version=project,\
	org.osgi.service.networkadapter;version=project,\
	osgi.core;version=4.2,\
	osgi.cmpn;version=4.2,\
	ee.minimum;version=1.2,\
	${junit}

Test-Cases							= ${testcases}
-signaturetest						= org.osgi.service.networkadapter

-runbundles:  \
	org.osgi.impl.service.networkadapter;version=latest,\
	org.osgi.impl.service.event

-runproperties = ${runproperties}

Export-Package:  \
	org.osgi.test.support.step