# Set javac settings from JDT prefs
-include: ${project.workspace}/cnf/eclipse/jdt.bnd	

Conditional-Package					= org.osgi.test.support.*
Private-Package						=  \
	${p}.*
Import-Package: ${-signaturetest}, *

-buildpath: org.osgi.test.support;version=project,\
	osgi.core;version=4.3,\
	osgi.cmpn;version=4.3,\
	ee.minimum;version=1.2.1,\
	${junit},\
	org.osgi.service.resourcemanagement;version=latest

-runbundles: \
	org.osgi.impl.service.log;version=latest,\
	org.osgi.service.resourcemanagement;version=latest,\
	org.osgi.impl.service.resourcemanagement;version=latest,\
	org.osgi.impl.service.resourcemanagement.threadmanager;version=latest,\
	org.osgi.impl.service.resourcemanagement.util;version=latest,\
	org.osgi.impl.service.resourcemanagement.fakemonitors;version=latest\


-runproperties = ${runproperties}

Test-Cases							= ${testcases}
-signaturetest                      =  org.osgi.service.resourcemanagement
	
