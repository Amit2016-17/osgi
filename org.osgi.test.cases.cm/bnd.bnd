# Set javac settings from JDT prefs
-include: ${project.workspace}/cnf/eclipse/jdt.bnd

Bundle-Description: Configuration Management Test R4
Bundle-Vendor: OSGi Alliance
Bundle-DocURL: http://www.osgi.org/
Bundle-ContactAddress: info@mail.osgi.org
Bundle-Category: osgi,test

Conditional-Package					= org.osgi.test.support.*
Private-Package						=  \
	${p}.junit, \
	${p}.common
	
Include-Resource					= \
	tb1.jar, \
	tb2.jar, \
	setallpermission.jar, \
	targetb1.jar, \
	targetb2.jar, \
	bundleT1.jar, \
	bundleT2.jar, \
	bundleT3.jar, \
	bundleT4.jar
	
Export-Package = \
	${p}.shared;version=1.0.0
Import-Package: ${-signaturetest}, *
	
Test-Cases							= ${testcases}
-signaturetest                      = org.osgi.service.cm

-buildpath							= \
	org.osgi.service.cm;			version=project, \
    org.osgi.test.support;			version=project, \
	osgi.core;						version=4.1, \
	ee.minimum; 					version=1.2, \
	${junit}

-runbundles							= \
	osgi.cmpn; version=latest, \
	org.osgi.impl.service.log; version=latest, \
	org.osgi.impl.service.cm; version=latest
	
-runproperties = ${runsecureproperties}




