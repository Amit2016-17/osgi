Bundle-Version						: 4.2.0.${project.build}
Bundle-Description: Configuration Management Test R4
Bundle-Vendor: OSGi Alliance
Bundle-DocURL: http://www.osgi.org/
Bundle-ContactAddress: info@mail.osgi.org
Bundle-Category: osgi,test

Private-Package						= \
	org.osgi.test.support.*, \
	${p}.junit, \
	${p}.common
	
Include-Resource					= \
	tb1.jar, \
	tb2.jar, \
	setallpermission.jar, \
	targetb1.jar, \
	targetb2.jar
	
Export-Package = \
	${p}.shared;version=1.0.0
	
Test-Cases							= ${testcases}
-signaturetest                      = org.osgi.service.cm

-buildpath							= \
	org.osgi.service.cm;			version=project, \
    org.osgi.test.support;			version=project, \
	osgi.core;						version=4.0, \
	ee.minimum; 					version=1.2, \
	${junit}

-runbundles							= \
	org.osgi.impl.service.log; version=latest, \
	org.apache.felix.configadmin; version=latest, \
	osgi.cmpn; version=4.2
	
-runproperties = report=true, \
	org.osgi.framework.security=osgi, \
	org.osgi.framework.trust.repositories=${build}/keystore




