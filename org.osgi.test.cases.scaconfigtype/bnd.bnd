javac.source            = 1.5
javac.target            = 1.5
Bundle-Version: 4.2.0.${project.build}

Import-Package = junit.framework;version="3.8.0",*

Private-Package						= org.osgi.test.support.*, \
	${p}.*

Include-Resource					= \
	ct00.jar, \
	ct00client.jar, \
	ct04defaultLocation.jar, \
	ct04specificLocation.jar, \
	ct04wildcardLocation.jar, \
	ct06.jar, \
	ct09.jar, \
	ct12.jar, \
	ct15.jar, \
	ct26.jar, \
	ct26client.jar, \
	
-buildpath							= \
    org.osgi.test.support;                  version=project , \
	osgi.core;								version=4.2		, \
	osgi.enterprise;						version=4.2	, \
	ee.j2se;								version=1.6	, \
	${junit}

-runbundles							= \
	osgi.enterprise; version=${enterprise.published.version};strategy=lowest	
	
-runproperties = ${runproperties}, \
	${p}.system.packages.extra="${junit.package}", \
	${p}.bundles="${uniq;${repo;org.osgi.impl.service.event},${repo;org.osgi.impl.bundle.sca},${repo;osgi.enterprise;${enterprise.published.version};lowest},${repo;org.osgi.impl.service.log}}", \
	${p}.serverconfig="service.exported.configs,org.osgi.sca.domain.registry", \
	service.exported.configs=org.osgi.sca, \
	org.osgi.sca.domain.registry="tribes://228.0.0.100:50000"

Test-Cases							= ${testcases}

Bundle-RequiredExecutionEnvironment = J2SE-1.5,JavaSE-1.6 	
