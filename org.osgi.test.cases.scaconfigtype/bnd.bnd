javac.source            = 1.5
javac.target            = 1.5
Bundle-Version: 4.2.0.${project.build}

Import-Package = junit.framework;version="3.8.0",*

Private-Package						= org.osgi.test.support.*, \
	${p}.*

Include-Resource					= \
	ct00.jar, \
	ct00client.jar, \
	ct04defaultLocation.jar, \
	ct04specificLocation.jar, \
	ct04wildcardLocation.jar, \
	ct06.jar, \
	ct09.jar, \
	ct12.jar, \
	ct15.jar, \
	ct26.jar, \
	ct26client.jar, \
	
-buildpath							= \
    org.osgi.test.support;                  version=project , \
	osgi.core;								version=4.2		, \
	osgi.enterprise;						version=4.2	, \
	osgi.cmpn;                              version=4.2    , \
	ee.j2se;								version=1.6.0	, \
	${junit}

-runbundles							= \
	osgi.cmpn;								version=latest	
	
# TODO bundles contains ${repo;com.springsource.junit} factor out to ${junit} as above
-runproperties = report=true, \
	osgi.compatibility.bootdelegation=false, \
	osgi.resolverMode=strict, \
	osgi.support.multipleHosts=true, \
	${p}.bundles=${repo;org.eclipse.equinox.event}|\${repo;com.springsource.junit}|${repo;org.apache.tuscany.sca.bundle}|${repo;osgi.cmpn}|${repo;org.osgi.impl.service.log}, \
	${p}.serverconfig="service.exported.configs,org.osgi.sca.domain.registry", \
	service.exported.configs=org.osgi.sca, \
	org.osgi.sca.domain.registry="tribes://228.0.0.100:50000"

Test-Cases							= ${testcases}

Bundle-RequiredExecutionEnvironment = J2SE-1.5,JavaSE-1.6 	
