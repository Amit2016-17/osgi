javac.source            = 1.5
javac.target            = jsr14
Bundle-Version: 4.2.0.${project.build}

Private-Package						= org.osgi.test.support.*, \
	${p}.*

Include-Resource					= \
	ct12.jar, \
	ct13.jar, \
	
-buildpath							= \
    org.osgi.test.support;                  version=project , \
	org.osgi.service.remoteserviceadmin;	version=project	, \
	osgi.core;								version=4.2		, \
	osgi.cmpn;								version=4.2	, \
	ee.minimum;								version=1.2.1	, \
	${junit}

-runbundles							= \
	org.apache.tuscany.sca.bundle;      version=2.0, \
	osgi.cmpn;								version=latest	, \
	org.osgi.impl.service.log;				version=latest
	
# TODO bundles contains ${repo;com.springsource.junit} factor out to ${junit} as above
-runproperties = report=true, \
	osgi.compatibility.bootdelegation=false, \
	osgi.resolverMode=strict, \
	osgi.support.multipleHosts=true, \
	${p}.bundles=${repo;com.springsource.junit}|${repo;org.apache.tuscany.sca.bundle}|${repo;osgi.core}|${repo;osgi.cmpn}|${repo;org.osgi.impl.service.log}, \
	${p}.serverconfig="service.exported.configs,org.apache.cxf.ws.address", \
	service.exported.configs=org.apache.cxf.ws, \
	org.apache.cxf.ws.address=http://localhost:9090/test

Test-Cases							= ${testcases}
	
