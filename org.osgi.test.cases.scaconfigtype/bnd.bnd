# Set javac settings from JDT prefs
-include: ${project.workspace}/cnf/eclipse/jdt.bnd

Conditional-Package					= org.osgi.test.support.*
Private-Package						=  \
	${p}.*

Include-Resource					= \
	ct00.jar, \
	ct00client.jar, \
	ct00event.jar, \
	ct01endpoint.jar, \
	ct04defaultLocation.jar, \
	ct04specificLocation.jar, \
	ct04wildcardLocation.jar, \
	ct06.jar, \
	ct08.jar, \
	ct09.jar, \
	ct12.jar, \
	ct15.jar, \
	ct23.jar, \
	ct24service.jar, \
	ct24configA.jar, \
	ct24configAB.jar, \
	ct26.jar, \
	
-buildpath							= \
    org.osgi.test.support;                  version=project , \
	osgi.core;								version=4.3		, \
	osgi.cmpn;						version=4.3	, \
	ee.j2se;								version=1.5	, \
	${junit}

-runbundles							= \
	org.osgi.impl.service.log; version=latest, \
	org.osgi.impl.service.cm; version=latest, \
	org.osgi.impl.service.event; version=latest
	
-runproperties = ${runproperties}, \
        ${p}.required.ports="9000,50000", \
        ${p}.system.packages.extra="${junit.package}", \
        ${p}.bundles="${uniq;${repo;org.osgi.impl.service.log},${repo;org.osgi.impl.service.cm},${repo;org.osgi.impl.service.event},${repo;org.osgi.impl.bundle.sca}}", \
        ${p}.serverconfig="service.exported.configs,org.osgi.sca.domain.registry", \
        service.exported.configs=org.osgi.sca, \
        org.osgi.sca.domain.registry="tribes://224.0.0.100:50000?receiverPort=9001&receiverAutoBind=2&receiverAddress=localhost&bind=localhost" 

Test-Cases							= ${testcases}

Bundle-RequiredExecutionEnvironment = J2SE-1.5	
