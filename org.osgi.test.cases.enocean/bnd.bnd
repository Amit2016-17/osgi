# Set javac settings from JDT prefs
-include: ${build}/eclipse/jdt.bnd

Bundle-Description					= EnOcean Test Bundle
Conditional-Package					= org.osgi.test.support.*
Private-Package                     = ${p}.*

Import-Package: ${-signaturetest}, *

Include-Resource: 

-buildpath							= \
	org.osgi.service.enocean;		version=project	, \
    org.osgi.test.support;          version=project , \
	osgi.core;						version=4.0.1		, \
	osgi.cmpn;						version=4.2		, \
	ee.foundation;					version=1.1		, \
	${junit}
	
-runbundles						= \
	org.osgi.impl.service.log; version=latest, \
	org.osgi.impl.service.cm; version=latest, \
	org.osgi.impl.service.event; version=latest, \
	org.osgi.impl.service.enocean; version=latest

-runproperties = ${runproperties}, \
	org.osgi.service.enocean.host.path=test-enocean-driver

Test-Cases							= ${testcases}
-signaturetest                      = org.osgi.service.enocean


