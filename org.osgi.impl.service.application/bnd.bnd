# Set javac settings from JDT prefs
-include: ${build}/eclipse/jdt.bnd

Bundle-Activator				= ${p}.Activator

-privatepackage 					= ${p}.*
Export-Package					= org.osgi.service.application; -split-package:=first; provide:=true

-buildpath						= \
	org.osgi.service.application;	version=project, \
	osgi.core;					version=4.3.1, \
	osgi.cmpn;					version=4.3.1
