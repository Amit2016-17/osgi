<?xml version="1.0" encoding="UTF-8"?>
<project name="project" default="_prepare"> 

	<target name="_prepare" depends="init">
		<copy todir="target/delivered" overwrite="true">
			<fileset dir="${workspace}/org.osgi.test.cases.deploymentadmin.mo/delivered" />
		</copy>
		<copy file="${workspace}/cnf/certificate/.keystore" tofile="target/keystore" overwrite="true"/>
		<subant target="publish" inheritAll="false" buildpath="${project.buildpath}" />
	</target>

	<import file="../cnf/build.xml"/>
</project>
