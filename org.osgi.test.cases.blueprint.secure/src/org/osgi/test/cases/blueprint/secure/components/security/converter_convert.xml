<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0">

    <description>
        Tests security permissions in a type-converter convert() method.
    </description>

	<type-converters>
        <bean id="converter" class="org.osgi.test.cases.blueprint.services.SecurityTestBean"/>
	</type-converters>

<!-- Property -->
    <bean id="compEuro" class="org.osgi.test.cases.blueprint.components.injection.RegionCodeInjection">
		<argument value="compEuro"/>
		<property name="regionCode"><value type="org.osgi.test.cases.blueprint.components.injection.EuropeanRegionCode">UK+86</value></property>
	</bean>

</blueprint>