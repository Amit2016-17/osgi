<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0">

    <description>
        Generate a security exception on an instance factory callback.
    </description>

    <bean id="factory" class="org.osgi.test.cases.blueprint.services.SecurityTestBean"/>

    <bean id="unsafe" factory-ref="factory" factory-method="makeStatic">
       <argument value="ServiceOne"/>
   </bean>
</blueprint>

