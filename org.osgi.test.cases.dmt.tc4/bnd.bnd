# Set javac settings from JDT prefs
-include: ${project.workspace}/cnf/eclipse/jdt.bnd

Bundle-Description: Dmt Test Case 004
Conditional-Package					= org.osgi.test.support.*
Private-Package						= ${p}.*	
Include-Resource: tb1.jar=resources/DMTTest_Bundles-1.0.0.jar

Test-Cases							= ${testcases}

-buildpath							= \
	org.osgi.service.dmt;					version=project, \
    org.osgi.test.support;                  version=project , \
	osgi.core;								version=4.2	, \
	osgi.cmpn;								version=4.2	, \
	ee.minimum;								version=1.2	, \
	${junit}

-runbundles							= \
	osgi.cmpn; version=latest, \
	org.osgi.impl.service.log; version=latest, \
	org.osgi.impl.service.cm; version=latest, \
	org.osgi.impl.service.event; version=latest, \
	org.osgi.impl.service.dmt;  version=project

-runproperties = ${runsecureproperties}, \
	org.osgi.vendor.dmtree.DigestDelegate=org.osgi.impl.service.dmt.export.Digest, \
	org.osgi.dmtree.max.segment.name.length=33, \
	org.osgi.dmtree.max.uri.length=129, \
	org.osgi.dmtree.max.uri.segments=21

