<project name="osgi.released" default="buildx">
	
	<import file="../cnf/build.xml"/> 
	
	<target name="buildx">
		<property name="release.dir" value="${project}"/>
		<foreach delimiter=" ," list="${jars.compile.order}" target="subbuild"  param="item.dir"/>
	</target>
	
	<target name="subbuild">
		<ant antfile="${workspace}/${item.dir}/build.xml" dir="${workspace}/${item.dir}" target="publish" inheritall="false">
			<property name="release.dir" value="${project}"/>
		</ant>
	</target>

</project>
