# Set javac settings from JDT prefs
-include: ${project.workspace}/cnf/eclipse/jdt.bnd

Bundle-Description: Tests the XML service

Private-Package						= org.osgi.test.support.*, \
	${p}.junit.*
	
Export-Package						= \
	org.osgi.util.xml;-split-package:=merge-first


Include-Resource = \
	tb1.jar, \
	tb2.jar, \
	tb3.jar
	
Test-Cases							= ${testcases}
-signaturetest                      = org.osgi.util.xml

-buildpath							= \
    org.osgi.test.support;                  version=project , \
	org.osgi.util.xml;						version=project	, \
	com.icl.saxon;			 				version=latest, \
	osgi.core;								version=4.0	, \
	ee.minimum; 							version=1.2	, \
	${junit}

-runbundles							= \
	osgi.cmpn; version=latest, \
	org.osgi.impl.bundle.jaxp; version=project
	

-runproperties = ${runproperties}
