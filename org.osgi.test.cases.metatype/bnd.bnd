# Set javac settings from JDT prefs
-include: ${build}/eclipse/jdt.bnd

Bundle-Description: Test Bundle for Metatype Service tests

Conditional-Package					= org.osgi.test.support.*
Private-Package						=  \
	${p}.junit.*
Import-Package: ${-signaturetest}, *

Include-Resource					= \
	tb1.jar, \
	tb1-r6.jar, \
	tb2.jar, \
	tb2-r6.jar, \
	tb3.jar, \
	tb3-r6.jar, \
	tb4.jar, \
	tb4-r6.jar, \
	bug2487.jar, \
	bug2487-r6.jar

Test-Cases							= ${testcases}
-signaturetest                      = org.osgi.service.metatype

-buildpath							= \
    org.osgi.test.support;                  version=project , \
    org.osgi.service.metatype;              version=latest , \
	osgi.core;								version=6.0	, \
	osgi.cmpn;								version=6.0	, \
	${junit}

-runbundles							= \
    ${junit},\
	org.osgi.impl.service.cm; version=latest, \
	org.osgi.impl.service.log; version=latest, \
	org.osgi.impl.service.metatype; version=latest

-runproperties = ${runproperties}
