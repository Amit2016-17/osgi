# Set javac settings from JDT prefs
-include: ${build}/eclipse/jdt.bnd

Bundle-Activator: 		org.osgi.impl.service.log.Log
Export-Package: 		org.osgi.service.log; -split-package:=first; provide:=true
Private-Package: 		${p}.*

Bundle-Name: 			impl.service.log
Bundle-Description: 	OSGi Log Service Reference Implementation by IBM Corporation.
Bundle-Category: 		osgi
Export-Service: 		org.osgi.service.log.LogService

-buildpath = \
	org.osgi.service.log; 	version=project, \
	osgi.core;				version=4.0, \
	ee.minimum;				version=1.1
