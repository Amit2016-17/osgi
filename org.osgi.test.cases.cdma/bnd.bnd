# Set javac settings from JDT prefs
-include: ${project.workspace}/cnf/eclipse/jdt.bnd

Private-Package						= org.osgi.test.support.*, \
	${p}.junit

-buildpath							= \
	org.osgi.util.cdma;					version=project, \
	org.osgi.test.support;				version=project, \
	osgi.core;							version=4.0.1, \
	ee.minimum;							version=1.1, \
	${junit}

-runbundles						= \
	osgi.cmpn; version=latest

-runproperties					= ${runproperties}

Test-Cases							= ${testcases}
-signaturetest                      = org.osgi.util.cdma
	