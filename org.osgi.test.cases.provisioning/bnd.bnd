Bundle-Version: 4.2.0.${project.build}
Bundle-Activator: ${p}.junit.Activator

Private-Package						= org.osgi.test.support.*, \
	${p}.junit

Include-Resource: ipa/simple.ipa=simple.ipa, \
	ipa/ref.ipa=ref.ipa, \
	ipa/osgi.cert=osgi.cert, \
	ipa/case-ref.ipa=case-ref.ipa, \
	ipa/delay-ref.ipa=delay-ref.ipa, \
	ipa/spid.ipa=spid.ipa, \
	ipa/ipa-ref-start.ipa=ipa-ref-start.ipa, \
	ipa/ipa-ref.ipa=ipa-ref.ipa, \
	ipa/keys-only.ipa=keys-only.ipa, \
	ipa/unicode.ipa=unicode.ipa, \
	www/test-1.jar=ximple.jar, \
	www/delay.jar=ximple.jar, \
	www/simple.jar=ximple.jar
	
-buildpath							= \
    org.osgi.test.support;                  version=project , \
	org.osgi.service.provisioning;			version=project	, \
	org.osgi.impl.bundle.servlet;		version=2.1, \
	osgi;									version=4.1		, \
	ee.j2se; version=1.4												, \
	${junit}

-runbundles						= \
	org.osgi.impl.service.log; version=latest, \
	org.osgi.impl.bundle.servlet;		version=2.1, \
	org.osgi.impl.service.http; version=latest, \
	org.osgi.impl.service.provisioning; version=project, \
	org.osgi.impl.bundle.rsh;		version=latest

-runproperties = report=true, \
	org.osgi.service.http.hostname=127.0.0.1, \
	org.osgi.service.http.port=8080


Test-Cases: ${classes;extending;org.junit.framework.TestCase|*DefaultTestBundleControl}


