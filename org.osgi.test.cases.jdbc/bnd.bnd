Bundle-Version: 3.0.0.${project.build}

Private-Package						= ${p}.*, \
	org.osgi.test.support.*

-buildpath							= \
	osgi;									version=4.0		, \
	ee.j2se;version=latest												, \
	org.osgi.test.support; 	version=project, \
	org.osgi.service.jdbc; 	version=project, \
	${junit}

-runbundles						= \
	osgi.cmpn; version=latest, \
	org.osgi.impl.service.jdbc; version=project, \
	com.springsource.org.apache.derby; version=latest

-runproperties					= report=true

Test-Cases: ${classes;extending;*TestCase}
	
