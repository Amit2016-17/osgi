# Set javac settings from JDT prefs
-include: ${project.workspace}/cnf/eclipse/jdt.bnd

Conditional-Package					= org.osgi.test.support.*
Private-Package						=  \
    ${p}.junit.*, \
    ${p}.util.*
Import-Package: ${-signaturetest}, *

-buildpath:  \
	org.osgi.test.support;version=project,\
	org.osgi.service.usbinfo;version=project,\
	osgi.core;version=4.2,\
	osgi.cmpn;version=4.2,\
	ee.minimum;version=1.2,\
	${junit}


-runproperties = ${runproperties}, \
    bcdUSB="0210", \
    bDeviceClass="ff", \
    bDeviceSubClas="ff", \
    bDeviceProtoco="ff", \
    bMaxPacketSize0="128", \
    idVendor="0403", \
    idProduct="8372", \
    bcdDevice="0200", \
    manufacturer="Buffalo Inc.", \
    product="USB2.0 PC Camera", \
    serialNumber="57B0002600000001", \
    bNumConfigurations="7", \
    bInterfaceNumber="8", \
    bAlternateSetting="9", \
    bNumEndpoints="10", \
    bInterfaceClass="ff", \
    bInterfaceSubClass="ff", \
    bInterfaceProtocol="ff", \
    interface="interface", \
    bus="1", \
    address="2", \
    \
    bcdUSB.2="0211", \
    bDeviceClass.2="fe", \
    bDeviceSubClas.2="fe", \
    bDeviceProtoco.2="fe", \
    bMaxPacketSize0.2="128", \
    idVendor.2="0404", \
