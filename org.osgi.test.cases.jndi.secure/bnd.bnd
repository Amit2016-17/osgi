# Set javac settings from JDT prefs
-include: ${project.workspace}/cnf/eclipse/jdt.bnd

Export-Package						= \
	${p}.provider, \
	${p}.provider.ct

Conditional-Package					= org.osgi.test.support.*
Private-Package						=  \
	${p}.*
	
Import-Package 						= \
	* 


Include-Resource 					= \
	{OSGI-INF/permissions.perm = perms/permissions.jndi.perm} , \
	builderBundle.jar , \
	factoryBundle.jar , \
	inaccessibleBundleContext.jar
	
Test-Cases							= ${testcases}

-buildpath							= ee.j2se; version=1.5, \
	org.osgi.service.jndi;					version=project, \
    org.osgi.test.support;                  version=project, \
	osgi.core; 								version=4.2, \
	${junit}
	
-runbundles = \
	osgi.enterprise; version=latest		, \
	org.osgi.impl.service.jndi; version=project

-runproperties = ${runsecureproperties}, \
	org.osgi.framework.bootdelegation=com.sun.*
