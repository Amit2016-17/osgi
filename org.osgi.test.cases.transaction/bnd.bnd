Bundle-Version: 4.2.0.${project.build}
Bundle-RequiredExecutionEnvironment = J2SE-1.5

Private-Package						= ${p}.*

-buildpath							= \
	org.apache.geronimo.specs.geronimo-j2ee-connector_1.5_spec;version=2.0,\
	org.apache.geronimo.specs.geronimo-jta_1.1_spec;version=1.1.1,\
	ee.j2se;								version=1.4,\
	osgi;									version=4.2,\
	${junit}

-runsystempackages=javax.naming,javax.security.auth
-runbundles						= \
	org.osgi.impl.bundle.transaction; version=project
#-runbundles=org.osgi.tools.console

-runproperties					= report=true

Test-Cases: ${classes;extending;*TestCase}

	