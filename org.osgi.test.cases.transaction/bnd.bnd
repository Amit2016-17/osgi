Bundle-Version: 4.2.0.${project.build}
Bundle-RequiredExecutionEnvironment = J2SE-1.5
Signature-Packages:  javax.transaction, javax.transaction.xa

Private-Package						= org.osgi.test.support.*, \
	${p}.*
	
Import-Package = \
 	javax.transaction;version=1.1, \
	javax.transaction.xa;version=1.1, \
 	*

 Export-Package =\
	org.osgi.test.cases.transaction.util;version=1.0, \
	javax.transaction.*, \
	javax.transaction.xa.*
	
-buildpath							= \
	org.apache.geronimo.specs.geronimo-j2ee-connector_1.5_spec;version=2.0, \
	org.apache.geronimo.specs.geronimo-jta_1.1_spec;version=1.1.1, \
	ee.j2se;								version=1.4, \
	osgi.core;									version=4.2, \
	org.osgi.test.support;          		version=project , \
	${junit}

-runbundles						= \
	org.osgi.impl.bundle.transaction; version=project
	
-runproperties					= report=true, \
    org.osgi.test.cases.transaction.waittime=30

Test-Cases						= ${classes;extending;org.junit.framework.TestCase|*TransactionTestBundleControl|*DefaultSignatureTestControl}

	