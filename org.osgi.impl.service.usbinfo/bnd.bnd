# Set javac settings from JDT prefs
-include: ${project.workspace}/cnf/eclipse/jdt.bnd

Bundle-Activator			= ${p}.Activator
Bundle-Description			= USB Base Driver for a Reference Implementation.

Private-Package 			= ${p}.*
Export-Package				= org.osgi.service.usbinfo; -split-package:=first; provide:=true

-buildpath:  \
	osgi.core;version=4.2,\
	osgi.cmpn;version=4.2,\
	ee.minimum;version=1.2,\
	org.osgi.service.usbinfo;version=project,\
	org.osgi.test.support;version=latest

