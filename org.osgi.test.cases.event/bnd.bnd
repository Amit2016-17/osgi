Bundle-Version						: 3.0.0.${project.build}
Bundle-Description					: A test case for the event generic mechanism implementation.

Private-Package						= org.osgi.test.support.*
Export-Package						= \
	org.osgi.service.event, \
	${p}.tbc.*

Include-Resource					= \
	tb1.jar, \
	tb2.jar
	
Test-Cases							= ${classes;extending;org.junit.framework.TestCase|*DefaultTestBundleControl}

-buildpath							= \
    org.osgi.test.support;                  version=project , \
    org.osgi.service.event;                 version=project , \
	osgi;									version=4.0.1	, \
	ee.foundation											, \
	${junit}

-runbundles							= \
	org.eclipse.equinox.event; version=latest, \
	osgi; version=4.0.1
	


-runproperties = report=true
