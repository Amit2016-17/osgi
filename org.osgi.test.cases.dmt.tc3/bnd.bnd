Bundle-Version: 4.2.0.${project.build}
Bundle-Description: Dmt Test Case 003
Private-Package						= ${p}.tbc.*, \
	org.osgi.test.support.*

Test-Cases							= ${classes;extending;org.junit.framework.TestCase|*DefaultTestBundleControl}

-buildpath							= \
	info.dmtree;							version=project, \
    org.osgi.test.support;                  version=project , \
	osgi.core;								version=4.0.1		, \
	osgi.mobile;							version=4.0		, \
	ee.minimum;								version=1.2		, \
	${junit}

-runbundles							= \
	osgi.mobile; version=latest, \
	org.osgi.impl.service.log; version=latest, \
	org.apache.felix.configadmin; version=latest, \
	org.eclipse.equinox.event; version=latest, \
	org.osgi.impl.service.dmt;  version=project

-runproperties = report=true
	org.osgi.framework.security=osgi, \
	org.osgi.framework.trust.repositories=${build}/keystore
	org.osgi.vendor.dmtree.DigestDelegate=org.osgi.impl.service.dmt.export.Digest, \
	info.dmtree.osgi.root=./OSGi/
	