<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd"
   version="1.0">
	<persistence-unit name="testUnit7">
		<description>Persistence unit to test reliance on specific provider where the installed provider does not match the specified range</description>
		<provider>org.apache.openjpa.persistence.PersistenceProviderImpl</provider>
		<properties>
			<property name="osgi.jpa.provider.version" value="[2.0.0,3.0.0)"/>
		</properties>
	</persistence-unit>
</persistence>