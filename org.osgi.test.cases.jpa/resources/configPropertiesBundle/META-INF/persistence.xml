<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd"
   version="1.0">
	<persistence-unit name="configPropertiesTestUnit">
		<description>Persistence unit for testing that configuration properties are merged and overwrite duplicates from the persistence unit</description>
		<properties>
			<property name="javax.persistence.jdbc.user" value="testUser"/>
			<property name="javax.persistence.jdbc.password" value="persistencePassword"/>
		</properties>
	</persistence-unit>
</persistence>