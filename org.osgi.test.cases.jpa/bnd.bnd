javac.source 			= 1.6
javac.target 			= 1.6

Bundle-Version: 4.2.0.${project.build}

Export-Package						= \
	org.osgi.service.jpa
	
Private-Package						= \
	org.osgi.test.support.* , \
	${p}.*

Include-Resource 					= \
	emfBundle.jar	, \
	emfBuilderBundle.jar ,	\
	defaultPersistenceLocation.jar , \
	nonStandardPersistenceLocation.jar , \
	multiplePersistenceLocations.jar , \
	nestedJarPersistenceLocation.jar

-buildpath							    = \
	ee.j2se; 						    version=1.5, \
    org.osgi.test.support;              version=project , \
	org.osgi.service.jpa;			    version=project	, \
	org.osgi.service.jdbc;			    version=project , \
	osgi.core;						    version=4.2		, \
	org.eclipse.persistence.jpa_spec;	version=2.0, \
	${junit}
	
-runbundles						= \
	osgi.enterprise; version=${enterprise.published.version};strategy=lowest, \
	org.osgi.service.jdbc;			    version=project , \
	org.eclipse.persistence.jpa_spec;	version=2.0 , \
	org.eclipse.gemini.dbaccess.derby.cs;	version=1.0.0 , \
    org.eclipse.persistence.asm;        version=2.0.1 , \
    org.eclipse.persistence.antlr;      version=2.0.1 , \
    org.eclipse.persistence.incubator.geminijpa.core; version=2.0.2 , \ 
    org.eclipse.persistence.incubator.geminijpa.jpa; version=2.0.2 , \ 
    org.eclipse.gemini.jpa;             version=1.0.0
	
-runproperties					= ${runproperties}

Test-Cases							= ${testcases}

-signaturetest                      = org.osgi.service.jpa
	