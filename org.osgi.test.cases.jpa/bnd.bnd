javac.source 			= 1.5
javac.target 			= 1.5

Bundle-Version: 4.2.0.${project.build}

Export-Package						= \
	org.osgi.service.jpa
	
Private-Package						= \
	org.osgi.test.support.* , \
	${p}.*

Include-Resource 					= \
	defaultPersistenceLocation.jar , \
	nonStandardPersistenceLocation.jar , \
	multiplePersistenceLocations.jar , \
	nestedJarPersistenceLocation.jar

-buildpath							    = \
	ee.j2se; 						    version=1.5, \
    org.osgi.test.support;              version=project , \
	org.osgi.service.jpa;			    version=project	, \
	org.osgi.service.jdbc;			    version=project , \
	osgi.core;						    version=4.2		, \
	org.eclipse.persistence.jpa_spec;	version=2.0, \
	${junit}
	
-runbundles						= \
	org.eclipse.persistence.jpa_spec;	        version=2.0 , \
	org.eclipse.gemini.dbaccess.derby.cs;	    version=1.0 , \
	org.eclipse.gemini.jpa;	                    version=1.0 

	
-runproperties					= report=true

Test-Cases							= ${testcases}

-signaturetest                      = org.osgi.service.jpa
	