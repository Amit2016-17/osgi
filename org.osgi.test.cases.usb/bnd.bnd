# Set javac settings from JDT prefs
-include: ${project.workspace}/cnf/eclipse/jdt.bnd

Conditional-Package					= org.osgi.test.support.*
Private-Package						=  \
    ${p}.junit.*, \
    ${p}.util.*
Import-Package: ${-signaturetest}, *

-buildpath							= \
    org.osgi.test.support;						version=project , \
    org.osgi.service.usb;						version=project	, \
    osgi.core;									version=4.3		, \
    osgi.cmpn;									version=4.2		, \
    ee.minimum;									version=1.2.1	, \
    org.osgi.test.cases.dal.step;version=project, \
    ${junit}

-runbundles						= \
    org.osgi.impl.service.log; version=latest, \
    org.osgi.impl.service.device.manager; version=latest, \
    org.osgi.test.cases.dal.step; version=latest, \
    org.osgi.impl.service.usb; version=latest

-runproperties = ${runproperties}, \
    bcdUSB="0x0210", \
    bDeviceClass="0xff", \
    bDeviceSubClass="0xff", \
    bDeviceProtocol="0xff", \
    idVendor="0x0403", \
    idProduct="0x8372", \
    bcdDevice="0x0200", \
    iManufacturer="Buffalo Inc.", \
    iProduct="USB2.0 PC Camera", \
    iSerialNumber="57B0002600000001", \
    interfaceclasses="0x080000,0x0a00ff", \
    bus="3", \
    address="2", \
    bcdUSB.2="0x0210", \
    bDeviceClass.2="0xff", \
    bDeviceSubClass.2="0xff", \
    bDeviceProtocol.2="0xff", \
    idVendor.2="0x0403", \
    idProduct.2="0x8372", \
    bcdDevice.2="0x0200", \
    iManufacturer.2="Buffalo Inc.", \
    iProduct.2="USB2.0 PC Camera", \
    iSerialNumber.2="57B0002600000001", \
    interfaceclasses.2="0x080000,0x0a00ff", \
    bus.2="3", \
    address.2="3", \
    serial.bcdUSB="0x0210", \
    serial.bDeviceClass="0xff", \
    serial.bDeviceSubClass="0xff", \
    serial.bDeviceProtocol="0xff", \
    serial.idVendor="0x0403", \
    serial.idProduct="0x8372", \
    serial.bcdDevice="0x0200", \
    serial.interfaceclasses="0x080000,0x0a00ff", \
    serial.bus="3", \
