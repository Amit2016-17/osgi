    storage.interfaceclasses="0x080000,0x0a00ff", \
    storage.bus="3", \
    storage.address="2", \
    storage.mountpoints="/mnt/media/usb-storage-01/"

Test-Cases							= ${testcases}
-signaturetest                      = org.osgi.service.usb
