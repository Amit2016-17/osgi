Bundle-Version: 1.0.0.${project.build}

Private-Package						= ${p}.tbc.*, \
	org.osgi.test.support.*

Include-Resource	=  \	
	www/register.jar=register.jar
	
Export-Package = \
	${p}.util;version=1.0.0
	
Test-Cases							= ${classes;extending;org.junit.framework.TestCase|*DefaultTestBundleControl}

-buildpath							= \
	org.osgi.test.support;                  version=project , \
	osgi;									version=latest	, \
	ee.foundation											, \
	${junit}

-runproperties = \
	report=true, \
	org.osgi.framework.security=osgi
	
	
-runbundles							= \
	org.osgi.impl.service.log; version=project, \
	org.osgi.impl.service.cm;  version=project, \
	org.osgi.impl.service.http; version=project, \
	org.osgi.impl.bundle.servlet
