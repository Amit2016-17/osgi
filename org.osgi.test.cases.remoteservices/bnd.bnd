# Set javac settings from JDT prefs
-include: ${project.workspace}/cnf/eclipse/jdt.bnd

Bundle-Description					: Remote Services test cases.

Export-Package 					= ${p}.common;version=1.0.0
	
Conditional-Package					= org.osgi.test.support.*
Private-Package						=  \
	${p}.*
	
Test-Cases							= ${testcases}

-buildpath							= \
    org.osgi.test.support;                  version=project , \
	osgi.core;								version=4.3.1	, \
	osgi.cmpn;								version=4.3.1	, \
	ee.minimum; 							version=1.2.1, \
	${junit}

-runbundles							= \
	osgi.cmpn; version=latest	, \
	org.osgi.impl.service.remoteserviceadmin;  version=latest, \
	org.osgi.impl.bundle.remoteserviceadmin.discovery;  version=latest, \
	org.osgi.impl.bundle.remoteserviceadmin.discovery.config;  version=latest, \
	org.osgi.impl.service.log;				version=latest
	

-runproperties = ${runproperties}, \
	rsa.ct.timeout=300000,\
	rsa.ct.timeout.factor=3,\
	${p}.bundles="${uniq;${repo;org.osgi.impl.service.remoteserviceadmin},${repo;org.osgi.impl.bundle.remoteserviceadmin.discovery.config},${cmpn.companion.jar},${repo;org.osgi.impl.service.log}}", \
	${p}.serverconfig="service.exported.configs,org.apache.cxf.ws.port", \
	service.exported.configs=org.apache.cxf.ws, \
	org.apache.cxf.ws.port=@@FREE_PORT@@

