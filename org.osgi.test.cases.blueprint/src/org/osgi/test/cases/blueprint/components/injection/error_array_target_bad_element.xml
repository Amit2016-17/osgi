<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0">

    <description>
       Tests the situation of an array target and elements from the source cannot convert
    </description>

    <bean id="listToIntArray" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
        <property name="primIntArray">
            <array>
                <value>1</value>
                <value>abc</value>
            </array>
        </property>
    </bean>
</blueprint>

