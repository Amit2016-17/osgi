<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0">

<!-- instance factory -->
    <bean id="compFactory" class="org.osgi.test.cases.blueprint.components.injection.InnerComponentInjectionInstanceFactory">
		<!-- collaborators and configuration for this component go here -->
    </bean>

	<bean id="innerCompFactory" class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjectionInstanceFactory">
		<!-- collaborators and configuration for this component go here -->
    </bean>

<!-- String -->
	<bean id="compInnerStringBothTyped" factory-method="makeInstance" factory-component="compFactory">
		<argument>
			<bean factory-method="makeInstance" factory-component="innerCompFactory">
				<argument type="java.lang.String"><value type="java.lang.String">STR2</value></argument>
			</bean>
		</argument>
	</bean>

	<bean id="compInnerStringValueTyped" factory-method="makeInstance" factory-component="compFactory">
		<argument>
			<bean factory-method="makeInstance" factory-component="innerCompFactory">
				<argument><value type="java.lang.String">STR3</value></argument>
			</bean>
		</argument>
	</bean>

	<bean id="compInnerStringArgTyped" factory-method="makeInstance" factory-component="compFactory">
		<argument>
			<bean factory-method="makeInstance" factory-component="innerCompFactory">
				<argument type="java.lang.String"><value>STR4</value></argument>
			</bean>
		</argument>
	</bean>

	<bean id="compInnerString" factory-method="makeInstance" factory-component="compFactory">
		<argument>
			<bean factory-method="makeInstance" factory-component="innerCompFactory">
				<argument value="STR5" type="java.lang.String"/>
			</bean>
		</argument>
	</bean>

