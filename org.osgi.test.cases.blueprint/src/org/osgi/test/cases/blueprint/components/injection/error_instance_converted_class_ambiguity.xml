<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0">
    <description>
        Tests the ambiguity arising from the a convertibility equivalance between class types
    </description>

    <!-- A factory bean used for the instance factory variations -->
    <bean id="factory" class="org.osgi.test.cases.blueprint.components.injection.TypeClassCompatibility"/>

    <bean id="regionCode" factory-ref="factory" factory-method="makeInstance">
       <argument value="CN+86"/>
    </bean>
</blueprint>

