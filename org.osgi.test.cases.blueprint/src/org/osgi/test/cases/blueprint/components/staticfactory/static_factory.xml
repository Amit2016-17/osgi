<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <bean id="comp1" class="org.osgi.test.cases.blueprint.components.staticfactory.SimpleStaticFactory" factory-method="createSimple">
     <!-- uses no argument method -->
    </bean>

    <bean id="comp2_id" class="org.osgi.test.cases.blueprint.components.staticfactory.SimpleStaticFactory" factory-method="createSimple">
        <!-- uses single argument constructor.  Will have wrong component name if not called -->
        <argument value="comp2_id"/>
    </bean>
</blueprint>

