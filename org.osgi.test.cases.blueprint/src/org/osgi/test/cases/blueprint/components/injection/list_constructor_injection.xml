<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <bean id="compEmptyList" class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
        <constructor-arg>
            <list></list>
        </argument>
    </bean>

    <bean id="compNullList" class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
        <argument type="java.util.List">
            <null/>
        </argument>
    </bean>

    <bean id="compStringItems" class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
        <constructor-arg>
            <list>
                <value>abc</value>
                <value>def</value>
            </list>
        </argument>
    </bean>

    <bean id="compDupItems" class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
        <constructor-arg>
            <list>
                <value>abc</value>
                <value>abc</value>
            </list>
        </argument>
    </bean>

    <bean id="compBooleanItems" class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
        <constructor-arg>
            <list>
                <value type="java.lang.Boolean">true</value>
                <value type="java.lang.Boolean">false</value>
            </list>
        </argument>
    </bean>

    <bean id="compMixedItems" class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
        <constructor-arg>
            <list>
                <value type="java.lang.String">abc</value>
                <value type="java.lang.Boolean">false</value>
                <value type="java.lang.Byte">3</value>
                <value type="java.lang.Character">4</value>
                <value type="java.lang.Integer">5</value>
                <value type="java.lang.Short">6</value>
                <value type="java.lang.Long">7</value>
                <value type="java.lang.Double">8.0</value>
                <value type="java.lang.Float">9.0</value>
                <value type="java.net.URL">http://www.osgi.org</value>
                <value type="java.lang.Class">java.lang.String</value>
                <value type="java.util.Locale">en_US</value>
                <null/>
            </list>
        </argument>
    </bean>

    <bean id="compTypedItems" class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
        <constructor-arg>
            <list value-type="java.lang.Double">
                <value>0.0</value>
                <value>1.0</value>
            </list>
        </argument>
    </bean>

    <bean id="compTypeOverride" class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
        <constructor-arg>
            <list value-type="java.lang.Double">
                <value>0.0</value>
                <value>1.0</value>
                <value type="java.lang.Boolean">true</value>
            </list>
        </argument>
    </bean>

    <bean id="compNestedList" class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
        <constructor-arg>
            <list>
                <list>
                    <value>abc</value>
                    <value>def</value>
                </list>
            </list>
        </argument>
    </bean>

    <bean id="compNestedSet" class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
        <constructor-arg>
            <list>
                <set>
                    <value>abc</value>
                    <value>def</value>
                </set>
            </list>
        </argument>
    </bean>

    <bean id="compNestedMap" class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
        <constructor-arg>
            <list>
                <map>
                    <entry key="abc" value="def"/>
                    <entry key="ghi" value="jkl"/>
                </map>
            </list>
        </argument>
    </bean>

    <bean id="compNestedProps" class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
        <constructor-arg>
            <list>
                <props>
                    <prop key="abc">def</prop>
                    <prop key="ghi">jkl</prop>
                </props>
            </list>
        </argument>
    </bean>
</blueprint>
