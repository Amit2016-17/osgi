<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0">

	<type-converters>
		<bean class="org.osgi.test.cases.blueprint.components.injection.ArrayListConverter"/>
		<bean class="org.osgi.test.cases.blueprint.components.injection.LinkedListConverter"/>
		<bean class="org.osgi.test.cases.blueprint.components.injection.VectorConverter"/>
	</type-converters>

    <bean id="arrayList" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
        <property name="arrayList">
            <list>
                <value>abc</value>
                <value>def</value>
            </list>
        </property>
    </bean>

    <bean id="linkedList" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
        <property name="linkedList">
            <list>
                <value>abc</value>
                <value>def</value>
            </list>
        </property>
    </bean>

    <bean id="vector" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
        <property name="vector">
            <list>
                <value>abc</value>
                <value>def</value>
            </list>
        </property>
    </bean>

    <bean id="arrayListFromSet" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
        <property name="arrayList">
            <set>
                <value>abc</value>
                <value>def</value>
                <value>def</value>
            </set>
        </property>
    </bean>

    <bean id="linkedListFromSet" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
        <property name="linkedList">
            <set>
                <value>abc</value>
                <value>def</value>
                <value>def</value>
            </set>
        </property>
    </bean>

    <bean id="vectorFromSet" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
        <property name="vector">
            <set>
                <value>abc</value>
                <value>def</value>
                <value>def</value>
            </set>
        </property>
    </bean>

    <bean id="implicitListFromSet" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
        <property name="list">
            <set>
                <value>abc</value>
                <value>def</value>
                <value>def</value>
            </set>
        </property>
    </bean>

</blueprint>

