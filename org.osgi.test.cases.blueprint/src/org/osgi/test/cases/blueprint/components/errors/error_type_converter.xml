<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <description>
       This is a tests a conversion error from a type converter.
    </description>

	<type-converters>
		<bean id="customBooleanConverter" class="org.osgi.test.cases.blueprint.components.injection.CustomBooleanConverter"/>
	</type-converters>

<!--Constructor-->
	<bean id="compCustomBoolean_cnst" class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
		<argument value="compCustomBoolean_cnst"/>
		<argument type="java.lang.Boolean" value="BAD"/>
	</bean>
</blueprint>

