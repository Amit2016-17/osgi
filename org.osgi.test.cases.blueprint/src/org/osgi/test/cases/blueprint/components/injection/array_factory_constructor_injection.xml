<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0">

	<bean id="compFactory"
		class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjectionInstanceFactory">
	</bean>

	<bean id="compPropsFactory"
		class="org.osgi.test.cases.blueprint.components.injection.PropsConstructorInjection">
	</bean>

	<!-- Injection of an empty array -->
	<bean id="compEmptyArray" factory-method="makeInstance"
		factory-ref="compFactory">
		<argument>
			<array value-type="java.lang.String"></array>
		</argument>
	</bean>

	<!-- Injection of a null array -->
	<bean id="compNullArray" factory-method="makeInstance"
		factory-ref="compFactory">
		<argument type="java.lang.String[]">
			<array value-type="java.lang.String">
				<null />
			</array>
		</argument>
	</bean>

	<!-- Injection of a String array -->
	<bean id="compStringArray" factory-method="makeInstance"
		factory-ref="compFactory">
		<argument>
			<array value-type="java.lang.String">
				<value>abc</value>
				<value>efg</value>
			</array>
		</argument>
	</bean>

	<!-- Injection of a primitive int array -->
	<bean id="compPrimIntArray" factory-method="makePrimInstance"
		factory-ref="compFactory">
		<argument>
			<array value-type="int">
				<value>1</value>
				<value>2</value>
			</array>
		</argument>
	</bean>

	<!-- Injection of a wrapped int array -->
	<bean id="compWrappedIntArray" factory-method="makeWrapperInstance"
		factory-ref="compFactory">
		<argument>
			<array value-type="java.lang.Integer">
				<value>1</value>
				<value>2</value>
			</array>
		</argument>
	</bean>

	<!-- Injection of a primitive boolean array -->
	<bean id="compPrimBoolArray" factory-method="makePrimInstance"
		factory-ref="compFactory">
		<argument>
			<array value-type="boolean">
				<value>true</value>
				<value>false</value>
			</array>
		</argument>
	</bean>

	<!-- Injection of a wrapped boolean array -->
	<bean id="compWrappedBooleanArray" factory-method="makeWrapperInstance"
		factory-ref="compFactory">
		<argument>
			<array value-type="java.lang.Boolean">
				<value type="java.lang.Boolean">true</value>
				<value type="java.lang.Boolean">false</value>
			</array>
		</argument>
	</bean>

	<!-- Injection of a primitive byte items -->
	<bean id="compPrimByteArray" factory-method="makePrimInstance"
		factory-ref="compFactory">
		<argument>
			<array value-type="byte">
				<value>1</value>
				<value>2</value>
			</array>
		</argument>
	</bean>

	<!-- Injection of a wrapped byte array -->
	<bean id="compWrappedByteArray" factory-method="makeWrapperInstance"
		factory-ref="compFactory">
		<argument>
			<array value-type="java.lang.Byte">
				<value>1</value>
				<value>2</value>
			</array>
		</argument>
	</bean>

	<!-- Injection of a primitive char array -->
	<bean id="compPrimCharArray" factory-method="makePrimInstance"
		factory-ref="compFactory">
		<argument>
			<array value-type="char">
				<value>a</value>
				<value>b</value>
			</array>
		</argument>
	</bean>

	<!-- Injection of a wrapped char array -->
	<bean id="compWrappedCharArray" factory-method="makeWrapperInstance"
		factory-ref="compFactory">
		<argument>
			<array value-type="java.lang.Character">
				<value>a</value>
				<value>b</value>
			</array>
		</argument>
	</bean>

	<!-- Injection of a primitive short array -->
	<bean id="compPrimShortArray" factory-method="makePrimInstance"
		factory-ref="compFactory">
		<argument>
			<array value-type="short">
				<value>3</value>
				<value>4</value>
			</array>
		</argument>
	</bean>

	<!-- Injection of a wrapped short array -->
	<bean id="compWrappedShortArray" factory-method="makeWrapperInstance"
		factory-ref="compFactory">
		<argument>
			<array value-type="java.lang.Short">
				<value>3</value>
				<value>4</value>
			</array>
		</argument>
	</bean>

	<!-- Injection of a primitive long array -->
	<bean id="compPrimLongArray" factory-method="makePrimInstance"
		factory-ref="compFactory">
		<argument>
			<array value-type="long">
				<value>3</value>
				<value>4</value>
			</array>
		</argument>
	</bean>

	<!-- Injection of a wrapped long array -->
	<bean id="compWrappedLongArray" factory-method="makeWrapperInstance"
		factory-ref="compFactory">
		<argument>
			<array value-type="java.lang.Long">
				<value>3</value>
				<value>4</value>
			</array>
		</argument>
	</bean>

	<!-- Injection of a primitive double array -->
	<bean id="compPrimDoubleArray" factory-method="makePrimInstance"
		factory-ref="compFactory">
		<argument>
			<array value-type="double">
				<value>3</value>
				<value>4</value>
			</array>
		</argument>
	</bean>

	<!-- Injection of a wrapped double array -->
	<bean id="compWrappedDoubleArray" factory-method="makeWrapperInstance"
		factory-ref="compFactory">
		<argument>
			<array value-type="java.lang.Double">
				<value>3</value>
				<value>4</value>
			</array>
		</argument>
	</bean>

	<!-- Injection of a primitive float array -->
	<bean id="compPrimFloatArray" factory-method="makePrimInstance"
		factory-ref="compFactory">
		<argument>
			<array value-type="float">
				<value>3</value>
				<value>4</value>
			</array>
		</argument>
	</bean>

	<!-- Injection of a wrapped float array -->
	<bean id="compWrappedFloatArray" factory-method="makeWrapperInstance"
		factory-ref="compFactory">
		<argument>
			<array value-type="java.lang.Float">
				<value>3</value>
				<value>4</value>
			</array>
		</argument>
	</bean>

	<!-- Injection of a Date array -->
	<bean id="compDateArray" factory-method="makeInstance"
		factory-ref="compFactory">
		<argument>
			<array value-type="java.util.Date">
				<value>19 Feb 2009</value>
				<value>20 Feb 2009</value>
			</array>
		</argument>
	</bean>

	<!-- Injection of an URL array -->
	<bean id="compURLArray" factory-method="makeInstance"
		factory-ref="compFactory">
		<argument>
			<array value-type="java.net.URL">
				<value>http://www.osgi.org</value>
				<value>http://www.gmail.com</value>
			</array>
		</argument>
	</bean>

	<!-- Injection of a Class array -->
	<bean id="compClassArray" factory-method="makeInstance"
		factory-ref="compFactory">
		<argument>
			<array value-type="java.lang.Class">
				<value>java.lang.String</value>
				<value>java.lang.Boolean</value>
			</array>
		</argument>
	</bean>

	<!-- Injection of a Locale array -->
	<bean id="compLocaleArray" factory-method="makeInstance"
		factory-ref="compFactory">
		<argument>
			<array value-type="java.util.Locale">
				<value>en_US</value>
				<value>zh_CN</value>
			</array>
		</argument>
	</bean>

	<!-- Injection of String values from props -->
	<bean id="compNestedProps" factory-method="makeInstance"
		factory-ref="compPropsFactory">
		<argument type="java.util.Properties[]">
			<array value-type="java.util.Properties">
				<props>
					<prop key="administrator">administrator@example.org
					</prop>
					<prop key="support">support@example.org</prop>
				</props>
			</array>
		</argument>
	</bean>

	<!-- Injection of String values from map -->
	<bean id="compNestedMap" factory-method="makeInstance"
		factory-ref="compFactory">
		<argument>
			<array value-type="java.util.Map">
				<map>
					<entry key="abc" value="1" />
					<entry key="abc" value="2" />
				</map>
			</array>
		</argument>
	</bean>

	<!-- Injection of String values from set -->
	<bean id="compNestedSet" factory-method="makeInstance"
		factory-ref="compFactory">
		<argument>
			<array value-type="java.util.Set">
				<set>
					<value>administrator@example.org</value>
					<value>support@example.org</value>
				</set>
			</array>
		</argument>
	</bean>

	<!-- Injection of String values from list -->
	<bean id="compNestedList" factory-method="makeInstance"
		factory-ref="compFactory">
		<argument>
			<array value-type="java.util.List">
				<list>
					<value>administrator@example.org</value>
					<value>support@example.org</value>
				</list>
			</array>
		</argument>
	</bean>

	<!-- Injection of nested array -->
	<bean id="compNestedArray" factory-method="makeInstance"
		factory-ref="compFactory">
		<argument type="java.lang.String[][]">
			<array value-type="java.lang.String[]">
				<array value-type="java.lang.String">
					<value>abc</value>
					<value>def</value>
				</array>
			</array>
		</argument>
	</bean>

</blueprint>
