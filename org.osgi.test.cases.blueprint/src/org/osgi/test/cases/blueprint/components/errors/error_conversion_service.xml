<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.osgi.org/xmlns/blueprint/v1.0.0 http://www.osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd">

    <description>
       This is a tests the conversion service via a conversion error.
    </description>

    <bean id="ConversionInjection" class="org.osgi.test.cases.blueprint.components.injection.ConversionServiceComponent">
		<argument value="ConversionInjection"/>
		<argument ref="conversionService"/>
        <!-- this will be converted using the converter service -->
        <property name="conversion" value="BAD"/>
	</bean>
</blueprint>

