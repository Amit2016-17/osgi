<?xml version="1.0" encoding="UTF-8"?> 
<components xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://www.osgi.org/xmlns/blueprint/v1.0.0 http://www.osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd">
    
    <description>
        This is a reusable config for validating different service registrations. 
    </description>
    
    
    <!-- single shared instance of the imported service --> 
    <reference id="ServiceOne" interface="org.osgi.test.cases.blueprint.services.TestGoodService"/>
    
   <component class="org.osgi.test.cases.blueprint.components.serviceimport.ServiceReferenceChecker"> 
       <constructor-arg value="ServiceOneChecker"/>
       <property name="good" ref="ServiceOne"/>
   </component> 
</components> 

