<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0">

    <description>
        This is a reusable config for validating different service registrations.
    </description>

    <!-- now one of each using just one of the exported set.  This should map to the same signature -->
    <reference id="ServiceOne" interface="org.osgi.test.cases.blueprint.services.TestServiceOne"/>
    <reference id="ServiceTwo" interface="org.osgi.test.cases.blueprint.services.TestServiceTwo"/>

   <bean id="ServiceOneChecker" class="org.osgi.test.cases.blueprint.components.serviceimport.ServiceReferenceChecker">
       <argument value="ServiceOneChecker"/>
       <property name="one" ref="ServiceOne"/>
   </bean>

   <bean id="ServiceTwoChecker" class="org.osgi.test.cases.blueprint.components.serviceimport.ServiceReferenceChecker">
       <argument value="ServiceTwoChecker"/>
       <property name="two" ref="ServiceTwo"/>
   </bean>
</blueprint>

