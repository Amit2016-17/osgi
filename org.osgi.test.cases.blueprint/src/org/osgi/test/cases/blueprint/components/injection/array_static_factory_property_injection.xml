	<!-- Injection of String values from props -->
	<component id="compNestedProps" factory-method="makeInstance"
		class="org.osgi.test.cases.blueprint.components.injection.PropertyInjectionStaticFactory">
		<property name="propsArray">
			<array value-type="java.util.Properties">
				<props>
					<prop key="administrator">administrator@example.org
					</prop>
					<prop key="support">support@example.org</prop>
				</props>
			</array>
		</property>
	</component>

