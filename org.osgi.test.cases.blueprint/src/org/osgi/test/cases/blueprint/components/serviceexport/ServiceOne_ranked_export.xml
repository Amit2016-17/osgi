<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
    default-init-method="init" default-destroy-method="destroy">

    <description>
        This is just a simple exported service using a single interface.
    </description>

    <!-- the node this service export maps into. -->
   <bean id="ServiceOne" class="org.osgi.test.cases.blueprint.services.TestGoodServiceSubclass">
       <argument value="ServiceOne"/>
   </bean>

   <!-- simple export using a reference and just one interface. The ranking override is the test-->
   <service id="GoodServiceService" ref="ServiceOne" interface="org.osgi.test.cases.blueprint.services.TestServiceOne" ranking="3"/>
</blueprint>

