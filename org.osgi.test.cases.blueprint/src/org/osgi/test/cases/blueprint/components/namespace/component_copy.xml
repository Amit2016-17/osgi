<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
    xmlns:t1="http://www.osgi.org/schema/blueprint/test/test1/v1.0.0"
    xmlns:t2="http://www.osgi.org/schema/blueprint/test/test2/v1.0.0">

    <!-- One of each of the component types -->
    <ref-list id="list" interface="org.osgi.service.blueprint.namespace.NamespaceHandler"/>
    <ref-set id="set" interface="org.osgi.service.blueprint.namespace.NamespaceHandler"/>
    <reference id="handler" interface="org.osgi.service.blueprint.namespace.NamespaceHandler"/>

    <!-- the node this service export maps into. -->
   <bean id="ServiceOne" class="org.osgi.test.cases.blueprint.services.TestGoodServiceSubclass">
       <argument value="ServiceOne"/>
   </bean>

   <!-- simple export using a reference and just one interface. -->
   <service id="ServiceOneService" ref="ServiceOne" interface="org.osgi.test.cases.blueprint.services.TestServiceOne"/>

   <t1:replace-all/>
</blueprint>

