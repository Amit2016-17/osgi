<!-- Properties: 4 cases -->
	<component id="compProperties" factory-method="makeInstance" class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjectionStaticFactory">
		<constructor-arg value="Name=Rex" type="java.util.Properties"/>
	</component>

	<component id="compProperties_EleValue" factory-method="makeInstance" class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjectionStaticFactory">
		<constructor-arg type="java.util.Properties">
			<value>
				Name= Rex
				Organization =OSGi Alliance
				Nationality    :    China
				Language: \u4e2d\u6587, English
				Graduated=Shanghai Jiao Tong University
				#key is the word before the first whitespace - "Major"
				Major Computer Science

				!Gender should be a String ""
				Gender

			</value>
		</constructor-arg>
	</component>

	<component id="compPropertiesEmpty" factory-method="makeInstance" class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjectionStaticFactory">
		<constructor-arg type="java.util.Properties"><value>  </value></constructor-arg>
	</component>

    <component id="compPropertiesNull" factory-method="makeInstance" class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjectionStaticFactory">
		<constructor-arg type="java.util.Properties"><null/></constructor-arg>
	</component>

