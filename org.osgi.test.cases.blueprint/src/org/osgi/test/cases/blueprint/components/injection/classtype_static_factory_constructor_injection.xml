<!-- Locale:5 cases -->
	<component id="compLocale" factory-method="makeInstance" class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjectionStaticFactory">
		<constructor-arg value="en" type="java.util.Locale"/>
	</component>

	<component id="compLocale2" factory-method="makeInstance" class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjectionStaticFactory">
		<constructor-arg value="zh_CN" type="java.util.Locale"/>
	</component>

	<component id="compLocale3" factory-method="makeInstance" class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjectionStaticFactory">
		<constructor-arg value="en_US_WIN" type="java.util.Locale"/>
	</component>

    <component id="compLocaleNull" factory-method="makeInstance" class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjectionStaticFactory">
