<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">



	<bean id="compInnerList" class="org.osgi.test.cases.blueprint.components.injection.InnerComponentInjection">
       	<argument>
           	<list>
				<bean class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
					<argument value="compInner1"/>
				</bean>
                <bean class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
					<argument value="compInner2"/>
				</bean>
          	 </list>
      	 </argument>
   	</bean>

   	<bean id="compInnerSet" class="org.osgi.test.cases.blueprint.components.injection.InnerComponentInjection">
       	<argument>
            <set>
				<bean class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
					<argument value="compInner3"/>
				</bean>
                <bean class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
					<property name="string" value="compInner4"/>
				</bean>
          	 </set>
      	 </argument>
    </bean>


    <bean id="compInnerMap" class="org.osgi.test.cases.blueprint.components.injection.InnerComponentInjection">
        <argument>
            <map>
                <entry>
                    <key>
                        <bean class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
							<argument value="compInnerEntryKey1"/>
						</bean>
                    </key>
                    <bean class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
						<argument value="compInnerEntryValue1"/>
					</bean>
                </entry>
                <entry>
                    <key>
                        <bean class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
							<property name="string" value="compInnerEntryKey2"/>
						</bean>
                    </key>
                    <bean class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
						<property name="string" value="compInnerEntryValue2"/>
					</bean>
                </entry>
            </map>
        </argument>
    </bean>

</blueprint>