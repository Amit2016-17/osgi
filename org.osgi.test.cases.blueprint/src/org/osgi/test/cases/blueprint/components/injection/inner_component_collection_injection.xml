<?xml version="1.0" encoding="UTF-8"?> 
<components xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://www.osgi.org/xmlns/blueprint/v1.0.0 http://www.osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd"> 
	

	
	<component id="compInnerList" class="org.osgi.test.cases.blueprint.components.injection.InnerComponentInjection"> 
       	<constructor-arg>
           	<list>  
				<component class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
					<constructor-arg value="compInner1"/>
				</component>
                <component class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
					<constructor-arg value="compInner2"/>
				</component>
          	 </list>
      	 </constructor-arg>
   	</component> 
   	
   	<component id="compInnerSet" class="org.osgi.test.cases.blueprint.components.injection.InnerComponentInjection"> 
       	<constructor-arg>
            <set>  
				<component class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
					<constructor-arg value="compInner3"/>
				</component>
                <component class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
					<property name="string" value="compInner4"/>
				</component>
          	 </set>
      	 </constructor-arg>
    </component> 
    
    
    <component id="compInnerMap" class="org.osgi.test.cases.blueprint.components.injection.InnerComponentInjection"> 
        <constructor-arg>
            <map>  
                <entry>
                    <key>
                        <component class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
							<constructor-arg value="compInnerEntryKey1"/>
						</component>
                    </key>
                    <component class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
						<constructor-arg value="compInnerEntryValue1"/>
					</component>
                </entry>
                <entry>
                    <key>
                        <component class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
							<property name="string" value="compInnerEntryKey2"/>
						</component>
                    </key>
                    <component class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
						<property name="string" value="compInnerEntryValue2"/>
					</component>
                </entry>
            </map>
        </constructor-arg>
    </component> 
    
</components>