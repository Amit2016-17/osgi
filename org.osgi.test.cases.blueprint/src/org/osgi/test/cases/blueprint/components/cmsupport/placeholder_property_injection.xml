<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:osgix="http://www.osgi.org/xmlns/blueprint-compendium/v1.0.0"
    xsi:schemaLocation="http://www.osgi.org/xmlns/blueprint/v1.0.0 http://www.osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd
    http://www.osgi.org/xmlns/blueprint-compendium/v1.0.0 http://www.osgi.org/xmlns/blueprint-compendium/v1.0.0/blueprint-compendium.xsd">

    <osgix:property-placeholder persistent-id="org.osgi.test.cases.blueprint.components.cmsupport.config2"/>

<!-- string -->
    <bean id="compString" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
        <property name="string" value="${string}"/>
    </bean>

    <bean id="compStringPart" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
        <property name="string" value="${string}xyz"/>
    </bean>

<!-- string empty -->
    <bean id="compStringEmpty" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
        <property name="string" value="${string_empty}"/>
    </bean>

    <bean id="compStringEmptyPart" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
        <property name="string" value="xyz${string_empty}"/>
    </bean>

<!-- string null -->
    <bean id="compStringNull" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
        <property name="string" value="${string_null}"/>
    </bean>

    <bean id="compStringNullPart" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
        <property name="string" value="xyz${string_null}xyz"/>
    </bean>

<!-- boolean -->
    <bean id="compWrapperBoolean" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
        <property name="boolean" value="${boolean}"/>
    </bean>

<!-- byte -->
    <bean id="compWrapperByte" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
        <property name="byte" value="${byte}"/>
    </bean>

<!-- char -->
    <bean id="compWrapperCharacter" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
        <property name="char" value="${char}"/>
    </bean>

<!-- int-->
    <bean id="compWrapperInteger" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
        <property name="int" value="${int}"/>
    </bean>

<!-- short -->
    <bean id="compWrapperShort" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
        <property name="short" value="${short}"/>
    </bean>

<!-- long -->
    <bean id="compWrapperLong" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
        <property name="long" value="${long}"/>
    </bean>

<!-- double -->
    <bean id="compWrapperDouble" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
        <property name="double" value="${double}"/>
    </bean>

<!-- float -->
    <bean id="compWrapperFloat" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
        <property name="float" value="${float}"/>
    </bean>

</blueprint>
