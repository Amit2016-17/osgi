<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0">

    <description>
        Your basic reference list importer with no validation.
    </description>

    <!-- This gets both a reference collection injected -->
   <bean id="ServiceTwoChecker" class="org.osgi.test.cases.blueprint.components.serviceimport.NullReferenceList">
       <argument value="ServiceTwoChecker"/>
       <property name="list">
           <ref-list interface="org.osgi.test.cases.blueprint.services.TestServiceOne" availability="optional"/>
       </property>
   </bean>
</blueprint>

