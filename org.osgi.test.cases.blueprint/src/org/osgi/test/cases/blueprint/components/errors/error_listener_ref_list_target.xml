<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0">

    <description>
        This uses a listener target that's a service element, which is invalid because it is not a
        target type.
    </description>

    <reference-list id="CollectionTarget" interface="org.osgi.test.cases.blueprint.services.TestServiceTwo" availability="optional"/>

    <!-- single shared instance of the imported service -->
    <reference id="ServiceOne" interface="org.osgi.test.cases.blueprint.services.TestServiceTwo" availability="optional">
        <reference-listener bind-method="bind" unbind-method="unbind" ref="CollectionTarget"/>
    </reference>
</blueprint>

