<?xml version="1.0" encoding="UTF-8"?> 
<components xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://www.osgi.org/xmlns/blueprint/v1.0.0 http://www.osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd"> 

<!-- URL: 3 cases -->
	<component id="compURL" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection"> 
		<property name="url" value="http://www.osgi.org"/>
	</component>  

    <component id="compURLNull" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection"> 
		<property name="url"><null/></property>
	</component>
	
	<component id="compURL_EleValue" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection"> 
		<property name="url"><value>http://www.osgi.org</value></property>
	</component> 
	
<!-- Date: 5 cases -->
	<component id="compDate" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection"> 
		<property name="date" value="6/1/1999"/>
	</component>  

    <component id="compDate2" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection"> 
		<property name="date" value="2000-12-31 23:59:59"/>
	</component> 

    <component id="compDate3" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection"> 
		<property name="date" value="January 1, 1970, 00:00:00 GMT"/>
	</component> 
	
    <component id="compDateNull" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection"> 
		<property name="date" type="java.util.Date"><null/></property>
	</component>
	
	<component id="compDate_EleValue" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection"> 
		<property name="date"><value type="java.util.Date">1970-1-1</value></property>
	</component>
		
<!-- Locale:5 cases -->
	<component id="compLocale" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection"> 
		<property name="locale" value="en"/>
	</component>

	<component id="compLocale2" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection"> 
		<property name="locale" value="zh_CN"/>
	</component>
	
	<component id="compLocale3" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection"> 
		<property name="locale" value="en_US_WIN"/>
	</component>
	
    <component id="compLocaleNull" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection"> 
		<property name="locale"><null/></property>
	</component>

	<component id="compLocale_EleValue" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection"> 
		<property name="locale"><value>en_US</value></property>
	</component>
	
<!-- Class:3 cases -->
	<component id="compClass" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection"> 
		<property name="class" value="java.lang.String"/>
	</component>
	
    <component id="compClassNull" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection"> 
		<property name="class"><null/></property>
	</component>

	<component id="compClass_EleValue" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection"> 
		<property name="class"><value>org.osgi.test.cases.blueprint.components.injection.PropertyInjection</value></property>
	</component>

<!-- Properties: 4 cases -->
	<component id="compProperties" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection"> 
		<property name="properties" value="Name=Rex"/>
	</component>

	<component id="compProperties_EleValue" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection"> 
		<property name="properties">
			<value>
				Name= Rex
				Organization =OSGi Alliance
				Nationality    :    China
				Language:中文, English
				Graduated=Shanghai Jiao Tong /
					University
				#key is the word before the first whitespace - "Major"
				Major Computer Science
					
				!Gender should be a String ""
				Gender
				
			</value>
		</property>
	</component>
	
	<component id="compPropertiesEmpty" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection"> 
		<property name="properties"><value>  </value></property>
	</component>
	
    <component id="compPropertiesNull" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection"> 
		<property name="properties"><null/></property>
	</component>


</components>