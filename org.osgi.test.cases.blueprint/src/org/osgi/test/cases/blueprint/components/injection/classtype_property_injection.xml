<!-- Locale:5 cases -->
	<bean id="compLocale" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
		<property name="locale" value="en"/>
	</bean>

	<bean id="compLocale2" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
		<property name="locale" value="zh_CN"/>
	</bean>

	<bean id="compLocale3" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
		<property name="locale" value="en_US_WIN"/>
	</bean>

    <bean id="compLocaleNull" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
		<property name="locale"><null/></property>
	</bean>

	<bean id="compLocale_EleValue" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
		<property name="locale"><value>en_US</value></property>
	</bean>

<!-- Class:3 cases -->
	<bean id="compClass" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
		<property name="class" value="java.lang.String"/>
	</bean>

    <bean id="compClassNull" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
		<property name="class"><null/></property>
	</bean>

	<bean id="compClass_EleValue" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
		<property name="class"><value>org.osgi.test.cases.blueprint.components.injection.PropertyInjection</value></property>
	</bean>

<!-- Properties: 4 cases -->
	<bean id="compProperties" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
		<property name="properties" value="Name=Rex"/>
	</bean>

	<bean id="compProperties_EleValue" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
		<property name="properties">
			<value>
				Name= Rex
				Organization =OSGi Alliance
				Nationality    :    China
				Language: \u4e2d\u6587, English
				Graduated=Shanghai Jiao Tong University
				#key is the word before the first whitespace - "Major"
				Major Computer Science

				!Gender should be a String ""
				Gender

			</value>
		</property>
	</bean>

	<bean id="compPropertiesEmpty" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
		<property name="properties"><value>  </value></property>
	</bean>

    <bean id="compPropertiesNull" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
		<property name="properties"><null/></property>
	</bean>


</blueprint>