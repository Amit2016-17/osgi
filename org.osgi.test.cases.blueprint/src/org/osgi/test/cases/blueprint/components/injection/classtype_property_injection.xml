<!-- URL: 3 cases -->
	<bean id="compURL" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
		<property name="url" value="http://www.osgi.org"/>
	</bean>

    <bean id="compURLNull" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
		<property name="url"><null/></property>
	</bean>

	<bean id="compURL_EleValue" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
		<property name="url"><value>http://www.osgi.org</value></property>
	</bean>

