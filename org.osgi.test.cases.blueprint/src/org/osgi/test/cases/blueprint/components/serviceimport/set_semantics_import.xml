<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0">

    <description>
        This is a checker for different set vs. list semantics
    </description>

    <!-- This gets both a ServiceManager and a reference collection injected -->
   <bean id="SetChecker" class="org.osgi.test.cases.blueprint.components.serviceimport.SetSemanticsChecker" init-method="init">
       <argument value="SetChecker"/>
       <!-- Not actually used in this test, but having it here enforces initialization order -->
       <property name="serviceManager">
           <reference interface="org.osgi.test.cases.blueprint.services.ServiceManager"/>
       </property>
       <property name="set">
           <ref-set interface="org.osgi.test.cases.blueprint.services.TestServiceOne"/>
       </property>
       <property name="list">
           <ref-list interface="org.osgi.test.cases.blueprint.services.TestServiceOne"/>
       </property>
   </bean>
</blueprint>

