	<!-- Primitive short array -->
	<component id="compPrimShortArray"
		class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
		<constructor-arg>
			<array value-type="short">
				<value>3</value>
				<value>4</value>
			</array>
		</constructor-arg>
	</component>

	<!-- Wrapped short array -->
	<component id="compWrappedShortArray"
		class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
		<constructor-arg>
			<array value-type="java.lang.Short">
				<value>3</value>
				<value>4</value>
			</array>
		</constructor-arg>
	</component>

	<!-- Primitive long array -->
	<component id="compPrimLongArray"
		class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
		<constructor-arg>
			<array value-type="long">
				<value>3</value>
				<value>4</value>
			</array>
		</constructor-arg>
	</component>

	<!-- Wrapped long array -->
	<component id="compWrappedLongArray"
		class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
		<constructor-arg>
			<array value-type="java.lang.Long">
				<value>3</value>
				<value>4</value>
			</array>
		</constructor-arg>
	</component>

	<!-- Primitive double array -->
	<component id="compPrimDoubleArray"
		class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
		<constructor-arg>
			<array value-type="double">
				<value>3</value>
				<value>4</value>
			</array>
		</constructor-arg>
	</component>

	<!-- Wrapped double array -->
	<component id="compWrappedDoubleArray"
		class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
		<constructor-arg>
			<array value-type="java.lang.Double">
				<value>3</value>
				<value>4</value>
			</array>
		</constructor-arg>
	</component>

	<!-- Primitive float array -->
	<component id="compPrimFloatArray"
		class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
		<constructor-arg>
			<array value-type="float">
				<value>3</value>
				<value>4</value>
			</array>
		</constructor-arg>
	</component>

	<!-- Wrapped float array -->
	<component id="compWrappedFloatArray"
		class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
		<constructor-arg>
			<array value-type="java.lang.Float">
				<value>3</value>
				<value>4</value>
			</array>
		</constructor-arg>
	</component>

	<!-- Date array -->
	<component id="compDateArray"
		class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
		<constructor-arg>
			<array value-type="java.util.Date">
				<value>2009-2-19</value>
				<value>2009-2-20</value>
			</array>
		</constructor-arg>
	</component>

	<!-- URL array -->
	<component id="compURLArray"
		class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
		<constructor-arg>
			<array value-type="java.net.URL">
				<value>http://www.osgi.org</value>
				<value>http://www.gmail.com</value>
			</array>
		</constructor-arg>
	</component>

	<!-- Class array -->
	<component id="compClassArray"
		class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
		<constructor-arg>
			<array value-type="java.lang.Class">
				<value>java.lang.String</value>
				<value>java.lang.Boolean</value>
			</array>
		</constructor-arg>
	</component>

	<!-- Locale array -->
	<component id="compLocaleArray"
		class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
		<constructor-arg>
			<array value-type="java.lang.Locale">
				<value>en_US</value>
				<value>zh_CN</value>
			</array>
		</constructor-arg>
	</component>

	<!-- String values from props -->
	<component id="compNestedProps"
		class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
		<constructor-arg>
			<array value-type="java.util.Properties">
				<props>
					<prop key="administrator">administrator@example.org
					</prop>
					<prop key="support">support@example.org</prop>
				</props>
			</array>
		</constructor-arg>
	</component>

	<!-- String values from map -->
	<component id="compNestedMap"
		class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
		<constructor-arg>
			<array value-type="java.util.Map">
				<map>
					<entry key="abc" value="1" />
					<entry key="def" value="2" />
				</map>
			</array>
		</constructor-arg>
	</component>

	<!-- String values from set -->
	<component id="compNestedSet"
		class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
		<constructor-arg>
			<array value-type="java.util.Set">
				<set>
					<value>administrator@example.org</value>
					<value>support@example.org</value>
				</set>
			</array>
		</constructor-arg>
	</component>

	<!-- String values from list -->
	<component id="compNestedList"
		class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
		<constructor-arg>
			<array value-type="java.util.List">
				<list>
					<value>administrator@example.org</value>
					<value>support@example.org</value>
				</list>
			</array>
		</constructor-arg>
	</component>

	<!-- Injection of nested array -->
	<component id="compNestedArray" factory-method="makeInstance"
		class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
		<constructor-arg>
			<array value-type="java.lang.Object">
				<array value-type="java.lang.String">
					<value>abc</value>
					<value>def</value>
				</array>
			</array>
		</constructor-arg>
	</component>

