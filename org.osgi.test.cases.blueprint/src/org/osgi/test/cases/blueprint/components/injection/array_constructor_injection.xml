<?xml version="1.0" encoding="UTF-8"?>
<components xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.osgi.org/xmlns/blueprint/v1.0.0 http://www.osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd">

	<!-- Empty Array -->
	<component id="compEmptyArray"
		class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
		<constructor-arg>
			<array value-type="java.lang.Object"></array>
		</constructor-arg>
	</component>

	<!-- Null Array -->
	<component id="compNullArray"
		class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
		<constructor-arg>
			<array value-type="java.lang.Object">
				<null />
			</array>
		</constructor-arg>
	</component>

	<!-- Single Item Array -->
	<component id="compSinglePrimIntItem"
		class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
		<constructor-arg>
			<array value-type="int">
				<value>1</value>
			</array>
		</constructor-arg>
	</component>

	<!-- Primitive int array -->
	<component id="compPrimIntArray"
		class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
		<constructor-arg>
			<array value-type="int">
				<value>1</value>
				<value>2</value>
			</array>
		</constructor-arg>
	</component>

	<!-- Wrapped int array -->
	<component id="compWrappedIntArray"
		class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
		<constructor-arg>
			<array value-type="java.lang.Integer">
				<value>1</value>
				<value>2</value>
			</array>
		</constructor-arg>
	</component>

	<!-- Primitive boolean array -->
	<component id="compPrimBoolArray"
		class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
		<constructor-arg>
			<array value-type="boolean">
				<value>true</value>
				<value>false</value>
			</array>
		</constructor-arg>
	</component>

	<!-- Wrapped boolean array -->
	<component id="compWrappedBoolArray"
		class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
		<constructor-arg>
			<array value-type="java.lang.Boolean">
				<value>true</value>
				<value>false</value>
			</array>
		</constructor-arg>
	</component>

	<!-- String array -->
	<component id="compStringArray"
		class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
		<constructor-arg>
			<array value-type="String">
				<value>abc</value>
				<value>def</value>
			</array>
		</constructor-arg>
	</component>

	<!-- Primitive byte array -->
	<component id="compPrimByteArray"
		class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
		<constructor-arg>
			<array value-type="byte">
				<value>1</value>
				<value>2</value>
			</array>
		</constructor-arg>
	</component>

	<!-- Wrapped byte array -->
	<component id="compWrappedByteArray"
		class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
		<constructor-arg>
			<array value-type="java.lang.Byte">
				<value>1</value>
				<value>2</value>
			</array>
		</constructor-arg>
	</component>

	<!-- Primitive char array -->
	<component id="compCharArray"
		class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
		<constructor-arg>
			<array value-type="char">
				<value>a</value>
				<value>b</value>
			</array>
		</constructor-arg>
	</component>

	<!-- Wrapped char array -->
	<component id="compWrappedCharArray"
