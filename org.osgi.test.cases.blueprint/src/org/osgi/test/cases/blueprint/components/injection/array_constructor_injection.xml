<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0">

	<!-- Empty Array -->
	<bean id="compEmptyArray"
		class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
		<argument>
			<array value-type="java.lang.String"></array>
		</argument>
	</bean>

	<!-- Null Array -->
	<bean id="compNullArray"
		class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
		<argument type="java.lang.String[]">
			<array value-type="java.lang.String">
				<null />
			</array>
		</argument>
	</bean>

	<!-- Single Item Array -->
	<bean id="compSinglePrimIntItem"
		class="org.osgi.test.cases.blueprint.components.injection.PrimitiveArrayConstructorInjection">
		<argument type="int[]">
			<array value-type="int">
				<value>1</value>
			</array>
		</argument>
	</bean>

	<!-- Primitive int array -->
	<bean id="compPrimIntArray"
		class="org.osgi.test.cases.blueprint.components.injection.PrimitiveArrayConstructorInjection">
		<argument>
			<array value-type="int">
				<value>1</value>
				<value>2</value>
			</array>
		</argument>
	</bean>

	<!-- Primitive int array -->
	<bean id="compImplicitPrimIntArray"
		class="org.osgi.test.cases.blueprint.components.injection.PrimitiveArrayConstructorInjection">
		<argument type="int[]">
			<array>
				<value>1</value>
				<value>2</value>
			</array>
		</argument>
	</bean>

	<!-- Wrapped int array -->
	<bean id="compWrappedIntArray"
		class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
		<argument type="java.lang.Integer[]">
			<array value-type="java.lang.Integer">
				<value>1</value>
				<value>2</value>
			</array>
		</argument>
	</bean>

	<!-- Primitive boolean array -->
	<bean id="compPrimBoolArray"
		class="org.osgi.test.cases.blueprint.components.injection.PrimitiveArrayConstructorInjection">
		<argument>
			<array value-type="boolean">
				<value>true</value>
				<value>false</value>
			</array>
		</argument>
	</bean>

	<!-- Wrapped boolean array -->
	<bean id="compWrappedBoolArray"
		class="org.osgi.test.cases.blueprint.components.injection.WrapperArrayConstructorInjection">
		<argument>
			<array value-type="java.lang.Boolean">
				<value>true</value>
				<value>false</value>
			</array>
		</argument>
	</bean>

	<!-- String array -->
	<bean id="compStringArray"
		class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
		<argument>
			<array value-type="java.lang.String">
				<value>abc</value>
				<value>def</value>
			</array>
		</argument>
	</bean>

	<!-- Primitive byte array -->
	<bean id="compPrimByteArray"
		class="org.osgi.test.cases.blueprint.components.injection.PrimitiveArrayConstructorInjection">
		<argument>
			<array value-type="byte">
				<value>1</value>
				<value>2</value>
			</array>
		</argument>
	</bean>

	<!-- Wrapped byte array -->
	<bean id="compWrappedByteArray"
		class="org.osgi.test.cases.blueprint.components.injection.WrapperArrayConstructorInjection">
		<argument>
			<array value-type="java.lang.Byte">
				<value>1</value>
				<value>2</value>
			</array>
		</argument>
	</bean>

	<!-- Primitive char array -->
	<bean id="compCharArray"
		class="org.osgi.test.cases.blueprint.components.injection.PrimitiveArrayConstructorInjection">
		<argument>
			<array value-type="char">
				<value>a</value>
				<value>b</value>
			</array>
		</argument>
	</bean>

	<!-- Wrapped char array -->
	<bean id="compWrappedCharArray"
		class="org.osgi.test.cases.blueprint.components.injection.WrapperArrayConstructorInjection">
		<argument>
			<array value-type="java.lang.Character">
				<value>a</value>
				<value>b</value>
			</array>
		</argument>
	</bean>

	<!-- Primitive short array -->
	<bean id="compPrimShortArray"
		class="org.osgi.test.cases.blueprint.components.injection.PrimitiveArrayConstructorInjection">
		<argument>
			<array value-type="short">
				<value>3</value>
				<value>4</value>
			</array>
		</argument>
	</bean>

	<!-- Wrapped short array -->
	<bean id="compWrappedShortArray"
		class="org.osgi.test.cases.blueprint.components.injection.WrapperArrayConstructorInjection">
		<argument>
			<array value-type="java.lang.Short">
				<value>3</value>
				<value>4</value>
			</array>
		</argument>
	</bean>

	<!-- Primitive long array -->
	<bean id="compPrimLongArray"
		class="org.osgi.test.cases.blueprint.components.injection.PrimitiveArrayConstructorInjection">
		<argument>
			<array value-type="long">
				<value>3</value>
				<value>4</value>
			</array>
		</argument>
	</bean>

	<!-- Wrapped long array -->
	<bean id="compWrappedLongArray"
		class="org.osgi.test.cases.blueprint.components.injection.WrapperArrayConstructorInjection">
		<argument>
			<array value-type="java.lang.Long">
				<value>3</value>
				<value>4</value>
			</array>
		</argument>
	</bean>

	<!-- Primitive double array -->
	<bean id="compPrimDoubleArray"
		class="org.osgi.test.cases.blueprint.components.injection.PrimitiveArrayConstructorInjection">
		<argument>
			<array value-type="double">
				<value>3</value>
				<value>4</value>
			</array>
		</argument>
	</bean>

	<!-- Wrapped double array -->
	<bean id="compWrappedDoubleArray"
		class="org.osgi.test.cases.blueprint.components.injection.WrapperArrayConstructorInjection">
		<argument>
			<array value-type="java.lang.Double">
				<value>3</value>
				<value>4</value>
			</array>
		</argument>
	</bean>

	<!-- Primitive float array -->
	<bean id="compPrimFloatArray"
		class="org.osgi.test.cases.blueprint.components.injection.PrimitiveArrayConstructorInjection">
		<argument>
			<array value-type="float">
				<value>3</value>
				<value>4</value>
			</array>
		</argument>
	</bean>

	<!-- Wrapped float array -->
	<bean id="compWrappedFloatArray"
		class="org.osgi.test.cases.blueprint.components.injection.WrapperArrayConstructorInjection">
		<argument>
			<array value-type="java.lang.Float">
				<value>3</value>
				<value>4</value>
			</array>
		</argument>
	</bean>

	<!-- Date array -->
	<bean id="compDateArray"
		class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
		<argument>
			<array value-type="java.util.Date">
				<value>19 Feb 2009</value>
				<value>20 Feb 2009</value>
			</array>
		</argument>
	</bean>

	<!-- URL array -->
	<bean id="compURLArray"
		class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
		<argument>
			<array value-type="java.net.URL">
				<value>http://www.osgi.org</value>
				<value>http://www.gmail.com</value>
			</array>
		</argument>
	</bean>

	<!-- Class array -->
	<bean id="compClassArray"
		class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
		<argument>
			<array value-type="java.lang.Class">
				<value>java.lang.String</value>
				<value>java.lang.Boolean</value>
			</array>
		</argument>
	</bean>

	<!-- Locale array -->
	<bean id="compLocaleArray"
		class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
		<argument>
			<array value-type="java.util.Locale">
				<value>en_US</value>
				<value>zh_CN</value>
			</array>
		</argument>
	</bean>

	<!-- String values from props -->
	<bean id="compNestedProps"
		class="org.osgi.test.cases.blueprint.components.injection.PropsConstructorInjection">
		<argument>
			<array value-type="java.util.Properties">
				<props>
					<prop key="administrator">administrator@example.org
					</prop>
					<prop key="support">support@example.org</prop>
				</props>
			</array>
		</argument>
	</bean>

	<!-- String values from map -->
	<bean id="compNestedMap"
		class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
		<argument>
			<array value-type="java.util.Map">
				<map>
					<entry key="abc" value="1" />
					<entry key="def" value="2" />
				</map>
			</array>
		</argument>
	</bean>

	<!-- String values from set -->
	<bean id="compNestedSet"
		class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
		<argument>
			<array value-type="java.util.Set">
				<set>
					<value>administrator@example.org</value>
					<value>support@example.org</value>
				</set>
			</array>
		</argument>
	</bean>

	<!-- String values from list -->
	<bean id="compNestedList"
		class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
		<argument>
			<array value-type="java.util.List">
				<list>
					<value>administrator@example.org</value>
					<value>support@example.org</value>
				</list>
			</array>
		</argument>
	</bean>

	<!-- Injection of nested array -->
	<bean id="compNestedArray"
		class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
		<argument type="java.lang.String[][]">
			<array value-type="java.lang.String[]">
				<array value-type="java.lang.String">
					<value>abc</value>
					<value>def</value>
				</array>
			</array>
		</argument>
	</bean>

</blueprint>
