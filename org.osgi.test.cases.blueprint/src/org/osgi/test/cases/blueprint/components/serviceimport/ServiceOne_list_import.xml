<?xml version="1.0" encoding="UTF-8"?>
<components xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.osgi.org/xmlns/blueprint/v1.0.0 http://www.osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd">

    <description>
        Your basic reference list importer with no validation.
    </description>

    <!-- This gets both a reference collection injected -->
   <component id="ServiceOneChecker" class="org.osgi.test.cases.blueprint.components.serviceimport.NullReferenceList">
       <constructor-arg value="ServiceOneChecker"/>
       <property name="list">
           <ref-list interface="org.osgi.test.cases.blueprint.services.TestServiceOne" availability="optional"/>
       </property>
   </component>
</components>

