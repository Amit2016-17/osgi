<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0">

    <description>
        Your basic reference list importer with no validation.
    </description>

    <!-- This gets both a reference collection injected -->
   <bean id="ServiceOneChecker" class="org.osgi.test.cases.blueprint.components.serviceimport.NullReferenceList">
       <argument value="ServiceOneChecker"/>
       <property name="list">
           <reference-list interface="org.osgi.test.cases.blueprint.services.TestServiceOne" component-name="ServiceOneCircular" availability="optional"/>
       </property>
   </bean>
</blueprint>

