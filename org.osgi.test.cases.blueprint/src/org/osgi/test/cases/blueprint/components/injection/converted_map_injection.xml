<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0">

	<type-converters>
		<bean class="org.osgi.test.cases.blueprint.beans.injection.HashMapConverter"/>
		<bean class="org.osgi.test.cases.blueprint.beans.injection.HashtableConverter"/>
		<bean class="org.osgi.test.cases.blueprint.beans.injection.TreeMapConverter"/>
	</type-converters>

    <bean id="hashMap" class="org.osgi.test.cases.blueprint.beans.injection.PropertyInjection">
        <property name="hashMap">
            <map>
                <entry key="abc" value="1"/>
                <entry key="def" value="2"/>
            </map>
        </property>
    </bean>

    <bean id="treeMap" class="org.osgi.test.cases.blueprint.beans.injection.PropertyInjection">
        <property name="treeMap">
            <map>
                <entry key="abc" value="1"/>
                <entry key="def" value="2"/>
            </map>
        </property>
    </bean>

    <bean id="hashtable" class="org.osgi.test.cases.blueprint.beans.injection.PropertyInjection">
        <property name="hashtable">
            <map>
                <entry key="abc" value="1"/>
                <entry key="def" value="2"/>
            </map>
        </property>
    </bean>

</blueprint>

