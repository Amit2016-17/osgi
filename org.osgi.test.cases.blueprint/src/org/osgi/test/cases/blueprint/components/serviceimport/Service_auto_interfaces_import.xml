<?xml version="1.0" encoding="UTF-8"?> 
<components xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://www.osgi.org/xmlns/blueprint/v1.0.0 http://www.osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd">
    
    <description>
        This is a reusable config for validating auto-export of interfaces.        
    </description>

   <component id="ServiceInterfaces" class="org.osgi.test.cases.blueprint.components.serviceimport.ServiceReferenceChecker"> 
       <constructor-arg value="ServiceInterfaces"/>
       <property name="one">
           <reference interface="org.osgi.test.cases.blueprint.services.TestServiceOne"/>
       </property>
       <property name="two">
           <reference interface="org.osgi.test.cases.blueprint.services.TestServiceTwo"/>
       </property>
       <property name="allSubclass">
           <reference interface="org.osgi.test.cases.blueprint.services.TestServiceAllSubclass"/>
       </property>
   </component> 
</components> 
