<?xml version="1.0" encoding="UTF-8"?> 
<components xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://www.osgi.org/xmlns/blueprint/v1.0.0 http://www.osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd"> 

	<type-converters>
		<component id="regionCodeConverter" class="org.osgi.test.cases.blueprint.components.injection.RegionCodeConverter"/>
		<component id="asianRegionCodeConverter" class="org.osgi.test.cases.blueprint.components.injection.AsianRegionCodeConverter"/>
	</type-converters>

	<component id="comp_cnst" class="org.osgi.test.cases.blueprint.components.injection.RegionCodeInjection"> 
		<constructor-arg value="comp_cnst"/>
		<constructor-arg type="org.osgi.test.cases.blueprint.components.injection.RegionCode" value="CN+86"/>
	</component>
    
    <component id="compAsia_cnst" class="org.osgi.test.cases.blueprint.components.injection.RegionCodeInjection"> 
		<constructor-arg value="compAsia_cnst"/>
		<constructor-arg type="org.osgi.test.cases.blueprint.components.injection.AsianRegionCode" value="CN+86"/>
	</component>
	
</components>