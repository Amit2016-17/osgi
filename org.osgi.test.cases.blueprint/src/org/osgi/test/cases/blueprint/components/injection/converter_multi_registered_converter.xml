<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0">

	<type-converters>
		<bean id="regionCodeConverter" class="org.osgi.test.cases.blueprint.components.injection.RegionCodeConverter"/>
		<bean id="asianRegionCodeConverter" class="org.osgi.test.cases.blueprint.components.injection.AsianRegionCodeConverter">
    		<argument value="asianRegionCodeConverter"/>
    	</bean>
	</type-converters>

	<bean id="comp_cnst" class="org.osgi.test.cases.blueprint.components.injection.RegionCodeInjection">
		<argument value="comp_cnst"/>
		<argument type="org.osgi.test.cases.blueprint.components.injection.RegionCode" value="CN+86"/>
	</bean>

    <bean id="compAsia_cnst" class="org.osgi.test.cases.blueprint.components.injection.RegionCodeInjection">
		<argument value="compAsia_cnst"/>
 		<argument type="org.osgi.test.cases.blueprint.components.injection.RegionCode">
 		    <value type="org.osgi.test.cases.blueprint.components.injection.AsianRegionCode">CN+86</value>
 		</argument>
	</bean>

</blueprint>