<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.osgi.org/xmlns/blueprint/v1.0.0 http://www.osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd">

	<type-converters>
		<bean id="asianRegionCodeConverter" class="org.osgi.test.cases.blueprint.components.injection.AsianRegionCodeConverter"/>
	</type-converters>

<!-- Constructor -->
	<bean id="comp_cnst" class="org.osgi.test.cases.blueprint.components.injection.RegionCodeInjection">
		<argument value="comp_cnst"/>
		<argument type="org.osgi.test.cases.blueprint.components.injection.RegionCode" value="CN+86"/>
	</bean>

<!-- Property -->
	<bean id="comp_prpt" class="org.osgi.test.cases.blueprint.components.injection.RegionCodeInjection">
		<argument value="comp_prpt"/>
		<property name="regionCode">
            <value type="org.osgi.test.cases.blueprint.components.injection.RegionCode">CN+86</value>
        </property>
	</bean>

</blueprint>