<?xml version="1.0" encoding="UTF-8"?> 
<components xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://www.osgi.org/xmlns/blueprint/v1.0.0 http://www.osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd"> 

	<type-converters>
		<component id="asianRegionCodeConverter" class="org.osgi.test.cases.blueprint.components.injection.AsianRegionCodeConverter"/>
	</type-converters>

<!-- Constructor -->
	<component id="comp_cnst" class="org.osgi.test.cases.blueprint.components.injection.RegionCodeInjection"> 
		<constructor-arg value="comp_cnst"/>
		<constructor-arg type="org.osgi.test.cases.blueprint.components.injection.RegionCode" value="CN+86"/>
	</component>
	
<!-- Property -->	
	<component id="comp_prpt" class="org.osgi.test.cases.blueprint.components.injection.RegionCodeInjection"> 
		<constructor-arg value="comp_prpt"/>
		<property name="regionCode" type="org.osgi.test.cases.blueprint.components.injection.RegionCode" value="CN+86"/>
	</component>
	
</components> 