<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
    default-lazy-init="false">

    <bean id="compTypedKeys" class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
        <argument>
            <map key-type="java.lang.Boolean">
                <entry key="true" value="true"/>
                <entry value="false">
                    <key><value>false</value></key>
                </entry>
                <entry value="double">
                    <key><value type="java.lang.Double">1.0</value></key>
                    <value>double</value>
                </entry>
            </map>
        </argument>
    </bean>
</blueprint>

