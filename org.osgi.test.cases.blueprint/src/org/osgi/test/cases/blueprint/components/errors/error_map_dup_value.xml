<?xml version="1.0" encoding="UTF-8"?>
<components xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.osgi.org/xmlns/blueprint/v1.0.0 http://www.osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd"
      default-init-method="init" default-destroy-method="destroy" default-lazy-init="false">

    <component id="compTypedKeys" class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
        <constructor-arg>
            <map key-type="java.lang.Boolean">
                <entry key="true" value="true"/>
                <entry value="false">
                    <key><value>false</value></key>
                </entry>
                <entry value="double">
                    <key><value type="java.lang.Double">1.0</value></key>
                    <value>double</value>
                </entry>
            </map>
        </constructor-arg>
    </component>
</components>

