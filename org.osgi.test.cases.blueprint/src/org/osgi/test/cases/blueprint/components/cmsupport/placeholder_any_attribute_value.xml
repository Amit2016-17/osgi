<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
    xmlns:bpcm="http://www.osgi.org/xmlns/blueprint-cm/v1.0.0">

    <bpcm:property-placeholder persistent-id="org.osgi.test.cases.blueprint.components.cmsupport.config2"/>


    <bean id="compConstructorArgValue" class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
        <argument value="${string}" type="java.lang.String"/>
    </bean>

    <bean id="compPropertyValue" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
        <property name="string" value="${string}"/>
    </bean>

    <bean id="compMapEntryValue" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
        <property name="map">
            <entry key="string" value="${string}"/>
        </property>
    </bean>

</blueprint>
