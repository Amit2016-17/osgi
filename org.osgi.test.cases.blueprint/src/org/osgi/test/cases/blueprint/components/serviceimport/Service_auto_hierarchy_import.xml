<?xml version="1.0" encoding="UTF-8"?>
<components xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.osgi.org/xmlns/blueprint/v1.0.0 http://www.osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd">

    <description>
        This is a reusable config for validating auto-export of concrete classes.
    </description>

   <component id="ServiceHierarchy" class="org.osgi.test.cases.blueprint.components.serviceimport.ServiceReferenceChecker">
       <constructor-arg value="ServiceHierarchy"/>
       <property name="good">
           <reference interface="org.osgi.test.cases.blueprint.services.TestGoodService"/>
       </property>
       <property name="goodSubclass">
           <reference interface="org.osgi.test.cases.blueprint.services.TestGoodServiceSubclass"/>
       </property>
   </component>
</components>

