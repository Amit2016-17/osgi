<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0">

    <description>
        This is a reusable config for validating auto-export of concrete classes.
    </description>

   <bean id="ServiceHierarchy" class="org.osgi.test.cases.blueprint.components.serviceimport.ServiceReferenceChecker">
       <argument value="ServiceHierarchy"/>
       <property name="good">
           <reference interface="org.osgi.test.cases.blueprint.services.TestGoodService"/>
       </property>
       <property name="goodSubclass">
           <reference interface="org.osgi.test.cases.blueprint.services.TestGoodServiceSubclass"/>
       </property>
   </bean>
</blueprint>

