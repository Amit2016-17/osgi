<?xml version="1.0" encoding="UTF-8"?> 
<components xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://www.osgi.org/xmlns/blueprint/v1.0.0 http://www.osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd"> 

	<type-converters>
		<component id="asianRegionCodeConverter" class="org.osgi.test.cases.blueprint.components.injection.AsianRegionCodeConverter"/>
	</type-converters>

<!-- Constructor -->
    <component id="compAsia_cnst" class="org.osgi.test.cases.blueprint.components.injection.RegionCodeInjection"> 
		<constructor-arg value="compAsia_cnst"/>
		<constructor-arg><value type="org.osgi.test.cases.blueprint.components.injection.AsianRegionCode">CN+86</value></constructor-arg>
	</component> 
	
	<component id="compImplicit_cnst" class="org.osgi.test.cases.blueprint.components.injection.RegionCodeInjection"> 
		<constructor-arg value="compImplicit_cnst"/>
		<constructor-arg value="CN+86"/>
	</component>
	
<!-- Property -->
    <component id="compAsia_prpt" class="org.osgi.test.cases.blueprint.components.injection.RegionCodeInjection"> 
		<constructor-arg value="compAsia_prpt"/>
		<property name="regionCode"><value type="org.osgi.test.cases.blueprint.components.injection.AsianRegionCode">CN+86</value></property>
	</component> 
    
    <component id="compImplicit_prpt" class="org.osgi.test.cases.blueprint.components.injection.RegionCodeInjection"> 
		<constructor-arg value="compImplicit_prpt"/>
		<property name="regionCode" value="CN+86"/>
	</component>
    
</components>