<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

	<type-converters>
		<bean id="asianRegionCodeConverter" class="org.osgi.test.cases.blueprint.components.injection.AsianRegionCodeConverter"/>
	</type-converters>

<!-- Constructor -->
    <bean id="compAsia_cnst" class="org.osgi.test.cases.blueprint.components.injection.RegionCodeInjection">
		<argument value="compAsia_cnst"/>
		<argument><value type="org.osgi.test.cases.blueprint.components.injection.AsianRegionCode">CN+86</value></argument>
	</bean>

	<bean id="compImplicit_cnst" class="org.osgi.test.cases.blueprint.components.injection.RegionCodeInjection">
		<argument value="compImplicit_cnst"/>
		<argument value="CN+86"/>
	</bean>

<!-- Property -->
    <bean id="compAsia_prpt" class="org.osgi.test.cases.blueprint.components.injection.RegionCodeInjection">
		<argument value="compAsia_prpt"/>
		<property name="regionCode"><value type="org.osgi.test.cases.blueprint.components.injection.AsianRegionCode">CN+86</value></property>
	</bean>

    <bean id="compImplicit_prpt" class="org.osgi.test.cases.blueprint.components.injection.RegionCodeInjection">
		<argument value="compImplicit_prpt"/>
		<property name="regionCode" value="CN+86"/>
	</bean>

</blueprint>