<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <bean id="compEmptyProps" class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
        <constructor-arg>
            <props></props>
        </argument>
    </bean>

    <bean id="compNullProps" class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
        <argument type="java.util.Properties">
            <null/>
        </argument>
    </bean>

    <bean id="compStringItems" class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
        <constructor-arg>
            <props>
                <prop key="administrator">administrator@example.org</prop>
				<prop key="support">support@example.org</prop>
				<prop key="development">development@example.org</prop>
            </props>
        </argument>
    </bean>

</blueprint>

