<?xml version="1.0" encoding="UTF-8"?>
<components xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.osgi.org/xmlns/blueprint/v1.0.0 http://www.osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd">

	<type-converters>
		<component class="org.osgi.test.cases.blueprint.components.injection.HashSetConverter"/>
		<component class="org.osgi.test.cases.blueprint.components.injection.LinkedHashSetConverter"/>
		<component class="org.osgi.test.cases.blueprint.components.injection.TreeSetConverter"/>
	</type-converters>

    <component id="hashSet" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
        <property name="hashSet">
            <set>
                <value>abc</value>
                <value>def</value>
                <value>def</value>
            </set>
        </property>
    </component>

    <component id="linkedHashSet" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
        <property name="linkedHashSet">
            <set>
                <value>abc</value>
                <value>def</value>
                <value>def</value>
            </set>
        </property>
    </component>

    <component id="treeSet" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
        <property name="treeSet">
            <set>
                <value>abc</value>
                <value>def</value>
                <value>def</value>
            </set>
        </property>
    </component>

    <component id="hashSetFromList" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
        <property name="hashSet">
            <list>
                <value>abc</value>
                <value>def</value>
                <value>def</value>
            </list>
        </property>
    </component>

    <component id="linkedHashSetFromList" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
        <property name="linkedHashSet">
            <list>
                <value>abc</value>
                <value>def</value>
                <value>def</value>
            </list>
        </property>
    </component>

    <component id="treeSetFromList" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
        <property name="treeSet">
            <list>
                <value>abc</value>
                <value>def</value>
                <value>def</value>
            </list>
        </property>
    </component>

    <component id="implicitSetFromList" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
        <property name="set">
            <list>
                <value>abc</value>
                <value>def</value>
                <value>def</value>
            </list>
        </property>
    </component>

</components>


