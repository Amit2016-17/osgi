<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0">

	<type-converters>
		<bean class="org.osgi.test.cases.blueprint.components.injection.HashSetConverter"/>
		<bean class="org.osgi.test.cases.blueprint.components.injection.LinkedHashSetConverter"/>
		<bean class="org.osgi.test.cases.blueprint.components.injection.TreeSetConverter"/>
	</type-converters>

    <bean id="hashSet" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
        <property name="hashSet">
            <set>
                <value>abc</value>
                <value>def</value>
                <value>def</value>
            </set>
        </property>
    </bean>

    <bean id="linkedHashSet" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
        <property name="linkedHashSet">
            <set>
                <value>abc</value>
                <value>def</value>
                <value>def</value>
            </set>
        </property>
    </bean>

    <bean id="treeSet" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
        <property name="treeSet">
            <set>
                <value>abc</value>
                <value>def</value>
                <value>def</value>
            </set>
        </property>
    </bean>

    <bean id="hashSetFromList" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
        <property name="hashSet">
            <list>
                <value>abc</value>
                <value>def</value>
                <value>def</value>
            </list>
        </property>
    </bean>

    <bean id="linkedHashSetFromList" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
        <property name="linkedHashSet">
            <list>
                <value>abc</value>
                <value>def</value>
                <value>def</value>
            </list>
        </property>
    </bean>

    <bean id="treeSetFromList" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
        <property name="treeSet">
            <list>
                <value>abc</value>
                <value>def</value>
                <value>def</value>
            </list>
        </property>
    </bean>

    <bean id="implicitSetFromList" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
        <property name="set">
            <list>
                <value>abc</value>
                <value>def</value>
                <value>def</value>
            </list>
        </property>
    </bean>

</blueprint>


