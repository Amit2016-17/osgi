<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

   <bean id="ContextAware" class="org.osgi.test.cases.blueprint.components.comp1.BlueprintContextAwareComponent" init-method="init">
       <argument value="ContextAware"/>
