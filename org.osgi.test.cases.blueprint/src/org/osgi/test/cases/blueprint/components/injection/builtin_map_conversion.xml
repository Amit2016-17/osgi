<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0">
    <bean id="mapToHashMap" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
        <property name="hashMap">
            <map>
                <entry key="abc" value="1"/>
                <entry key="def" value="2"/>
            </map>
        </property>
    </bean>

    <bean id="mapToTreeMap" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
        <property name="treeMap">
            <map>
                <entry key="abc" value="1"/>
                <entry key="def" value="2"/>
            </map>
        </property>
    </bean>

    <bean id="mapToHashtable" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
        <property name="hashtable">
            <map>
                <entry key="abc" value="1"/>
                <entry key="def" value="2"/>
            </map>
        </property>
    </bean>

    <bean id="mapToDictionary" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
        <property name="dictionary">
            <map>
                <entry key="abc" value="1"/>
                <entry key="def" value="2"/>
            </map>
        </property>
    </bean>

    <bean id="mapToSortedMap" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
        <property name="sortedMap">
            <map>
                <entry key="abc" value="1"/>
                <entry key="def" value="2"/>
            </map>
        </property>
    </bean>

    <!-- ConcurrentMap does not exist in JDK 1.4
    <bean id="mapToConcurrentMap" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
        <property name="concurrentMap">
            <map>
                <entry key="abc" value="1"/>
                <entry key="def" value="2"/>
            </map>
        </property>
    </bean>
    -->

</blueprint>

