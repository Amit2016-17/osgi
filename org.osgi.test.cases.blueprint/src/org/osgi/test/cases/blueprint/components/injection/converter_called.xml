<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0">

	<type-converters>
		<bean id="europeanRegionCodeConverter" class="org.osgi.test.cases.blueprint.components.injection.EuropeanRegionCodeConverter">
			<argument value="europeanRegionCodeConverter"/>
		</bean>
	</type-converters>

<!--
Since the EuropeanRegionCode class has a String constructor,
should test if the instance was created via the type converter but not the String constructor of EuropeanRegionCode.
-->

    <bean id="compEuropean_cnst" class="org.osgi.test.cases.blueprint.components.injection.RegionCodeInjection">
		<argument value="compEuropean_cnst"/>
		<argument type="org.osgi.test.cases.blueprint.components.injection.EuropeanRegionCode" value="UK+32"/>
	</bean>


</blueprint>