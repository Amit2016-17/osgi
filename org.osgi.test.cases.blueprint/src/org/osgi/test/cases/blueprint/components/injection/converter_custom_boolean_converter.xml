<?xml version="1.0" encoding="UTF-8"?>
<components xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.osgi.org/xmlns/blueprint/v1.0.0 http://www.osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd">

	<type-converters>
        <ref id="compCustomBoolean_cnst">
	</type-converters>

    <component id="customBooleanConverter" class="org.osgi.test.cases.blueprint.components.injection.CustomBooleanConverter"/>

<!--Constructor-->
	<component id="compCustomBoolean_cnst" class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
		<constructor-arg type="java.lang.Boolean" value="T"/>
	</component>

<!--Property-->
	<component id="compCustomBoolean_prpt" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
		<property name="boolean">
            <value type="java.lang.Boolean">F</value>
        </property>
	</component>
</components>