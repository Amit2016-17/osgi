<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0">

	<type-converters>
             <ref component="customBooleanConverter"/>
	</type-converters>

    <bean id="customBooleanConverter" interface="org.osgi.service.blueprint.convert.Converter"/>

<!--Constructor-->
	<bean id="compCustomBoolean_cnst" class="org.osgi.test.cases.blueprint.components.injection.ConstructorInjection">
		<argument type="java.lang.Boolean" value="T"/>
	</bean>

<!--Property-->
	<bean id="compCustomBoolean_prpt" class="org.osgi.test.cases.blueprint.components.injection.PropertyInjection">
		<property name="boolean">
            <value type="java.lang.Boolean">F</value>
        </property>
	</bean>
</blueprint>
