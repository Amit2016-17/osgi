javac.source 			= 1.5
javac.target 			= 1.5

Private-Package						= org.osgi.test.support.*
Bundle-Version: 4.2.0.${project.build}
Export-Package						= ${p}.*
Bundle-RequiredExecutionEnvironment = J2SE-1.5
Bundle-Vendor: IBM
Bundle-DocURL: http://www.osgi.org/
Bundle-ContactAddress: info@mail.osgi.org
Bundle-Category: osgi,test
Bundle-Copyright: ${copyright}

Include-Resource					= \
	www/generic_collection_injection.jar=generic_collection_injection.jar , \
	www/error_generic_conversion.jar=error_generic_conversion.jar , \
	www/error_generic_rawclass.jar=error_generic_rawclass.jar , \
	www/error_reference_list_import.jar=error_reference_list_import.jar , \
	www/error_reference_list_service_import.jar=error_reference_list_service_import.jar , \
	www/error_enum_conversion.jar=error_enum_conversion.jar , \
	www/generic_property_injection.jar=generic_property_injection.jar , \
	www/managed_service_export.jar=managed_service_export.jar , \
	www/reference_collection_import.jar=reference_collection_import.jar , \
	www/reference_list_import.jar=reference_list_import.jar , \
	www/reference_list_service_import.jar=reference_list_service_import.jar , \
	www/pattern_injection.jar=pattern_injection.jar

Test-Cases							= ${testcases}

-buildpath							= ee.j2se; version=1.5, \
    org.osgi.test.support;                  version=project , \
    org.osgi.service.blueprint;             version=project , \
    org.osgi.test.cases.blueprint;          version=project , \
    osgi.core;				    version=4.1	, \
    ${junit}

-runbundles							= \
 	osgi.cmpn; version=latest, \
	org.osgi.impl.service.log; version=latest, \
	org.osgi.impl.service.cm;  version=latest, \
	org.osgi.impl.service.event; version=latest, \
 	org.osgi.test.cases.blueprint;  version=project, \
    org.osgi.impl.service.blueprint; version=latest
    
-runproperties = report=true, \
	osgi.compatibility.bootdelegation=false, \
	osgi.resolverMode=strict, \
	osgi.support.multipleHosts=true
    