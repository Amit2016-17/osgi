<?xml version="1.0" encoding="UTF-8"?>
<components xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0">

    <description>
        This is a checker for Pattern type conversion.
    </description>

    <!-- This gets both a ServiceManager and a reference collection injected -->
   <bean id="PatternChecker" class="org.osgi.test.cases.blueprint.java5.components.injection.PatternInjection">
       <argument value="a*b"/>
       <property name="pattern" value="a*b"/>
   </bean>
</components>

