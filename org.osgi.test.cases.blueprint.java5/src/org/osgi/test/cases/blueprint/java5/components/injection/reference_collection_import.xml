<?xml version="1.0" encoding="UTF-8"?>
<components xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.osgi.org/xmlns/blueprint/v1.0.0 http://www.osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd">

    <description>
        This is a checker for generic reference list validation.
    </description>

    <!-- This gets both a ServiceManager and a reference collection injected -->
   <component id="ReferenceChecker" class="org.osgi.test.cases.blueprint.java5.components.injection.GenericCollectionChecker" init-method="init">
       <constructor-arg value="ReferenceChecker"/>
       <property name="serviceManager">
           <reference interface="org.osgi.test.cases.blueprint.services.ServiceManager"/>
       </property>
       <property name="collection">
           <ref-list interface="org.osgi.test.cases.blueprint.services.TestServiceOne" availability="optional"/>
       </property>
   </component>
</components>

