<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0">

    <description>
        Injects a reference list of ServiceReferences into an incompatible property. The injected
        property expects TestServiceOne instances, but the member-type is service-reference.
    </description>

    <!-- This gets both a ServiceManager and a reference collection injected -->
   <bean id="ReferenceChecker" class="org.osgi.test.cases.blueprint.java5.components.injection.GenericServiceCollectionChecker" init-method="init">
       <argument value="ReferenceChecker"/>
       <property name="serviceManager">
           <reference interface="org.osgi.test.cases.blueprint.services.ServiceManager"/>
       </property>
       <property name="genericList">
           <reference-list interface="org.osgi.test.cases.blueprint.services.TestServiceOne" member-type="service-reference" availability="optional"/>
       </property>
   </bean>
</blueprint>

