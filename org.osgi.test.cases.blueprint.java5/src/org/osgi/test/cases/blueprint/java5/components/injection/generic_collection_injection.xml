<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0">

    <bean id="GenericListConstructor" class="org.osgi.test.cases.blueprint.java5.components.injection.GenericListInjection">
        <argument>
            <list>
                <value>123</value>
                <value>456</value>
            </list>
        </argument>
    </bean>

    <bean id="GenericListProperty" class="org.osgi.test.cases.blueprint.java5.components.injection.GenericListInjection">
        <property name="string">
            <list>
                <value>abc</value>
                <value>def</value>
            </list>
        </property>
        <property name="classList">
            <list>
                <value>java.lang.String</value>
                <value>org.osgi.test.cases.blueprint.java5.components.injection.GenericListInjection</value>
            </list>
        </property>
    </bean>

    <bean id="GenericSetConstructor" class="org.osgi.test.cases.blueprint.java5.components.injection.GenericSetInjection">
        <argument>
            <set>
                <value>3.1415926</value>
                <value>1.0</value>
            </set>
        </argument>
    </bean>

    <bean id="GenericSetProperty" class="org.osgi.test.cases.blueprint.java5.components.injection.GenericSetInjection">
        <property name="integer">
            <set>
                <value>123</value>
                <value>456</value>
            </set>
        </property>
        <property name="url">
            <set>
                <value>http://www.osgi.org</value>
                <value>http://www.w3c.org</value>
            </set>
        </property>
    </bean>

    <bean id="GenericMapConstructor" class="org.osgi.test.cases.blueprint.java5.components.injection.GenericMapInjection">
        <argument>
            <map>
                <entry key="1" value="true"/>
                <entry key="2" value="no"/>
                <entry key="3" value="on"/>
                <entry key="4" value="false"/>
            </map>
        </argument>
    </bean>

    <bean id="GenericMapProperty" class="org.osgi.test.cases.blueprint.java5.components.injection.GenericMapInjection">
        <property name="map1">
            <map>
                <entry key="1.0" value="true"/>
                <entry key="2.0" value="false"/>
            </map>
        </property>
        <property name="map2">
            <map>
                <entry key="osgi" value="http://www.osgi.org"/>
                <entry key="w3c" value="http://www.w3c.org"/>
            </map>
        </property>
    </bean>


    <!--Combining a simple test for ConcurrentMap conversion in here -->
    <bean id="mapToConcurrentMap" class="org.osgi.test.cases.blueprint.java5.components.injection.GenericMapInjection">
        <property name="concurrentMap">
            <map>
                <entry key="abc" value="1"/>
                <entry key="def" value="2"/>
            </map>
        </property>
    </bean>


    <!--Combining simple tests for Queue conversion in here -->
    <bean id="listToQueue" class="org.osgi.test.cases.blueprint.java5.components.injection.GenericListInjection">
        <property name="queue">
            <list>
                <value>abc</value>
                <value>def</value>
                <value>def</value>
            </list>
        </property>
    </bean>

    <bean id="setToQueue" class="org.osgi.test.cases.blueprint.java5.components.injection.GenericListInjection">
        <property name="queue">
            <set>
                <value>abc</value>
                <value>def</value>
                <value>def</value>
            </set>
        </property>
    </bean>

    <bean id="arrayToQueue" class="org.osgi.test.cases.blueprint.java5.components.injection.GenericListInjection">
        <property name="queue">
            <array>
                <value>abc</value>
                <value>def</value>
                <value>def</value>
            </array>
        </property>
    </bean>
</blueprint>

