# Set javac settings from JDT prefs
-include: ${build}/eclipse/jdt.bnd

Bundle-Description: Test Bundle for subsystem tests

Conditional-Package					= org.osgi.test.support.*
Private-Package						=  \
	${p}.junit.*, \
	${p}.resource, \
	${p}.util
Import-Package: ${-signaturetest}, *

Include-Resource					= \
	no.deps.a@1.0.0.jar, \
	no.deps.a@2.0.0.jar, \
	no.deps.b@1.0.0.jar, \
	no.deps.b@2.0.0.jar, \
	no.deps.c@1.0.0.jar, \
	no.deps.c@2.0.0.jar, \
	no.deps.d@1.0.0.jar, \
	no.deps.e@1.0.0.jar, \
	no.deps.f@1.0.0.jar, \
	share.a@1.0.0.jar, \
	share.b@1.0.0.jar, \
	share.c@1.0.0.jar, \
	share.d@1.0.0.jar, \
	share.e@1.0.0.jar, \
	share.f@1.0.0.jar, \
	share.g@1.0.0.jar, \
	share.h@1.0.0.jar, \
	share.i@1.0.0.jar, \
	share.j@1.0.0.jar, \
	share.k@1.0.0.jar, \
	app.service.dep.a.jar, \
	app.service.dep.b.jar, \
	app.service.dep.c.jar, \
	app.service.dep.d.jar, \
	app.service.dep.e.jar, \
	app.service.dep.f.jar, \
	app.service.dep.g.jar, \
	app.service.dep.h.jar, \
	app.service.dep.i.jar, \
	app.service.dep.j.jar, \
	app.service.dep.k.jar, \
	weaving.a.jar, \
	weaving.aa.jar, \
	weaving.b.jar, \
	weaving.c.jar, \
	deployment.manifest.a.jar, \
	deployment.headers.a.jar, \
	deployment.headers.b.jar, \
	preferred.provider.a.jar, \
	preferred.provider.b.jar, \
	preferred.provider.c.jar, \
	require.ee.native.jar

Test-Cases							= ${testcases}
-signaturetest                      = org.osgi.service.subsystem

-buildpath							= \
    org.osgi.test.support;                  version=project , \
    org.osgi.service.subsystem;				version=project , \
	osgi.core;								version=6.0	, \
	osgi.cmpn;         						version=5.0 , \
	${junit}

-runbundles = \
    ${junit},\
	org.osgi.impl.service.log; version=latest, \
	org.osgi.impl.service.cm; version=latest, \
	org.osgi.impl.service.coordinator; version=latest, \
	org.osgi.impl.service.resolver; version=latest, \
	org.osgi.impl.service.repository; version=latest, \
	org.osgi.impl.service.component; version=latest, \
	org.osgi.impl.service.subsystem; version=latest

-runproperties = ${runproperties}

-noee = true
