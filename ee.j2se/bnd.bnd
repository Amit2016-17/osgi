# Set javac settings from JDT prefs
-include: ${build}/eclipse/jdt.bnd

-nouses = true
Bundle-Description					: Contains the Java SE 1.7 execution environment stubbed classes and sources
Bundle-Version: 1.7.0.$(project.build)
Bundle-RequiredExecutionEnvironment	: JavaSE-1.7

Export-Package						: java.*;-noimport:=true, javax.*;-noimport:=true, org.*;-noimport:=true
Private-Package						: *
Include-Resource 					: legal/about.html, legal/LICENSE
