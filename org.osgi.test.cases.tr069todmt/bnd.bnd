# Set javac settings from JDT prefs
-include: ${project.workspace}/cnf/eclipse/jdt.bnd	

Conditional-Package					= org.osgi.test.support.*
Private-Package						=  \
	${p}.*

-buildpath							= \
    org.osgi.test.support;          version=project , \
    org.osgi.service.dmt;           version=project, \
    org.osgi.service.tr069todmt;    version=project , \
    biz.aQute.bnd;                  version=1.40, \
    osgi.cmpn;                      version=4.1, \
    osgi.core;                      version=4.3	, \
    ee.j2se;                        version=1.5 , \
    ${junit}

-runbundles						= \
	org.osgi.impl.service.cm; version=latest, \
	org.osgi.impl.service.event; version=latest, \
	org.osgi.impl.service.dmt; version=project, \
	org.osgi.impl.service.tr069todmt; version=project, \
	osgi.residential; version=latest

Test-Cases							= ${testcases}
-signaturetest                      = org.osgi.service.tr069todmt
	
