Bundle-Name:                             ${p}.client
Bundle-Version:                          1.0.0
Private-Package:                          ${p}.client

Bundle-Activator: org.osgi.test.cases.serviceloader.client.LegacyClient

# require the ServiceLoader Mediator to opt-in to processing
Require-Capability:\
 osgi.extender;filter:="(osgi.extender=osgi.serviceloader.processor)",\
 osgi.serviceloader; filter:="(osgi.serviceloader=org.osgi.test.cases.serviceloader.spi.ColorProvider)"
