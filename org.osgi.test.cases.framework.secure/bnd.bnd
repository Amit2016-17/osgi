Bundle-Version						: 4.2.0.${project.build}
Bundle-Description					: Secure Framework test cases.

Private-Package						= org.osgi.test.support.*, \
	${p}.junit.*
	
Export-Package						= \
	${p}.permissions.util;version=1.0.0

Include-Resource = \
	classloading.tb1.jar, \
	classloading.tb6a.jar, \
	classloading.tb6b.jar, \
	classloading.tb6c.jar, \
	classloading.tb6d.jar, \
	classloading.tb16b.jar, \
	classloading.tb16d.jar, \
	fragments.tb2a.jar, \
	fragments.tb2b.jar, \
	fragments.tb2c.jar, \
	fragments.tb2d.jar, \
	fragments.tb7f.jar, \
	fragments.tb7g.jar, \
	fragments.tb11.jar, \
	fragments.tb14.jar, \
	fragments.tb16a.jar, \
	fragments.tb16b.jar, \
	fragments.tb17a.jar, \
	fragments.tb17b.jar, \
	fragments.tb18.jar, \
	fragments.tb19.jar, \
	permissions.exporter1.jar, \
	permissions.exporter2.jar, \
	permissions.get.jar, \
	permissions.importer1.jar, \
	permissions.importer2.jar, \
	permissions.register.jar, \
	permissions.registerModify.jar, \
	permissions.registerPlural.jar, \
	permissions.setPermission.jar
	
Test-Cases							= ${classes;extending;org.junit.framework.TestCase|*DefaultTestBundleControl}

-buildpath							= \
    org.osgi.test.support;                  version=project , \
	org.osgi.framework;									version=project	, \
	osgi;									version=latest	, \
	ee.minimum; version=latest											, \
	${junit}

-runbundles							= \
	

-runproperties = report=true, \
	osgi.compatibility.bootdelegation=false, \
	osgi.resolverMode=strict, \
	org.osgi.framework.security=osgi, \
	org.osgi.framework.trust.repositories=${build}/keystore
	