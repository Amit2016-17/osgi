javac.profile=compact2
# Set javac settings from JDT prefs
-include: ${build}/eclipse/jdt.bnd

Bundle-Description: Tests the service layre bundle

Conditional-Package					= org.osgi.test.support.*
Private-Package: \
	${p}.junit.*,\
	org.osgi.test.cases.onem2m.http.json.applicationEntity,\
	org.osgi.test.cases.onem2m.http.json.container,\
	org.osgi.test.cases.onem2m.http.json.contentInstance,\
	org.osgi.test.cases.onem2m.http.json.cseBaseManagement,\
	org.osgi.test.cases.onem2m.http.json.discovery,\
	org.osgi.test.cases.onem2m.http.json.notification,\
	org.osgi.test.cases.onem2m.http.json.notificationlistener,\
	org.osgi.test.cases.onem2m.http.json.subscription

Import-Package:  *

Test-Cases							= ${testcases}

-buildpath: \
	org.osgi.test.support;version=project,\
	osgi.core;version=4.3.1,\
	${junit},\
	org.osgi.dto;version=latest,\
	slf4j.api,\
	org.osgi.util.promise;version=latest,\
	org.osgi.service.onem2m;version=latest

