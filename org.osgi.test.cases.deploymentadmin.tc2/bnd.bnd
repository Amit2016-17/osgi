Bundle-Version: 4.2.0.${project.build}

Private-Package						= ${p}.tbc.*;-split-package:=merge-first, \
	org.osgi.test.support.*;-split-package:=merge-first
	
Export-Package						= ${p}.tbc.*;-split-package:=merge-first
	
Bundle-Category: osgi,test
	
Include-Resource					= \
	www/simple.dp=res/simple.dp,\
	www/rp_resource_install.dp=res/rp_resource_install.dp,\
	www/rp_resource_update.dp=res/rp_resource_update.dp,\
	www/rp_resource_uninstall.dp=res/rp_resource_uninstall.dp,\
	www/auto_config.dp=res/auto_config.dp,\
	www/session_test.dp=res/session_test.dp,\
	www/session_update_test.dp=res/session_update_test.dp,\
	www/transactional_session.dp=res/transactional_session.dp,\
	www/resource_processor2.dp=res/resource_processor2.dp,\
	www/session_resource_processor.dp=res/session_resource_processor.dp,\
	www/block_session.dp=res/block_session.dp,\
	www/manifest_not_1st_file.dp=res/manifest_not_1st_file.dp,\
	www/rp_from_other_dp.dp=res/rp_from_other_dp.dp,\
	www/installation_fails.dp=res/installation_fails.dp,\
	www/resource_processor_rp3.dp=res/resource_processor_rp3.dp,\
	www/resource_processor_customizer.dp=res/resource_processor_customizer.dp,\
	www/bsn_dif_from_manifest.dp=res/bsn_dif_from_manifest.dp,\
	www/bversion_dif_from_manifest.dp=res/bversion_dif_from_manifest.dp,\
	www/simple_bundle_res.dp=res/simple_bundle_res.dp,\
	www/bundle_fail_res.dp=res/bundle_fail_res.dp,\
	www/bundle_fail_on_stop_res.dp=res/bundle_fail_on_stop_res.dp,\
	www/simple_resource_rp3.dp=res/simple_resource_rp3.dp,\
	www/simple_no_resource.dp=res/simple_no_resource.dp,\
	www/simple_no_bundle.dp=res/simple_no_bundle.dp,\
	www/simple_uninstall_bundle.dp=res/simple_uninstall_bundle.dp
			
Test-Cases							= ${classes;extending;org.junit.framework.TestCase|*DefaultTestBundleControl}

-runbundles							= \
	org.osgi.impl.service.log; version=latest,\
	org.eclipse.equinox.util;  version=latest, \
	org.eclipse.equinox.ds;  version=latest, \
	org.osgi.impl.bundle.jaxp;  version=latest, \
	org.eclipse.equinox.metatype;  version=latest, \
	org.eclipse.equinox.event;  version=latest, \
	org.osgi.impl.bundle.autoconf; version=project, \
	org.osgi.impl.service.deploymentadmin; version=project, \
	org.apache.felix.configadmin;  version=latest, \
	osgi.mobile; version=latest

-buildpath=\
    org.osgi.test.support; 				version=project , \
	org.osgi.service.deploymentadmin; 	version=project, \
	osgi.core;							version=4.2, \
	osgi.mobile;						version=4.0, \
	ee.foundation; 						version=1.1, \
	${junit}
		
-runproperties= \
	report=true, \
	org.osgi.framework.security=osgi, \
	org.osgi.framework.trust.repositories=${build}/keystore, \
	org.osgi.test.cases.deploymentadmin.timeout=180000, \
	org.osgi.test.cases.deploymentadmin.short_timeout=3500, \
	org.osgi.impl.service.deploymentadmin.keystore.file=${build}/keystore, \
	org.osgi.impl.service.deploymentadmin.userprompt=false, \
	org.osgi.impl.service.deploymentadmin.deliveredarea=${target-dir}, \
	org.osgi.vendor.deploymentadmin=org.osgi.impl.service.deploymentadmin.perm, \
	org.osgi.vendor.dmtree.DigestDelegate=org.osgi.impl.service.dmt.export.Digest, \
	info.dmtree.osgi.root=./OSGi/
	
	
-runsystempackages=javax.xml.parsers, org.xml.sax, org.xml.sax.helpers, javax.security.auth.x500		
				