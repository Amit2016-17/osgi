# Set javac settings from JDT prefs
-include: ${project.workspace}/cnf/eclipse/jdt.bnd

Export-Package: org.osgi.test.support.step;-noimport:=true

Conditional-Package: org.osgi.test.support.*

Private-Package	= \
	${p}.*

Test-Cases		= \
	${testcases}

-buildpath		= \
	org.osgi.test.support;			version=latest, \
	org.osgi.service.dal;			version=latest, \
	org.osgi.service.dal.functions;	version=latest, \
	osgi.core;						version=4.2, \
	osgi.cmpn;						version=4.2, \
	ee.minimum;						version=1.2, \
	${junit}

-runbundles		= \
	org.osgi.service.event;			version=latest, \
	org.osgi.service.dal;			version=latest, \
	org.osgi.service.dal.functions;	version=latest, \
	org.osgi.service.cm;			version=latest, \
	org.osgi.service.component;	 	version=latest, \
	org.osgi.impl.service.dal; 		version=latest, \
	org.osgi.impl.service.dal.step;	version=latest, \
	org.osgi.impl.service.event;	version=latest

-runproperties	= ${runsecureproperties}

-signaturetest	= \
	org.osgi.service.dal.functions, \
	org.osgi.service.dal.functions.data

