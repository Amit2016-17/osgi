Bundle-Version: 1.0.0.${project.build}

Include-Resource: dummy.jar

Private-Package						= ${p}.junit, \
	org.osgi.test.support.*

-buildpath							= \
	org.osgi.test.support; 	version=project, \
    org.osgi.service.coordinator;           version=project , \
    osgi.core;                              version=4.2     , \
    osgi.cmpn;                              version=4.2     , \
	ee.j2se;								version=1.5		, \
	${junit}

Test-Cases							= ${testcases}
-signaturetest                      = org.osgi.service.coordinator

-runbundles						= \
	osgi.cmpn; version=latest, \
    org.osgi.impl.service.coordinator; version=latest, \
    org.osgi.impl.service.log; version=latest, \
	org.osgi.impl.service.cm; version=latest, \
	org.osgi.impl.service.component; version=latest

-runproperties					= ${runproperties}, org.osgi.service.http.port=9091

javac.source            = 1.5
javac.target            = jsr14
