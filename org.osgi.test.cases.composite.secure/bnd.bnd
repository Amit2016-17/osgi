javac.source            = 1.5
javac.target            = jsr14

Bundle-Version						: 4.2.0.${project.build}
Bundle-Description					: Secure Framework test cases.

Private-Package						= org.osgi.test.support.*, \
	org.osgi.test.cases.composite, \
	${p}.junit.*

Export-Package						= \
	org.osgi.test.cases.composite

Include-Resource					= \
	tb1.jar
	
Test-Cases							= ${testcases}

-buildpath							= \
    org.osgi.test.support;                  version=project , \
    org.osgi.test.cases.composite;          version=project , \
	org.osgi.framework;						version=project	, \
	org.osgi.service.composite;				version=project , \
	osgi.core;								version=4.2	, \
	osgi.cmpn;								version=4.0	, \
	ee.minimum; 							version=1.2.1	, \
	${junit}

-runbundles							= \
	

-runproperties = ${runsecureproperties}, \
	osgi.console=test

