javac.source            = 1.5
javac.target            = jsr14
Bundle-Version						: 1.0.0.${project.build}
Bundle-Description					: Remote Service Admin test cases.

Export-Package 					= ${p}.common;version=1.0.0
	
Private-Package						= org.osgi.test.support.*, \
	${p}.*
	
Bundle-Category: osgi,test

Include-Resource					= \
	tb1.jar=tb1.jar, \
	tb2.jar=tb2.jar
	
Test-Cases							= ${testcases}
-signaturetest                      = org.osgi.service.remoteserviceadmin

-buildpath							= \
    org.osgi.test.support;                  version=project , \
	org.osgi.service.remoteserviceadmin;	version=project	, \
	osgi.core;								version=4.2	, \
    osgi.enterprise;           				version=4.2 , \
	ee.minimum; 							version=1.2.1	, \
	${junit}

-runbundles							= \
	osgi.enterprise;						version=latest	, \
	org.eclipse.equinox.event;              version=latest, \
	org.apache.cxf.dosgi.singlebundle;      version=latest, \
	com.springsource.org.apache.log4j;		version=latest, \
	org.apache.cxf.dosgi.discovery.zookeeper; version=latest, \
	org.apache.cxf.dosgi.discovery.server; version=latest, \
	org.apache.cxf.dosgi.discovery.server.config; version=latest, \
	org.osgi.impl.service.log;				version=latest
	

-runproperties = report=true, \
	osgi.compatibility.bootdelegation=false, \
	osgi.resolverMode=strict, \
	osgi.console=1111, \
	osgi.support.multipleHosts=true, \
	${p}.bundles=${repo;osgi.cmpn}|${repo;org.osgi.impl.service.log}|${repo;org.apache.cxf.dosgi.singlebundle}|${repo;org.apache.cxf.dosgi.discovery.server.config}|${repo;org.osgi.test.support}, \
	${p}.serverconfig="service.exported.configs,org.apache.cxf.ws.address", \
	service.exported.configs=org.apache.cxf.ws, \
	org.apache.cxf.ws.address=http://localhost:9090/test
	
	