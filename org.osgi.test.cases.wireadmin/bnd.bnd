Bundle-Description: Test Bundle for WireAdmin Service tests


Private-Package						= org.osgi.test.support.*, \
	${p}.junit.*
	
Test-Cases							= ${testcases}
-signaturetest                      = org.osgi.service.wireadmin

Include-Resource				= \
	tb1.jar, \
	tb2.jar, \
	tb3.jar, \
	tb4.jar, \
	tb5.jar, \
	tb6.jar, \
	tb7.jar

-buildpath							= \
	org.osgi.service.wireadmin;				version=project, \
    org.osgi.test.support;                  version=project , \
	osgi.core;								version=4.1	, \
	ee.minimum;								version=1.2		, \
	${junit}

-runbundles							= \
	osgi.cmpn; version=latest, \
	org.osgi.impl.service.log; version=latest, \
	org.osgi.impl.service.cm; version=latest, \
	org.osgi.impl.service.wireadmin; version=project

-runproperties = ${runsecureproperties}
	