javac.profile=compact2
# Set javac settings from JDT prefs
-include: ${build}/eclipse/jdt.bnd

Bundle-Activator				= $(p).Activator
Bundle-Description				= An OSGi Midlet Container
Bundle-Vendor					= Nokia

Export-Package 					= javax.microedition.midlet, \
    org.osgi.application; -split-package:=first; provide:=true
-privatepackage					= ${p}
-includeresource				= TestIcon.gif

-buildpath						= \
	osgi.core;					version=4.3.1, \
	osgi.cmpn;					version=4.3.1
