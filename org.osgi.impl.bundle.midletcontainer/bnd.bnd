# Set javac settings from JDT prefs
-include: ${project.workspace}/cnf/eclipse/jdt.bnd

Bundle-Activator				= $(p).Activator
Bundle-Description				= An OSGi Midlet Container
Bundle-Vendor					= Nokia
Bundle-RequiredExecutionEnvironment = J2SE-1.4

Export-Package 					= javax.microedition.midlet, \
    org.osgi.application
Private-Package					= ${p}
Include-Resource				= TestIcon.gif

-buildpath						= \
	ee.j2se;					version=1.4, \
	osgi.core;					version=4.2, \
	osgi.cmpn;					version=4.1
