# Set javac settings from JDT prefs
-include: ${project.workspace}/cnf/eclipse/jdt.bnd

Bundle-Description: CT for the OSGi/CDI integration

Conditional-Package					= org.osgi.test.support.*
Private-Package						=  \
	${p}.junit.*
Import-Package: ${-signaturetest}, *

Include-Resource: \
	serviceapi.jar,\
	tb.jar,\
	tb1.jar, \
	tb2.jar, \
	tb3.jar, \
	tb4.jar, \
	tb5.jar, \
	tb6.jar, \
	tb7.jar,\
	tb8.jar,\
	tb9.jar,\
	tb10.jar,\
	tb11.jar,\
	tf3.jar

Test-Cases							= ${testcases}
-signaturetest                      = org.osgi.service.cdi

javac.profile=

-buildpath							= \
	org.osgi.service.event;                 version=project, \
	org.osgi.service.cdi;                   version=project, \
	org.osgi.test.support;                  version=project, \
	org.osgi.util.converter;                version=project, \
	osgi.core;                              version=6.0, \
	org.apache.aries.javax.inject-api;      version=latest, \
	org.apache.aries.javax.cdi-api;         version=latest, \
	org.apache.aries.javax.annotation-api;  version=latest, \
	${junit}

-runbundles							= \
    ${junit},\
	org.osgi.impl.bundle.cdi;               version=latest

-runproperties = \
	${runproperties},\
	osgi.console=
