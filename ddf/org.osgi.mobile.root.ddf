                <CaseSense>
                    <CS></CS>
                </CaseSense>
            </DFProperties>
        </Node>
        <!-- Policy Management Object reference -->
        <Node>
            <NodeName>Policy</NodeName>
            <DFProperties>
                <AccessType>
                    <Get></Get>
                </AccessType>
                <Description>Policy subtree.</Description>
                <DFFormat>
                    <node/>
                </DFFormat>
                <Occurrence>
                    <One/>
                </Occurrence>
                <Scope>
                    <Permanent/>
                </Scope>
                <DFTitle>Policy subtree</DFTitle>
                <DFType>
                    <DDFName>org.osgi/1.0/PolicyManagementObject</DDFName>
                </DFType>
                <CaseSense>
                    <CS></CS>
                </CaseSense>
            </DFProperties>
        </Node>
    </Node>
</MgmtTree>
