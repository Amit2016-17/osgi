<MgmtTree>
    <VerDTD>1.2</VerDTD>
    <Man>OSGi Alliance</Man>
    <!-- OSGi Root node -->
    <Node>
        <!-- The name and location of the OSGi root is not defined -->
        <NodeName></NodeName>
        <DFProperties>
            <AccessType>
                <Get/>
            </AccessType>
            <Description>Provides a root for all OSGi defined nodes. The 
                location of the OSGi subtree in the DMT is not defined by the 
                specification.</Description>
            <DFFormat>
                <node/>
            </DFFormat>
            <Occurrence>
                <One/>
            </Occurrence>
            <Scope>
                <Permanent/>
            </Scope>
            <DFTitle>OSGi Root Node</DFTitle>
            <DFType>
                <DDFName/>
            </DFType>
            <CaseSense>
                <CS></CS>
            </CaseSense>
        </DFProperties>
        <!-- Configuration Management Object reference -->
        <Node>
            <NodeName>Configuration</NodeName>
            <DFProperties>
                <AccessType>
                    <Get></Get>
                </AccessType>
                <Description>Access to Configuration objects from Configuration 
                    Admin.</Description>
                <DFFormat>
                    <node/>
                </DFFormat>
                <Occurrence>
                    <One/>
                </Occurrence>
                <Scope>
                    <Permanent/>
                </Scope>
                <DFTitle>Configuration subtree</DFTitle>
                <DFType>
                    <DDFName>org.osgi/1.0/ConfigurationManagementObject</DDFName>
                </DFType>
                <CaseSense>
                    <CS></CS>
                </CaseSense>
            </DFProperties>
        </Node>
        <!-- Log Management Object reference -->
        <Node>
            <NodeName>Log</NodeName>
            <DFProperties>
                <AccessType>
                    <Get></Get>
                </AccessType>
                <Description>Access to log records.</Description>
                <DFFormat>
                    <node/>
                </DFFormat>
                <Occurrence>
                    <One/>
                </Occurrence>
                <Scope>
                    <Permanent/>
                </Scope>
                <DFTitle>Log subtree</DFTitle>
                <DFType>
                    <DDFName>org.osgi/1.0/LogManagementObject</DDFName>
                </DFType>
                <CaseSense>
                    <CS></CS>
                </CaseSense>
            </DFProperties>
        </Node>
        <!-- Monitor Management Object reference -->
        <Node>
            <NodeName>Monitor</NodeName>
            <DFProperties>
                <AccessType>
                    <Get></Get>
                </AccessType>
                <Description>Monitoring subtree, optional.</Description>
                <DFFormat>
                    <node/>
                </DFFormat>
                <Occurrence>
                    <ZeroOrOne/>
                </Occurrence>
                <Scope>
                    <Permanent/>
                </Scope>
                <DFTitle>Monitoring subtree</DFTitle>
                <DFType>
                    <DDFName>org.osgi/1.0/MonitorManagementObject</DDFName>
                </DFType>
                <CaseSense>
                    <CS></CS>
                </CaseSense>
            </DFProperties>
        </Node>
        <!-- Application Management Object reference -->
        <Node>
            <NodeName>Application</NodeName>
            <DFProperties>
                <AccessType>
                    <Get></Get>
                </AccessType>
                <Description>Applications subtree.</Description>
                <DFFormat>
                    <node/>
                </DFFormat>
                <Occurrence>
                    <One/>
                </Occurrence>
                <Scope>
                    <Permanent/>
                </Scope>
                <DFTitle>Applications subtree</DFTitle>
                <DFType>
                    <DDFName>org.osgi/1.0/ApplicationManagementObject</DDFName>
                </DFType>
                <CaseSense>
                    <CS></CS>
                </CaseSense>
            </DFProperties>
        </Node>
