<MgmtTree>
    <VerDTD>1.2</VerDTD>
    <Man>OSGi Alliance</Man>
    <!-- Log Management Object -->
    <Node>
        <NodeName>Log</NodeName>
        <DFProperties>
            <AccessType>
                <Get></Get>
            </AccessType>
            <Description>Access to log records.</Description>
            <DFFormat>
                <node/>
            </DFFormat>
            <Occurrence>
                <One/>
            </Occurrence>
            <Scope>
                <Permanent/>
            </Scope>
            <DFTitle>Log subtree</DFTitle>
            <DFType>
                <DDFName></DDFName>
            </DFType>
            <CaseSense>
                <CS></CS>
            </CaseSense>
        </DFProperties>
        <!-- search id -->
        <Node>
            <!-- Name is dynamic -->
            <NodeName></NodeName>
            <DFProperties>
                <AccessType>
                    <Add></Add>
                    <Delete></Delete>
                    <Get></Get>
                </AccessType>
                <Description>A log search request.</Description>
                <DFFormat>
                    <node/>
                </DFFormat>
                <Occurrence>
                    <ZeroOrMore/>
                </Occurrence>
                <Scope>
                    <Dynamic></Dynamic>
                </Scope>
                <DFTitle>Log search request subtree</DFTitle>
                <DFType>
                    <DDFName></DDFName>
                </DFType>
                <CaseSense>
                    <CS></CS>
                </CaseSense>
            </DFProperties>
            <!-- Filter -->
            <Node>
                <NodeName>Filter</NodeName>
                <DFProperties>
                    <AccessType>
                        <Get></Get>
                        <Replace></Replace>
                    </AccessType>
                    <DefaultValue></DefaultValue>
                    <Description>A search filter for log records.</Description>
                    <DFFormat>
                        <chr/>
                    </DFFormat>
                    <Occurrence>
                        <One/>
                    </Occurrence>
                    <!-- automatic node -->
                    <Scope>
                        <Dynamic></Dynamic>
                    </Scope>
                    <DFTitle>Search filter</DFTitle>
                    <DFType>
                        <MIME>text/plain</MIME>
                    </DFType>
                    <CaseSense>
                        <CS></CS>
                    </CaseSense>
                </DFProperties>
            </Node>
            <!--end log/search_id/filter-->
            <!-- exclude -->
            <Node>
                <NodeName>Exclude</NodeName>
                <DFProperties>
                    <AccessType>
                        <Get></Get>
                        <Replace></Replace>
                    </AccessType>
                    <DefaultValue></DefaultValue>
                    <Description>A list of fields to exclude from 
                        the result.</Description>
                    <DFFormat>
                        <chr/>
                    </DFFormat>
                    <Occurrence>
                        <One/>
                    </Occurrence>
                    <!-- automatic node -->
                    <Scope>
                        <Dynamic></Dynamic>
                    </Scope>
                    <DFTitle>Excluded fields</DFTitle>
                    <DFType>
                        <MIME>text/plain</MIME>
                    </DFType>
                    <CaseSense>
                        <CS></CS>
                    </CaseSense>
                </DFProperties>
            </Node>
            <!--end log/search_id/exclude-->
            <!-- maxrecords -->
            <Node>
                <NodeName>MaxRecords</NodeName>
                <DFProperties>
                    <AccessType>
                        <Get></Get>
                        <Replace></Replace>
                    </AccessType>
                    <DefaultValue>0</DefaultValue>
                    <Description>Maximum number of records in 
                        the result.</Description>
                    <DFFormat>
                        <int/>
                    </DFFormat>
                    <Occurrence>
                        <One/>
                    </Occurrence>
                    <!-- automatic node -->
                    <Scope>
                        <Dynamic></Dynamic>
                    </Scope>
                    <DFTitle>Maximum number of records in result</DFTitle>
                    <DFType>
                        <MIME>text/plain</MIME>
                    </DFType>
                    <CaseSense>
                        <CS></CS>
                    </CaseSense>
                </DFProperties>
            </Node>
            <!--end log/search_id/maxrecords-->
            <!-- logresult -->
            <Node>
                <NodeName>LogResult</NodeName>
                <DFProperties>
                    <AccessType>
                        <Get></Get>
                    </AccessType>
                    <Description>The result of the search.</Description>
                    <DFFormat>
                        <node/>
                    </DFFormat>
                    <Occurrence>
                        <One/>
                    </Occurrence>
                    <!--automatic-->
                    <Scope>
                        <Dynamic></Dynamic>
                    </Scope>
                    <DFTitle>Search result subtree</DFTitle>
                    <DFType>
                        <DDFName></DDFName>
                    </DFType>
                    <CaseSense>
                        <CS></CS>
                    </CaseSense>
                </DFProperties>
                <!-- record id -->
                <Node>
                    <!--generated name-->
                    <NodeName></NodeName>
                    <DFProperties>
                        <AccessType>
                            <Get></Get>
                        </AccessType>
                        <Description>One log entry of the search
                            results.</Description>
                        <DFFormat>
                            <node/>
                        </DFFormat>
                        <Occurrence>
                            <ZeroOrMore></ZeroOrMore>
                        </Occurrence>
                        <Scope>
                            <Dynamic></Dynamic>
                        </Scope>
                        <DFTitle>Log record subtree.</DFTitle>
                        <DFType>
                            <DDFName></DDFName>
                        </DFType>
                        <CaseSense>
                            <CS></CS>
                        </CaseSense>
                    </DFProperties>
                    <!-- time -->
                    <Node>
                        <NodeName>Time</NodeName>
                        <DFProperties>
                            <AccessType>
                                <Get></Get>
                            </AccessType>
                            <Description>Timestamp of the 
                                record.</Description>
                            <DFFormat>
                                <chr/>
                            </DFFormat>
                            <Occurrence>
                                <ZeroOrOne></ZeroOrOne>
                            </Occurrence>
                            <!--automatic-->
                            <Scope>
                                <Dynamic></Dynamic>
                            </Scope>
                            <DFTitle>Timestamp</DFTitle>
                            <DFType>
                                <MIME>text/plain</MIME>
                            </DFType>
                            <CaseSense>
                                <CS></CS>
                            </CaseSense>
                        </DFProperties>
                    </Node>
                    <!--end log/id/logresult/recordid/time-->
                    <!-- severity -->
                    <Node>
                        <NodeName>Severity</NodeName>
                        <DFProperties>
                            <AccessType>
                                <Get></Get>
                            </AccessType>
                            <Description>Severity of the 
                                record.</Description>
                            <DFFormat>
                                <int/>
                            </DFFormat>
                            <Occurrence>
                                <ZeroOrOne></ZeroOrOne>
                            </Occurrence>
                            <!--automatic-->
                            <Scope>
                                <Dynamic></Dynamic>
                            </Scope>
                            <DFTitle>Severity</DFTitle>
                            <DFType>
                                <MIME>text/plain</MIME>
                            </DFType>
                            <CaseSense>
                                <CS></CS>
                            </CaseSense>
                        </DFProperties>
                    </Node>
                    <!--end log/id/logresult/recordid/Severity-->
                    <!-- system -->
                    <Node>
                        <NodeName>System</NodeName>
                        <DFProperties>
                            <AccessType>
                                <Get></Get>
                            </AccessType>
                            <Description>Originating system of the record.</Description>
                            <DFFormat>
                                <chr/>
                            </DFFormat>
                            <Occurrence>
                                <ZeroOrOne></ZeroOrOne>
                            </Occurrence>
                            <!--automatic-->
                            <Scope>
                                <Dynamic></Dynamic>
                            </Scope>
                            <DFTitle>Originating system</DFTitle>
                            <DFType>
                                <MIME>text/plain</MIME>
                            </DFType>
                            <CaseSense>
                                <CS></CS>
                            </CaseSense>
                        </DFProperties>
                    </Node>
                    <!--end log/id/logresult/recordid/System-->
                    <!-- subsystem -->
                    <Node>
                        <NodeName>SubSystem</NodeName>
                        <DFProperties>
                            <AccessType>
                                <Get></Get>
                            </AccessType>
                            <Description>Originating subsystem of the 
                                record.</Description>
                            <DFFormat>
                                <chr/>
                            </DFFormat>
                            <Occurrence>
                                <ZeroOrOne></ZeroOrOne>
                            </Occurrence>
                            <!--automatic-->
                            <Scope>
                                <Dynamic></Dynamic>
                            </Scope>
                            <DFTitle>Originating subsystem</DFTitle>
                            <DFType>
                                <MIME>text/plain</MIME>
                            </DFType>
                            <CaseSense>
                                <CS></CS>
                            </CaseSense>
                        </DFProperties>
                    </Node>
                    <!--end log/id/logresult/recordid/SubSystem-->
                    <!-- Message -->
                    <Node>
                        <NodeName>Message</NodeName>
                        <DFProperties>
                            <AccessType>
                                <Get></Get>
                            </AccessType>
                            <Description>The textual description
                                of the record.</Description>
                            <DFFormat>
                                <chr/>
                            </DFFormat>
                            <Occurrence>
                                <ZeroOrOne></ZeroOrOne>
                            </Occurrence>
                            <!--automatic-->
                            <Scope>
                                <Dynamic></Dynamic>
                            </Scope>
                            <DFTitle>Message</DFTitle>
                            <DFType>
                                <MIME>text/plain</MIME>
                            </DFType>
                            <CaseSense>
                                <CS></CS>
                            </CaseSense>
                        </DFProperties>
                    </Node>
                    <!--end log/id/logresult/recordid/Message-->
                    <!-- Data -->
                    <Node>
                        <NodeName>Data</NodeName>
                        <DFProperties>
                            <AccessType>
                                <Get></Get>
                            </AccessType>
                            <Description>Supplementary data of the
                                record.</Description>
                            <DFFormat>
                                <chr/>
                            </DFFormat>
                            <Occurrence>
                                <ZeroOrOne></ZeroOrOne>
                            </Occurrence>
                            <!--automatic-->
                            <Scope>
                                <Dynamic></Dynamic>
                            </Scope>
                            <DFTitle>Supplementary data</DFTitle>
                            <DFType>
                                <MIME>text/plain</MIME>
                            </DFType>
                            <CaseSense>
                                <CS></CS>
                            </CaseSense>
                        </DFProperties>
                    </Node>
                    <!--end log/id/logresult/recordid/Data-->
                </Node>
                <!--end log/id/logresult/recordid-->
            </Node>
            <!-- log/id/logresult -->
        </Node>
        <!--end log/search_id-->
    </Node>
    <!--end log-->
</MgmtTree>
