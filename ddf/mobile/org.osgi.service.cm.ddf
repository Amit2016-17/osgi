<MgmtTree>
    <VerDTD>1.2</VerDTD>
    <Man>OSGi Alliance</Man>
    <!-- Configuration Management Object -->
    <Node>
        <NodeName>Configuration</NodeName>
        <DFProperties>
            <AccessType>
                <Get></Get>
            </AccessType>
            <Description>Access to Configuration objects from Configuration 
                Admin.</Description>
            <DFFormat>
                <node/>
            </DFFormat>
            <Occurrence>
                <One/>
            </Occurrence>
            <Scope>
                <Permanent/>
            </Scope>
            <DFTitle>Configuration subtree</DFTitle>
            <DFType>
                <DDFName></DDFName>
            </DFType>
            <CaseSense>
                <CS></CS>
            </CaseSense>
        </DFProperties>
        <Node>
            <!-- service.pid -->
            <!-- Name is not given here, it is dynamic -->
            <NodeName></NodeName>
            <DFProperties>
                <AccessType>
                    <Add/>
                    <Delete/>
                    <Get/>
                </AccessType>
                <Description>Contains the configuration dictionary of the 
                    managed object.</Description>
                <DFFormat>
                    <node/>
                </DFFormat>
                <Occurrence>
                    <ZeroOrMore/>
                </Occurrence>
                <Scope>
                    <Dynamic/>
                </Scope>
                <DFTitle>Unique identifier of the managed service</DFTitle>
                <DFType>
                    <DDFName></DDFName>
                </DFType>
                <CaseSense>
                    <CS></CS>
                </CaseSense>
            </DFProperties>
            <!-- factory pid-->
            <Node>
                <NodeName>FactoryPid</NodeName>
                <DFProperties>
                    <AccessType>
                        <Add></Add>
                        <Get></Get>
                    </AccessType>
                    <Description>This node, if present, indicates 
                        that the subtree is for a factory configuration, 
                        and contains the PID of the Managed Service
                        Factory.</Description>
                    <DFFormat>
                        <chr/>
                    </DFFormat>
                    <Occurrence>
                        <ZeroOrOne></ZeroOrOne>
                    </Occurrence>
                    <Scope>
                        <Dynamic></Dynamic>
                    </Scope>
                    <DFTitle>The PID of the Managed Service Factory</DFTitle>
                    <DFType>
                        <MIME>text/plain</MIME>
                    </DFType>
                    <CaseSense>
                        <CS></CS>
                    </CaseSense>
                </DFProperties>
            </Node>
            <!--end factory pid-->
            <!-- location-->
            <Node>
                <NodeName>Location</NodeName>
                <DFProperties>
                    <AccessType>
                        <Add></Add>
                        <Get></Get>
                    </AccessType>
                    <Description>Contains the location of the
                        bundle to which this configuration is bound
                        (if any).</Description>
                    <DFFormat>
                        <chr/>
                    </DFFormat>
                    <Occurrence>
                        <One/>
                    </Occurrence>
                    <Scope>
                        <Dynamic></Dynamic>
                    </Scope>
                    <DFTitle>The location of the bundle to which
                    this configuration is bound</DFTitle>
                    <DFType>
                        <MIME>text/plain</MIME>
                    </DFType>
                    <CaseSense>
                        <CS></CS>
                    </CaseSense>
                </DFProperties>
            </Node>
            <!--end location-->
            <!-- pid -->
            <Node>
                <NodeName>Pid</NodeName>
                <DFProperties>
                    <AccessType>
                        <Add></Add>
                        <Get></Get>
                    </AccessType>
                    <Description>Contains the actual PID of the
                        configuration as used by the Configuration
                        Admin service, automatically generated for
                        factory configurations.</Description>
                    <DFFormat>
                        <chr/>
                    </DFFormat>
                    <Occurrence>
                        <One/>
                    </Occurrence>
                    <Scope>
                        <Dynamic></Dynamic>
                    </Scope>
                    <DFTitle>The real PID of the configuration</DFTitle>
                    <DFType>
                        <MIME>text/plain</MIME>
                    </DFType>
                    <CaseSense>
                        <CS></CS>
                    </CaseSense>
                </DFProperties>
            </Node>
            <!--end pid-->
            <Node>
                <!-- keys -->
                <NodeName>Keys</NodeName>
                <DFProperties>
                    <!-- Automatic node: dynamic but Add is not supported -->
                    <AccessType>
                        <Get/>
                    </AccessType>
                    <Description>Holds the key nodes that contain
                        the values for a configuration
                        dictionary.</Description> 
                    <DFFormat>
                        <node/>
                    </DFFormat>
                    <Occurrence>
                        <One/>
                    </Occurrence>
                    <Scope>
                        <Dynamic/>
                    </Scope>
                    <DFTitle>Keys-values subtree for a configuration 
                        dictionary</DFTitle>
                    <DFType>
                        <DDFName></DDFName>
                    </DFType>
                    <CaseSense>
                        <CS></CS>
                    </CaseSense>
                </DFProperties>
                <!-- key -->
                <Node>
                    <!-- Name is not given here, it is dynamic -->
                    <NodeName></NodeName>
                    <DFProperties>
                        <AccessType>
                            <Add/>
                            <Delete/>
                            <Get/>
                        </AccessType>
                        <Description>Stores a key-value pair in this 
                            configuration.</Description>
                        <DFFormat>
                            <node/>
                        </DFFormat>
                        <Occurrence>
                            <ZeroOrMore/>
                        </Occurrence>
                        <Scope>
                            <Dynamic/>
                        </Scope>
                        <DFTitle>Name of the dictionary key</DFTitle>
                        <DFType>
                            <DDFName></DDFName>
                        </DFType>
                        <CaseSense>
                            <CS></CS>
                        </CaseSense>
                    </DFProperties>
                    <!-- Type-->
                    <Node>
                        <NodeName>Type</NodeName>
                        <DFProperties>
                            <AccessType>
                                <Add></Add>
                                <Get></Get>
                            </AccessType>
                            <Description>Identifies the type of
                                the configuration data.</Description> 
                            <DFFormat>
                                <chr/>
                            </DFFormat>
                            <Occurrence>
                                <One/>
                            </Occurrence>
                            <Scope>
                                <Dynamic></Dynamic>
                            </Scope>
                            <DFTitle>The type of the configuration
                                data</DFTitle>
                            <DFType>
                                <MIME>text/plain</MIME>
                            </DFType>
                            <CaseSense>
                                <CS></CS>
                            </CaseSense>
                        </DFProperties>
                    </Node>
                    <!-- end cfg/pid/keys/key/type-->
                    <!-- Cardinality-->
                    <Node>
                        <NodeName>Cardinality</NodeName>
                        <DFProperties>
                            <AccessType>
                                <Add></Add>
                                <Get></Get>
                            </AccessType>
                            <Description>Identifies the
                                cardinality of the configuration data,
                                the value is either scalar, vector or
                                array.</Description>
                            <DFFormat>
                                <chr/>
                            </DFFormat>
                            <Occurrence>
                                <One/>
                            </Occurrence>
                            <Scope>
                                <Dynamic></Dynamic>
                            </Scope>
                            <DFTitle>The cardinality of the
                                configuration data</DFTitle>
                            <DFType>
                                <MIME>text/plain</MIME>
                            </DFType>
                            <CaseSense>
                                <CS></CS>
                            </CaseSense>
                        </DFProperties>
                    </Node>
                    <!-- end cfg/pid/keys/key/Cardinality-->
                    <!-- Value-->
                    <Node>
                        <NodeName>Value</NodeName>
                        <DFProperties>
                            <AccessType>
                                <Add></Add>
                                <Get></Get>
                                <Replace></Replace>
                            </AccessType>
                            <Description>Contains the value of a scalar 
                                configuration item.</Description>
                            <!-- The format can be chr/bin/int/bool/float 
                                but this can not be expressed in ddf -->
                            <DFFormat>
                                <chr/>
                            </DFFormat>
                            <Occurrence>
                                <ZeroOrOne></ZeroOrOne>
                            </Occurrence>
                            <Scope>
                                <Dynamic></Dynamic>
                            </Scope>
                            <DFTitle>The value of the scalar
                                configuration item</DFTitle>
                            <!--The type can also vary based on the format-->
                            <DFType>
                                <MIME>text/plain</MIME>
                            </DFType>
                            <CaseSense>
                                <CS></CS>
                            </CaseSense>
                        </DFProperties>
                    </Node>
                    <!-- end cfg/pid/keys/key/value-->
                    <!-- values -->
                    <Node>
                        <NodeName>Values</NodeName>
                        <DFProperties>
                            <AccessType>
                                <Add></Add>
                                <Get></Get>
                            </AccessType>
                            <Description>Stores values of a vector or 
                                array item.</Description>
                            <DFFormat>
                                <node/>
                            </DFFormat>
                            <Occurrence>
                                <ZeroOrOne></ZeroOrOne>
                            </Occurrence>
                            <Scope>
                                <Dynamic></Dynamic>
                            </Scope>
                            <DFTitle>Stores values of a vector or array 
                                item</DFTitle>
                            <DFType>
                                <DDFName></DDFName>
                            </DFType>
                            <CaseSense>
                                <CS></CS>
                            </CaseSense>
                        </DFProperties>
                        <!-- n -->
                        <Node>
                            <!--name is an integer, an index in the array-->
                            <NodeName></NodeName>
                            <DFProperties>
                                <AccessType>
                                    <Add></Add>
                                    <Delete></Delete>
                                    <Get></Get>
                                    <Replace></Replace>
                                </AccessType>
                                <Description>Contains the value of
                                    one element in a non-scalar
                                    configuration item.</Description>
                                <!-- The format can be chr/int/bool/float 
                                    but this can not be expressed in ddf -->
                                <DFFormat>
                                    <chr/>
                                </DFFormat>
                                <Occurrence>
                                    <ZeroOrMore></ZeroOrMore>
                                </Occurrence>
                                <Scope>
                                    <Dynamic></Dynamic>
                                </Scope>
                                <DFTitle>An element of an array or
                                    vector configuration item</DFTitle>
                                <!--The type can also vary based on the format-->
                                <DFType>
                                    <MIME>text/plain</MIME>
                                </DFType>
                                <CaseSense>
                                    <CS></CS>
                                </CaseSense>
                            </DFProperties>
                        </Node>
                        <!-- end cfg/pid/keys/key/values/n-->
                    </Node>
                    <!--end cfg/pid/keys/key/values -->
                </Node>
                <!--end cfg/pid/keys/key-->
            </Node>
            <!--end cfg/pid/keys-->
        </Node>
        <!--end configuration/pid-->
    </Node>
    <!--end configuration-->
</MgmtTree>
