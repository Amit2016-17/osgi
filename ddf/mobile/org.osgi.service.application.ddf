            <Node>
                <NodeName>Version</NodeName>
                <DFProperties>
                    <AccessType>
                        <Get></Get>
                    </AccessType>
                    <Description>Version of the app</Description>
                    <DFFormat>
                        <chr/>
                    </DFFormat>
                    <Occurrence>
                        <ZeroOrOne/>
                    </Occurrence>
                    <Scope>
                        <Permanent/>
                    </Scope>
                    <DFTitle>Version of the app</DFTitle>
                    <DFType>
                        <MIME>text/plain</MIME>
                    </DFType>
                    <CaseSense>
                        <CS></CS>
                    </CaseSense>
                </DFProperties>
            </Node>
