                            <Node>
                                <!--dynamic name-->
                                <NodeName></NodeName>
                                <DFProperties>
                                    <AccessType>
                                        <Add></Add>
                                        <Delete></Delete>
                                        <Get></Get>
                                    </AccessType>
                                    <Description>A startup 
                                        argument</Description>
                                    <DFFormat>
                                        <node/>
                                    </DFFormat>
                                    <Occurrence>
                                        <ZeroOrMore/>
                                    </Occurrence>
                                    <Scope>
                                        <Dynamic/>
                                    </Scope>
                                    <DFTitle>A startup argument</DFTitle>
                                    <DFType>
                                        <DDFName></DDFName>
                                    </DFType>
                                    <CaseSense>
                                        <CS></CS>
                                    </CaseSense>
                                </DFProperties>
                                <!-- Name -->
                                <Node>
                                    <NodeName>Name</NodeName>
                                    <DFProperties>
                                        <AccessType>
                                            <Get></Get>
                                            <Replace></Replace>
                                        </AccessType>
                                        <Description>Name of the 
                                            argument</Description>
                                        <DFFormat>
                                            <chr/>
                                        </DFFormat>
                                        <Occurrence>
                                            <One/>
                                        </Occurrence>
                                        <!--automatic-->
                                        <Scope>
                                            <Dynamic/>
                                        </Scope>
                                        <DFTitle>Name of the 
                                            argument</DFTitle>
                                        <DFType>
                                            <MIME>text/plain</MIME>
                                        </DFType>
                                        <CaseSense>
                                            <CS></CS>
                                        </CaseSense>
                                    </DFProperties>
                                </Node>
                                <!-- end of Name -->
                                <!-- Value -->
                                <Node>
                                    <NodeName>Value</NodeName>
                                    <DFProperties>
                                        <AccessType>
                                            <Get></Get>
                                            <Replace></Replace>
                                        </AccessType>
                                        <Description>Value of the 
                                            argument</Description>
                                        <!--format can be bin, chr, int, float, bool, null-->
                                        <DFFormat>
                                            <chr/>
                                        </DFFormat>
                                        <Occurrence>
                                            <ZeroOrOne/>
                                        </Occurrence>
                                        <!--automatic-->
                                        <Scope>
                                            <Dynamic/>
                                        </Scope>
                                        <DFTitle>Value of the 
                                            argument</DFTitle>
                                        <DFType>
                                            <MIME>text/plain</MIME>
                                        </DFType>
                                        <CaseSense>
                                            <CS></CS>
                                        </CaseSense>
                                    </DFProperties>
                                </Node>
                                <!-- end of Value -->
                            </Node>
                            <!-- end of arg_id -->
                        </Node>
                        <!-- end of Arguments -->
                        <!-- Result -->
                        <Node>
                            <NodeName>Result</NodeName>
                            <DFProperties>
                                <AccessType>
                                    <Get></Get>
                                </AccessType>
                                <Description>Result of the latest launch 
                                    operation</Description>
                                <DFFormat>
                                    <node/>
                                </DFFormat>
                                <Occurrence>
                                    <One/>
                                </Occurrence>
                                <!--automatic-->
                                <Scope>
                                    <Dynamic></Dynamic>
                                </Scope>
                                <DFTitle>Result of the latest launch 
                                    operation</DFTitle>
                                <DFType>
                                    <DDFName></DDFName>
                                </DFType>
                                <CaseSense>
                                    <CS></CS>
                                </CaseSense>
                            </DFProperties>
                            <!-- InstanceID -->
                            <Node>
                                <NodeName>InstanceID</NodeName>
                                <DFProperties>
                                    <AccessType>
                                        <Get></Get>
                                    </AccessType>
                                    <Description>ID of the created 
                                        instance</Description>
                                    <DFFormat>
                                        <chr/>
                                    </DFFormat>
                                    <Occurrence>
                                        <One/>
                                    </Occurrence>
                                    <!--automatic-->
                                    <Scope>
                                        <Dynamic/>
                                    </Scope>
                                    <DFTitle>ID of the created 
                                        instance</DFTitle>
                                    <DFType>
                                        <MIME>text/plain</MIME>
                                    </DFType>
                                    <CaseSense>
                                        <CS></CS>
                                    </CaseSense>
                                </DFProperties>
                            </Node>
                            <!-- end of InstanceID -->
                            <!-- Status -->
                            <Node>
                                <NodeName>Status</NodeName>
                                <DFProperties>
                                    <AccessType>
                                        <Get></Get>
                                    </AccessType>
                                    <Description>OK or Exception 
                                        class</Description>
                                    <DFFormat>
                                        <chr/>
                                    </DFFormat>
                                    <Occurrence>
                                        <One/>
                                    </Occurrence>
                                    <!--automatic-->
                                    <Scope>
                                        <Dynamic/>
                                    </Scope>
                                    <DFTitle>OK or Exception class</DFTitle>
                                    <DFType>
                                        <MIME>text/plain</MIME>
                                    </DFType>
                                    <CaseSense>
                                        <CS></CS>
                                    </CaseSense>
                                </DFProperties>
                            </Node>
                            <!-- end of Status -->
                            <!-- Message -->
                            <Node>
                                <NodeName>Message</NodeName>
                                <DFProperties>
                                    <AccessType>
                                        <Get></Get>
                                    </AccessType>
                                    <Description>Empty or Exception 
                                        message</Description>
                                    <DFFormat>
                                        <chr/>
                                    </DFFormat>
                                    <Occurrence>
                                        <One/>
                                    </Occurrence>
                                    <!--automatic-->
                                    <Scope>
                                        <Dynamic/>
                                    </Scope>
                                    <DFTitle>Empty or Exception 
                                        message</DFTitle>
                                    <DFType>
                                        <MIME>text/plain</MIME>
                                    </DFType>
                                    <CaseSense>
                                        <CS></CS>
                                    </CaseSense>
                                </DFProperties>
                            </Node>
                            <!-- end of Message -->
                        </Node>
                        <!-- end of Result -->
                    </Node>
                    <!-- end of Launch_ID -->
                </Node>
                <!-- end of Launch -->
            </Node>
            <!-- end of Operations -->
            <!-- Schedules -->
            <Node>
                <NodeName>Schedules</NodeName>
                <DFProperties>
                    <AccessType>
                        <Get></Get>
                    </AccessType>
                    <Description>Subtree of the existing 
