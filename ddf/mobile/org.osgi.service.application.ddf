                    <DFTitle>Vendor of the app</DFTitle>
                    <DFType>
                        <MIME>text/plain</MIME>
                    </DFType>
                    <CaseSense>
                        <CS></CS>
                    </CaseSense>
                </DFProperties>
            </Node>
