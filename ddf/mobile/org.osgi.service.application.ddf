                    <Occurrence>
                        <One/>
                    </Occurrence>
                    <Scope>
                        <Permanent/>
                    </Scope>
                    <DFTitle>Is the app locked?</DFTitle>
                    <DFType>
                        <MIME>text/plain</MIME>
                    </DFType>
                    <CaseSense>
                        <CS></CS>
                    </CaseSense>
                </DFProperties>
            </Node>
            <!-- end of Locked -->
            <!-- ContainerID -->
            <Node>
                <NodeName>ContainerID</NodeName>
                <DFProperties>
                    <AccessType>
                        <Get></Get>
                    </AccessType>
                    <Description>The container that registered this 
                        app</Description>
                    <DFFormat>
                        <chr/>
                    </DFFormat>
                    <Occurrence>
                        <One/>
                    </Occurrence>
                    <Scope>
                        <Permanent/>
                    </Scope>
                    <DFTitle>The container that registered this 
                        app</DFTitle>
                    <DFType>
                        <MIME>text/plain</MIME>
                    </DFType>
                    <CaseSense>
                        <CS></CS>
                    </CaseSense>
                </DFProperties>
            </Node>
            <!-- end of ContainerID -->
            <!-- Location -->
            <Node>
                <NodeName>Location</NodeName>
                <DFProperties>
                    <AccessType>
                        <Get></Get>
                    </AccessType>
                    <Description>The application.location 
                        property</Description>
                    <DFFormat>
                        <chr/>
                    </DFFormat>
                    <Occurrence>
                        <One/>
                    </Occurrence>
                    <Scope>
                        <Permanent/>
                    </Scope>
                    <DFTitle>The application.location property</DFTitle>
                    <DFType>
                        <MIME>text/plain</MIME>
                    </DFType>
                    <CaseSense>
                        <CS></CS>
                    </CaseSense>
                </DFProperties>
            </Node>
            <!-- end of Location -->
            <!-- Ext -->
            <Node>
                <NodeName>Ext</NodeName>
                <DFProperties>
                    <AccessType>
                        <Get></Get>
                    </AccessType>
                    <Description>Application model and container specific 
                        extensions</Description>
                    <DFFormat>
                        <node/>
                    </DFFormat>
                    <Occurrence>
                        <One/>
                    </Occurrence>
                    <Scope>
                        <Permanent/>
                    </Scope>
                    <DFTitle>Application model and container specific 
                        extensions</DFTitle>
                    <DFType>
                        <DDFName></DDFName>
                    </DFType>
                    <CaseSense>
                        <CS></CS>
                    </CaseSense>
                </DFProperties>
            </Node>
            <!-- end of Ext -->
            <!-- Instances -->
            <Node>
                <NodeName>Instances</NodeName>
                <DFProperties>
                    <AccessType>
                        <Get></Get>
                    </AccessType>
                    <Description>Currently existing app 
