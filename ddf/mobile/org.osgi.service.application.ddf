                    <NodeName>Lock</NodeName>
                    <DFProperties>
                        <AccessType>
                            <Exec></Exec>
                            <Get></Get>
                        </AccessType>
                        <Description>Locks the app when 
                            executed</Description>
                        <DFFormat>
                            <null/>
                        </DFFormat>
                        <Occurrence>
                            <One/>
                        </Occurrence>
                        <Scope>
                            <Permanent/>
                        </Scope>
                        <DFTitle>Locks the app when executed</DFTitle>
                        <DFType>
                            <MIME>text/plain</MIME>
                        </DFType>
                        <CaseSense>
                            <CS></CS>
                        </CaseSense>
                    </DFProperties>
                </Node>
                <!-- end of Lock -->
                <!-- Unlock -->
                <Node>
                    <NodeName>Unlock</NodeName>
                    <DFProperties>
                        <AccessType>
                            <Exec></Exec>
                            <Get></Get>
                        </AccessType>
                        <Description>Unlocks the app when 
                            executed</Description>
                        <DFFormat>
                            <null/>
                        </DFFormat>
                        <Occurrence>
                            <One/>
                        </Occurrence>
                        <Scope>
                            <Permanent/>
                        </Scope>
                        <DFTitle>Unlocks the app when executed</DFTitle>
                        <DFType>
                            <MIME>text/plain</MIME>
                        </DFType>
                        <CaseSense>
                            <CS></CS>
                        </CaseSense>
                    </DFProperties>
                </Node>
