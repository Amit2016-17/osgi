                        <Occurrence>
                            <One/>
                        </Occurrence>
                        <Scope>
                            <Permanent/>
                        </Scope>
                        <DFTitle>Subtree for launching</DFTitle>
                        <DFType>
                            <DDFName></DDFName>
                        </DFType>
                        <CaseSense>
                            <CS></CS>
                        </CaseSense>
                    </DFProperties>
                    <!-- Launch_ID -->
                    <Node>
                        <!--dynamic name-->
                        <NodeName></NodeName>
                        <DFProperties>
                            <AccessType>
                                <Add></Add>
                                <Delete></Delete>
                                <Exec></Exec>
                                <Get></Get>
                            </AccessType>
                            <Description>A set of launching 
                                parameters</Description>
                            <DFFormat>
                                <node/>
                            </DFFormat>
                            <Occurrence>
                                <ZeroOrMore/>
                            </Occurrence>
                            <Scope>
                                <Dynamic/>
                            </Scope>
