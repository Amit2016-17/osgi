                                    <node/>
                                </DFFormat>
                                <Occurrence>
                                    <One/>
                                </Occurrence>
                                <!--automatic-->
                                <Scope>
                                    <Dynamic></Dynamic>
                                </Scope>
                                <DFTitle>Argument subtree</DFTitle>
                                <DFType>
                                    <DDFName></DDFName>
                                </DFType>
                                <CaseSense>
                                    <CS></CS>
                                </CaseSense>
                            </DFProperties>
