                    <Scope>
                        <Permanent/>
                    </Scope>
                    <DFTitle>URI of the application's icon</DFTitle>
                    <DFType>
                        <MIME>text/plain</MIME>
                    </DFType>
                    <CaseSense>
                        <CS></CS>
                    </CaseSense>
                </DFProperties>
            </Node>
