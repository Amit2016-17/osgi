                    <DFFormat>
                        <node/>
                    </DFFormat>
                    <Occurrence>
                        <One/>
                    </Occurrence>
                    <Scope>
                        <Permanent/>
                    </Scope>
                    <DFTitle>Subtree of the existing schedules</DFTitle>
                    <DFType>
                        <DDFName></DDFName>
                    </DFType>
                    <CaseSense>
                        <CS></CS>
                    </CaseSense>
                </DFProperties>
                <!-- schedule_id -->
                <Node>
                    <!--dynamic name-->
                    <NodeName></NodeName>
                    <DFProperties>
                        <AccessType>
                            <Add></Add>
                            <Delete></Delete>
                            <Get></Get>
                        </AccessType>
                        <Description>A schedule for this app</Description>
                        <DFFormat>
