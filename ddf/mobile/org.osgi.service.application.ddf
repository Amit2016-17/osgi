                        <DFProperties>
                            <AccessType>
                                <Get></Get>
                            </AccessType>
                            <Description>State of the instance</Description>
                            <DFFormat>
                                <chr/>
                            </DFFormat>
                            <Occurrence>
                                <One/>
                            </Occurrence>
                            <Scope>
                                <Permanent/>
                            </Scope>
                            <DFTitle>State of the instance</DFTitle>
                            <DFType>
                                <MIME>text/plain</MIME>
                            </DFType>
                            <CaseSense>
                                <CS></CS>
                            </CaseSense>
                        </DFProperties>
                    </Node>
                    <!-- end of State -->
                    <!-- InstanceID -->
                    <Node>
                        <NodeName>InstanceID</NodeName>
                        <DFProperties>
                            <AccessType>
                                <Get></Get>
                            </AccessType>
                            <Description>service.pid, 
                                unmangled</Description>
                            <DFFormat>
                                <chr/>
                            </DFFormat>
                            <Occurrence>
                                <One/>
                            </Occurrence>
                            <Scope>
                                <Permanent/>
                            </Scope>
                            <DFTitle>service.pid, unmangled</DFTitle>
                            <DFType>
                                <MIME>text/plain</MIME>
                            </DFType>
                            <CaseSense>
                                <CS></CS>
                            </CaseSense>
                        </DFProperties>
                    </Node>
                    <!-- end of InstanceID -->
                    <!-- Operations -->
                    <Node>
                        <NodeName>Operations</NodeName>
                        <DFProperties>
                            <AccessType>
                                <Get></Get>
                            </AccessType>
                            <Description>Subtree for lifecycle operations 
                                for this instance</Description>
                            <DFFormat>
                                <node/>
                            </DFFormat>
                            <Occurrence>
                                <One/>
                            </Occurrence>
                            <Scope>
                                <Permanent/>
                            </Scope>
                            <DFTitle>Subtree for lifecycle operations for 
                                this instance</DFTitle>
                            <DFType>
                                <DDFName></DDFName>
                            </DFType>
                            <CaseSense>
                                <CS></CS>
                            </CaseSense>
                        </DFProperties>
                        <!-- Stop -->
                        <Node>
                            <NodeName>Stop</NodeName>
                            <DFProperties>
                                <AccessType>
                                    <Exec></Exec>
                                    <Get></Get>
                                </AccessType>
                                <Description>Stops the instance when 
                                    executed</Description>
                                <DFFormat>
                                    <null/>
                                </DFFormat>
                                <Occurrence>
                                    <One/>
                                </Occurrence>
                                <Scope>
                                    <Permanent/>
                                </Scope>
                                <DFTitle>Stops the instance when 
                                    executed</DFTitle>
                                <DFType>
                                    <MIME>text/plain</MIME>
                                </DFType>
                                <CaseSense>
                                    <CS></CS>
                                </CaseSense>
                            </DFProperties>
                        </Node>
                        <!-- end of Stop -->
                        <!-- Ext -->
                        <Node>
                            <NodeName>Ext</NodeName>
                            <DFProperties>
                                <AccessType>
                                    <Get></Get>
                                </AccessType>
                                <Description>Application model specific 
                                    extensions</Description>
                                <DFFormat>
                                    <node/>
                                </DFFormat>
                                <Occurrence>
                                    <One/>
                                </Occurrence>
                                <Scope>
                                    <Permanent/>
                                </Scope>
                                <DFTitle>Application model specific 
                                    extensions</DFTitle>
                                <DFType>
                                    <DDFName></DDFName>
                                </DFType>
                                <CaseSense>
                                    <CS></CS>
                                </CaseSense>
                            </DFProperties>
                        </Node>
                        <!-- end of Ext -->
                    </Node>
                    <!-- end of Operations -->
                </Node>
                <!-- end of instance_id -->
            </Node>
            <!-- end of Instances -->
            <!-- Operations -->
            <Node>
                <NodeName>Operations</NodeName>
                <DFProperties>
                    <AccessType>
                        <Get></Get>
                    </AccessType>
                    <Description>Subtree for parameters of lifecycle 
                        operations</Description>
                    <DFFormat>
                        <node/>
                    </DFFormat>
                    <Occurrence>
                        <One/>
                    </Occurrence>
                    <Scope>
                        <Permanent/>
                    </Scope>
                    <DFTitle>Subtree for parameters of lifecycle 
