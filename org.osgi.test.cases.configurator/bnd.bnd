# Set javac settings from JDT prefs
-include: ${build}/eclipse/jdt.bnd

Bundle-Description                  : A test case for the event generic mechanism implementation.

-conditionalpackage                 = org.osgi.test.support.*
-privatepackage                     =  \
    ${p}.junit
Export-Package                      = \
    ${p}.service
Import-Package: *

-includeresource                    = \
   tb1.jar ,\
   tb2.jar ,\
   tb3.jar ,\
   tb4.jar ,\
   tb5.jar ,\
   tb6.jar ,\
   tb7.jar ,\
   tb8.jar ,\
   tb9.jar ,\
   tb10.jar,\
   tb11a.jar ,\
   tb11b.jar ,\
   init_config.json=configs/init_config.json

Test-Cases                          = ${testcases}

-buildpath                          = \
    org.osgi.test.support;                  version=project , \
    org.osgi.service.configurator;          version=project , \
    org.osgi.util.function;                 version=project , \
    org.osgi.util.promise;                  version=project , \
    osgi.core;                              version=6.0.0   , \
    osgi.cmpn;                              version=6.0.0   , \
    ${junit4}

-runbundles                         = \
    ${junit4},\
    org.osgi.impl.service.cm; version=latest , \
    org.osgi.impl.service.configurator; version=latest , \
    org.osgi.impl.service.log; version=latest , \
    org.osgi.util.function; version=latest, \
    org.osgi.util.promise; version=latest

-runproperties = ${runproperties},\
    ${p}.bundles="${uniq;${repo;biz.aQute.junit;latest},${repo;org.osgi.impl.service.cm;latest},${repo;org.osgi.impl.service.configurator;latest},${repo;org.osgi.impl.service.log;latest},${repo;org.osgi.util.function;latest},${repo;org.osgi.util.promise;latest}}"
