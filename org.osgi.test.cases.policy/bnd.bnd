Bundle-Version						: 4.0.0.${project.build}
Bundle-Description					: Policy Test Case.
Bundle-DocUrl                       : http://www.osgi.org/testcases/4/test.cases.template

Export-Package						= \
    org.osgi.test.support.*        , \
	${p}.tbc.*

Include-Resource					= \
	tb1.jar, \
	com.provider.messages.jar

Test-Cases							= ${testcases}
-signaturetest                      = org.osgi.util.mobile

-buildpath							= \
    org.osgi.test.support;                  version=project , \
    info.dmtree;                  			version=project , \
	osgi.core;								version=4.0.1	, \
	osgi.mobile;							version=4.0	, \
	ee.foundation;							version=1.1	, \
	${junit}

-runbundles							= \
	org.osgi.impl.service.event;        version=latest, \
	org.osgi.impl.service.component; version=latest, \
	org.osgi.impl.service.policy;     version=project, \
    org.osgi.impl.service.cm;     version=latest, \
	org.osgi.impl.service.dmt;        version=latest, \
	osgi.cmpn; version=${cmpn.published.version};strategy=lowest


-runproperties = report=true, \
    org.osgi.test.cases.policy.automatic=true, \
	org.osgi.vendor.dmtree.DigestDelegate=org.osgi.impl.service.dmt.export.Digest, \
	info.dmtree.osgi.root=./OSGi/
    