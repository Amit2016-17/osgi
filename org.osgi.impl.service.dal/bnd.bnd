# Set javac settings from JDT prefs
-include: ${project.workspace}/cnf/eclipse/jdt.bnd

Bundle-Vendor					= ProSyst
Bundle-Activator				= ${p}.Activator

Private-Package					= ${p}.*

Export-Package 					= \
    org.osgi.service.dal.simulator; -split-package:=first; provide:=true

-buildpath						= \
    org.osgi.service.dal;				version=project, \
	osgi.core;							version=4.2, \
	osgi.cmpn;							version=4.2, \
	ee.minimum;							version=1.2