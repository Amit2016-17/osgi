# Set javac settings from JDT prefs
-include: ${project.workspace}/cnf/eclipse/jdt.bnd

Conditional-Package					= org.osgi.test.support.*
Private-Package						=  \
	${p}.*

-buildpath			= \
	osgi.core;						version=4.2		, \
	osgi.cmpn;						version=4.2		, \
	ee.foundation									, \
	org.osgi.test.support;      version=project 	, \
	${junit}

-runbundles			= \
	org.osgi.impl.service.log;						version=project, \
    org.apache.felix.configadmin;					version=latest, \
	org.osgi.impl.service.dmt;						version=project, \
	org.osgi.impl.service.residentialmanagement;	version=project, \
	osgi.cmpn;										version=latest
	
-runproperties = report=true, \
	org.osgi.framework.system.packages=org.osgi.test.cases.residentialmanagement.syspkg;"version=1.0.0" ,\
	org.osgi.framework.security=osgi, \
	org.osgi.framework.trust.repositories=${build}/keystore

Test-Cases: ${testcases}

Export-Package = \
	${p}.util;version=1.0.0

-signaturetest = org.osgi.service.component

Include-Resource	= \
	org.osgi.test.cases.residentialmanagement.tb1.jar=tb1.jar ,\
	org.osgi.test.cases.residentialmanagement.tb2.jar=tb2.jar ,\
	org.osgi.test.cases.residentialmanagement.tb3.jar=tb3.jar ,\
	org.osgi.test.cases.residentialmanagement.tb4.jar=tb4.jar ,\
	org.osgi.test.cases.residentialmanagement.tb5.jar=tb5.jar ,\
	org.osgi.test.cases.residentialmanagement.tb6.jar=tb6.jar ,\
	org.osgi.test.cases.residentialmanagement.tb7.jar=tb7.jar ,\
	org.osgi.test.cases.residentialmanagement.tb8.jar=tb8.jar ,\
	org.osgi.test.cases.residentialmanagement.tb9.jar=tb9.jar
