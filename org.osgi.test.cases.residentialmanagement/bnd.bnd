# Set javac settings from JDT prefs
-include: ${build}/eclipse/jdt.bnd

Conditional-Package					= org.osgi.test.support.*
Private-Package						=  \
	${p}.*

-buildpath			= \
	osgi.core;						version=4.3.1	, \
	ee.minimum;						version=1.2.1	, \
	org.osgi.test.support;      	version=project 	, \
	org.osgi.service.dmt;			version=project		, \
	osgi.residential;				version=4.3		, \
	${junit}

-runbundles			= \
	org.osgi.impl.service.log;						version=latest, \
    org.osgi.impl.service.cm;						version=latest, \
    org.osgi.impl.service.event;					version=latest, \
	org.osgi.impl.service.dmt;						version=latest, \
	org.osgi.impl.service.residentialmanagement;	version=latest
	
-runproperties = ${runsecureproperties}, \
	org.osgi.framework.system.packages.extra=${p}.syspkg;"version=1.0.0" ,\
	org.osgi.dmt.residential=./Scaffold/RMT ,\
	org.osgi.test.cases.rmt.delay=1000

Test-Cases: ${testcases}

Export-Package = \
	${p}.util;version=1.0.0

Include-Resource	= \
	${p}.tb1.jar=tb1.jar ,\
	${p}.tb2.jar=tb2.jar ,\
	${p}.tb3.jar=tb3.jar ,\
	${p}.tb4.jar=tb4.jar ,\
	${p}.tb5.jar=tb5.jar ,\
	${p}.tb6.jar=tb6.jar ,\
	${p}.tb7.jar=tb7.jar ,\
	${p}.tb8.jar=tb8.jar ,\
	${p}.tb9.jar=tb9.jar ,\
	${p}.tbnontrusted.jar=resources/NonTrustedBundle.jar
