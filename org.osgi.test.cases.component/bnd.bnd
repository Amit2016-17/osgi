Bundle-Version: 4.2.0.${project.build}
Bundle-Description: Declarative Services Test Case
-debug: true

Private-Package						= org.osgi.test.support.*, \
	${p}.junit
Export-Package						= \
	${p}.service

Include-Resource					= \
	tb1.jar, \
	tb2.jar, \
	tb3.jar, \
	tb4.jar, \
	tb5.jar, \
	tb6.jar, \
	tb7.jar, \
	tb8.jar, \
	tb9.jar, \
	tb10.jar, \
	tb11.jar, \
	tb12.jar, \
	tb13.jar, \
	tb13a.jar, \
	tb14.jar
	
Test-Cases							= ${testcases}
-signaturetest                      = org.osgi.service.component

-buildpath							= \
    org.osgi.test.support;                  version=project, \
    org.osgi.service.component;             version=project, \
	osgi.core;								version=4.1, \
	osgi.cmpn;								version=4.1, \
	ee.minimum; 							version=1.2, \
	${junit}

-runbundles							= \
	org.osgi.impl.service.component; version=latest, \
	org.osgi.impl.service.cm; version=latest, \
	org.osgi.impl.service.log; version=latest, \
	osgi.cmpn; version=latest

-runproperties = report=true, \
	scr.bundle.name=org.eclipse.equinox.ds
