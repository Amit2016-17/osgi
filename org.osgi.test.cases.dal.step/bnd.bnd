# Set javac settings from JDT prefs
-include: ${project.workspace}/cnf/eclipse/jdt.bnd

Bundle-Vendor					= ProSyst
Bundle-Activator				= org.osgi.test.cases.step.dal.Activator

Private-Package					= org.osgi.test.cases.step.dal.*

Export-Package 					= \
    org.osgi.test.cases.step; -split-package:=first; provide:=true

-buildpath						= \
    org.osgi.impl.service.dal;			version=project, \
	osgi.core;							version=4.2, \
	osgi.cmpn;							version=4.2, \
	ee.minimum;							version=1.2