javac.profile=compact3
# Set javac settings from JDT prefs
-include: ${build}/eclipse/jdt.bnd
-noee = true

-conditionalpackage                 = org.osgi.test.support.*
-privatepackage                     =  \
    ${p}.junit.*
Import-Package: ${-signaturetest}, *

-includeresource                    = \
    tb1.jar ,\
    tb2.jar ,\
    tb2_updated.jar ,\
    tb3.jar ,\
    tb4.jar

-buildpath = \
    org.osgi.test.support;          version=project, \
    org.osgi.jmx;                   version=project , \
    osgi.core;maven-scope=provided;version=4.3.1   , \
    osgi.cmpn;                      version=4.3.1   , \
    ${junit4}

-runbundles                         = \
    ${junit4},\
    org.osgi.impl.bundle.jmx; version=latest, \
    org.osgi.impl.service.cm; version=latest, \
    org.osgi.impl.service.log; version=latest

Test-Cases                          = ${testcases}
-signaturetest                      = \
            org.osgi.jmx, \
            org.osgi.jmx.framework, \
            org.osgi.jmx.framework.wiring

-runproperties = ${runproperties}, \
    ${p}.system.packages.extra="", \
    ${p}.bundles="${uniq;${repo;biz.aQute.junit;latest},${repo;org.osgi.impl.service.cm;latest},${repo;org.osgi.impl.service.log;latest},${repo;org.osgi.impl.bundle.jmx;latest}}"
#   osgi.console=1111

