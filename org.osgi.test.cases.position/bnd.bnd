# Set javac settings from JDT prefs
-include: ${build}/eclipse/jdt.bnd

Bundle-Description: Tests the Position classes

Conditional-Package					= org.osgi.test.support.*
Private-Package						=  \
	${p}.junit.*
Import-Package: ${-signaturetest}, *
	
Test-Cases							= ${testcases}
-signaturetest                      = org.osgi.util.position

-buildpath							= \
    org.osgi.test.support;                  version=project , \
    org.osgi.util.position;                 version=project , \
	osgi.core;								version=4.0	, \
	osgi.cmpn;								version=4.0	, \
	${junit}

-runbundles						= \
	org.osgi.util.position; version=latest, \
	org.osgi.util.measurement; version=latest


-runproperties = ${runproperties}
