# Set javac settings from JDT prefs
-include: ${build}/eclipse/jdt.bnd

Private-Package 				= ${p}
Service-Component				= OSGI-INF/services.xml
Bundle-Vendor					= Nokia
Include-Resource				= OSGI-INF=src/OSGI-INF
Bundle-RequiredExecutionEnvironment = J2SE-1.4

-buildpath						= \
	ee.j2se;					version=${javac.compliance}, \
	osgi.core;					version=4.1, \
	osgi.cmpn;					version=4.1

	