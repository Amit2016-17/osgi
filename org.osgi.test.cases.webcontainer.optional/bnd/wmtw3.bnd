Bundle-Vendor: OSGi Alliance
Bundle-Name: ${p}.wmtw3
Bundle-Description: OSGi Web Container RFC 66 Optional Compliance Test test war 3 with manifest somewhat pre-specified

# We don't want bnd to put the source code in the jar
-sources=false

Private-Package: ${p}.tw2.*, \
 ${p}.util.*
-wab: resources/tw3
-resourceonly=true
-removeheaders: !Manifest-Version, \
 !Bundle-Vendor, \
 !Bundle-ClassPath, \
 !Bundle-Name, \
 !Bundle-Description, \
 !Import-Package, \
 !Web-ContextPath, \
 *

	