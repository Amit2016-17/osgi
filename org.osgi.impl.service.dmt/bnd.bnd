# Set javac settings from JDT prefs
-include: ${project.workspace}/cnf/eclipse/jdt.bnd

Bundle-Vendor					= Nokia
Bundle-Version					= 1.0.0.${project.build}
Bundle-Activator				= $(p).Activator
Export-Service					= \
	info.dmtree.DmtAdmin, \
	info.dmtree.notification.NotificationService
Import-Service					= \
	org.osgi.service.log.LogService, \
	org.osgi.service.cm.ConfigurationAdmin

Bundle-RequiredExecutionEnvironment = J2SE-1.4

Export-Package 					= ${p}.export,info.dmtree.*;-split-package:=first
Private-Package					= ${p}.*

javac.source					= 1.5
javac.target					= jsr14

-buildpath						= \
    info.dmtree;				version=project, \
	ee.j2se;					version=1.5, \
	osgi.core;					version=4.0.1, \
	osgi.cmpn;					version=4.0.1