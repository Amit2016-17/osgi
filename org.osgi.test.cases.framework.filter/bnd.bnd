Bundle-Description					= Installs and runs the other test bundles one by one.
Bundle-Version						= 4.0.0.${project.build}
Export-Package						= ${p}.tbc.*, org.osgi.test.support.*
Include-Resource					= tb1.jar

-buildpath							= \
	org.osgi.framework;						version=project	, \
	org.osgi.test.support;					version=project	, \
	osgi;									version=4.0		, \
	ee.minimum												, \
	com.springsource.junit; version=3.8.2

-runproperties						= report=true, org.osgi.framework.security=osgi

Test-Cases							= ${classes;extending;*TestCase}
