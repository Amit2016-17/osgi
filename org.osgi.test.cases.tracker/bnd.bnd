# Set javac settings from JDT prefs
-include: ${project.workspace}/cnf/eclipse/jdt.bnd

Private-Package						= ${p}.junit, \
	org.osgi.test.support.*
Export-Package						= ${p}.service

-buildpath							= \
	ee.minimum; 							version=1.2.1, \
	org.osgi.framework;						version=project, \
	org.osgi.util.tracker;					version=project, \
	org.osgi.test.support;					version=project, \
	${junit}
	
-runbundles							= \
	osgi.cmpn; version=latest

-runproperties						= ${runproperties}

# Uncomment the following to test the tracker code in the org.osgi.util.tracker 
# project instead of the one in Equinox.
#-runpath							= \
#	org.osgi.util.tracker;	version=project, \
#	org.osgi.impl.framework; version=latest, \
#	${junit}
	
Test-Cases							= ${testcases}
-signaturetest                      = org.osgi.util.tracker

Include-Resource				= \
	tb1.jar, \
	tb2.jar, \
	tb3.jar, \
	tb4.jar
