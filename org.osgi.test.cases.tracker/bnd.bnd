Bundle-Version						= 4.0.0.${project.build}
Private-Package						= ${p}.junit, \
	org.osgi.test.support.*
Export-Package						= ${p}.service

-buildpath							= \
	org.osgi.util.tracker;					version=project, \
	org.osgi.test.support;					version=project, \
	osgi.core;								version=4.0		, \
	ee.minimum;								version=1.2	, \
	${junit}
	
-runbundles							= \
	osgi.cmpn; version=latest

-runproperties						= report=true
	
Test-Cases							= ${testcases}
-signaturetest                      = org.osgi.util.tracker

Include-Resource				= \
	tb1.jar, \
	tb2.jar, \
	tb3.jar, \
	tb4.jar
